<inherited ParentReport="PecRetrato" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1535" ReportOptions.Name="Conciliação Bancária" ReportOptions.LastChange="44925,3637616782" ReportOptions.VersionBuild="0" ReportOptions.VersionMajor="2" ReportOptions.VersionMinor="400" ReportOptions.VersionRelease="1" ScriptText.Text="function AgenciaBancaria: String;&#13;&#10;begin&#13;&#10;  if VarToStr(&#60;SqlContaBancaria.&#34;DV_AGENCIA&#34;&#62;) &#60;&#62; '' then&#13;&#10;    Result := &#60;SqlContaBancaria.&#34;CD_AGENCIA&#34;&#62; + '-' + &#60;SqlContaBancaria.&#34;DV_AGENCIA&#34;&#62;&#13;&#10;  else&#13;&#10;    Result := &#60;SqlContaBancaria.&#34;CD_AGENCIA&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;function ContaBancaria: String;&#13;&#10;begin&#13;&#10;  if VarToStr(&#60;SqlContaBancaria.&#34;DV_CONTA_BANCARIA&#34;&#62;) &#60;&#62; '' then&#13;&#10;    Result := &#60;SqlContaBancaria.&#34;CD_CONTA_BANCARIA&#34;&#62; + '-' + &#60;SqlContaBancaria.&#34;DV_CONTA_BANCARIA&#34;&#62;&#13;&#10;  else&#13;&#10;    Result := &#60;SqlContaBancaria.&#34;CD_CONTA_BANCARIA&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure chbContaBancariaFichaOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chbContaBancariaFicha.Visible := &#60;ConciliacaoPorFicha&#62; = 1;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cabFichasOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cabFichas.Visible := &#60;ConciliacaoPorFicha&#62; = 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndFichasOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bndFichas.Visible := &#60;ConciliacaoPorFicha&#62; = 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ftrfichasOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  ftrFichas.Visible := &#60;ConciliacaoPorFicha&#62; = 0;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndPendenciasOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bndPendencias.Visible := &#60;SqlPendencias.&#34;NIVEL&#34;&#62; = 1;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure chbPendenciasOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  chbPendencias.Visible := &#60;SqlPendencias.&#34;NIVEL&#34;&#62; = 2;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  if &#60;IdRelatorioFiltro&#62; &#62; 0 then&#13;&#10;    SqlContaBancaria.SQL.Text := StringReplace(SqlContaBancaria.SQL.Text, '/*FILTRO_CONTA*/', 'AND CONTA.ID_CONTA_BANCARIA    = FILTR.ID_FILTRO');&#13;&#10;end." StoreInDFM="False">
