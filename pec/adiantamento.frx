      <TableDataSource Name="Table1" Alias="SqlAdiantamento" DataType="System.Int32" Enabled="true" SelectCommand="SELECT EMPEN.ID_EMPENHO&#13;&#10;     , EMPEN.NR_EMPENHO&#13;&#10;     , EMPEN.ANO_EMPENHO&#13;&#10;     , EMPEN.ID_CREDOR&#13;&#10;     , SPESS.NOME_RAZAO&#13;&#10;     , SPESS.DOCUMENTO_FORMATADO&#13;&#10;     , ADNTO.ID_LIQUIDACAO&#13;&#10;     , ADNTO.ID_ORDEM_PAGAMENTO&#13;&#10;     , ADNTO.PERIODO_APLICACAO&#13;&#10;     , ADNTO.DT_CONCESSAO&#13;&#10;     , STADI.ID_ST_ADIANTAMENTO&#13;&#10;     , STADI.DS_ST_ADIANTAMENTO&#13;&#10;     , LIQUD.NR_LIQUIDACAO&#13;&#10;     , LIQUD.ANO_LIQUIDACAO&#13;&#10;     , ORDPG.NR_PARCELA&#13;&#10;     , SUM(CASE WHEN ADMOV.ID_TP_ADIANTAMENTO_MOV = 1 THEN ADMOV.VL_MOVIMENTO     ELSE 0 END) AS VL_CONCESSAO&#13;&#10;     , SUM(CASE WHEN ADMOV.ID_TP_ADIANTAMENTO_MOV = 2 THEN ADMOV.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_UTILIZACAO&#13;&#10;     , SUM(CASE WHEN ADMOV.ID_TP_ADIANTAMENTO_MOV = 3 THEN ADMOV.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_DEVOLUCAO&#13;&#10;     , SUM(COALESCE(ADMOV.VL_MOVIMENTO, 0))                                                   AS VL_SALDO&#13;&#10;  FROM PEC_EMPENHO EMPEN&#13;&#10;       INNER JOIN PEC_ADIANTAMENTO                   ADNTO ON ADNTO.ID_EMPENHO           = EMPEN.ID_EMPENHO&#13;&#10;       LEFT JOIN PEC_ST_ADIANTAMENTO                 STADI ON STADI.ID_ST_ADIANTAMENTO   = ADNTO.ID_ST_ADIANTAMENTO&#13;&#10;       LEFT JOIN PEC_LIQUIDACAO                      LIQUD ON LIQUD.ID_LIQUIDACAO        = ADNTO.ID_LIQUIDACAO&#13;&#10;       LEFT JOIN PEC_ORDEM_PAGAMENTO                 ORDPG ON ORDPG.ID_ORDEM_PAGAMENTO   = ADNTO.ID_ORDEM_PAGAMENTO&#13;&#10;       LEFT JOIN SIS_PESSOA                          SPESS ON SPESS.ID_PESSOA            = EMPEN.ID_CREDOR&#13;&#10;       LEFT JOIN PEC_ADIANTAMENTO_MOV                ADMOV ON ADMOV.ID_EMPENHO           = ADNTO.ID_EMPENHO&#13;&#10;                                                          AND ADMOV.CP_ANO              &lt;= :CP_ANO&#13;&#10;