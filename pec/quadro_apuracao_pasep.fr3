    <TfrxADOQuery Name="SqlReceita" UserName="SqlReceita" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT CASE WHEN B.ID_SEQUENCIA &#60; 3 THEN UDF_DIGITS(A.CD_PLANO_CONTA)&#13;&#10;            WHEN B.ID_SEQUENCIA = 3 THEN UDF_DIGITS(A.CD_PLANO_CONTA) || '0.0'&#13;&#10;            WHEN B.ID_SEQUENCIA = 4 THEN UDF_DIGITS(A.CD_PLANO_CONTA) || COALESCE(A.CD_TP_DEDUCAO, '0.0')&#13;&#10;       END  AS CHAVE&#13;&#10;&#13;&#10;     , B.ID_SEQUENCIA AS NIVEL&#13;&#10;&#13;&#10;     , CASE WHEN B.ID_SEQUENCIA = 1 THEN A.CD_PLANO_CONTA_REL || ' - ' || A.DS_PLANO_CONTA&#13;&#10;            WHEN B.ID_SEQUENCIA = 2 THEN '     ' || A.CD_PLANO_CONTA_REL || ' - ' || A.DS_PLANO_CONTA&#13;&#10;            WHEN B.ID_SEQUENCIA = 3 THEN '          0.0 - Receita Bruta'&#13;&#10;            WHEN B.ID_SEQUENCIA = 4 THEN '          ' || A.CD_TP_DEDUCAO || ' - ' || A.DS_TP_DEDUCAO_RELATORIO&#13;&#10;       END  AS DESCRICAO&#13;&#10;&#13;&#10;     , ROUND(SUM(A.VL_ARRECADADO), 2) AS VL_ARRECADADO&#13;&#10;  FROM (SELECT C1.CD_TP_DEDUCAO&#13;&#10;             , CAST(C1.DS_TP_DEDUCAO_RELATORIO AS VARCHAR(100) )    AS DS_TP_DEDUCAO_RELATORIO    &#13;&#10;             , B1.CD_PLANO_CONTA&#13;&#10;             , B1.CD_PLANO_CONTA_REL&#13;&#10;             , B1.DS_PLANO_CONTA&#13;&#10;             , B1.NIVEL                            &#13;&#10;             , MAX(A1.TEM_DEDUTORA)                                 AS TEM_DEDUTORA&#13;&#10;             , SUM(A1.VL_ARRECADADO)                                AS VL_ARRECADADO&#13;&#10;          FROM (&#13;&#10;                SELECT B2.ID_ESTADO&#13;&#10;                     , B2.CP_ANO&#13;&#10;                     , B2.ID_PLANO_CONTA_CP&#13;&#10;                     , B2.ID_PLANO_CONTA_PAI&#13;&#10;                     , B2.NIVEL&#13;&#10;                     , A2.ID_TP_DEDUCAO   &#13;&#10;                     , A2.TEM_DEDUTORA&#13;&#10;                     , SUM(A2.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                  FROM ( &#13;&#10;                        SELECT B3.ID_ESTADO&#13;&#10;                             , B3.CP_ANO&#13;&#10;                             , B3.ID_PLANO_CONTA_CP&#13;&#10;                             , B3.ID_PLANO_CONTA_PAI&#13;&#10;                             , B3.NIVEL&#13;&#10;                             , A3.ID_TP_DEDUCAO    &#13;&#10;                             , A3.TEM_DEDUTORA&#13;&#10;                             , SUM(A3.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                          FROM ( &#13;&#10;                                SELECT B4.ID_ESTADO&#13;&#10;                                     , B4.CP_ANO&#13;&#10;                                     , B4.ID_PLANO_CONTA_CP&#13;&#10;                                     , B4.ID_PLANO_CONTA_PAI&#13;&#10;                                     , B4.NIVEL&#13;&#10;                                     , A4.ID_TP_DEDUCAO    &#13;&#10;                                     , A4.TEM_DEDUTORA&#13;&#10;                                     , SUM(A4.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                  FROM ( &#13;&#10;                                        SELECT B5.ID_ESTADO&#13;&#10;                                             , B5.CP_ANO&#13;&#10;                                             , B5.ID_PLANO_CONTA_CP&#13;&#10;                                             , B5.ID_PLANO_CONTA_PAI&#13;&#10;                                             , B5.NIVEL&#13;&#10;                                             , A5.ID_TP_DEDUCAO       &#13;&#10;                                             , A5.TEM_DEDUTORA&#13;&#10;                                             , SUM(A5.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                          FROM ( &#13;&#10;                                                SELECT B6.ID_ESTADO&#13;&#10;                                                     , B6.CP_ANO&#13;&#10;                                                     , B6.ID_PLANO_CONTA_CP&#13;&#10;                                                     , B6.ID_PLANO_CONTA_PAI&#13;&#10;                                                     , B6.NIVEL&#13;&#10;                                                     , A6.ID_TP_DEDUCAO      &#13;&#10;                                                     , A6.TEM_DEDUTORA&#13;&#10;                                                     , SUM(A6.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                  FROM ( &#13;&#10;                                                        SELECT B7.ID_ESTADO&#13;&#10;                                                             , B7.CP_ANO&#13;&#10;                                                             , B7.ID_PLANO_CONTA_CP&#13;&#10;                                                             , B7.ID_PLANO_CONTA_PAI&#13;&#10;                                                             , B7.NIVEL&#13;&#10;                                                             , A7.ID_TP_DEDUCAO  &#13;&#10;                                                             , A7.TEM_DEDUTORA&#13;&#10;                                                             , SUM(A7.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                          FROM ( &#13;&#10;                                                                SELECT B8.ID_ESTADO&#13;&#10;                                                                     , B8.CP_ANO&#13;&#10;                                                                     , B8.ID_PLANO_CONTA_CP&#13;&#10;                                                                     , B8.ID_PLANO_CONTA_PAI&#13;&#10;                                                                     , B8.NIVEL&#13;&#10;                                                                     , A8.ID_TP_DEDUCAO  &#13;&#10;                                                                     , A8.TEM_DEDUTORA&#13;&#10;                                                                     , SUM(A8.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                                  FROM ( &#13;&#10;                                                                        SELECT B9.ID_ESTADO &#13;&#10;                                                                             , B9.CP_ANO &#13;&#10;                                                                             , B9.ID_PLANO_CONTA_CP &#13;&#10;                                                                             , B9.ID_PLANO_CONTA_PAI &#13;&#10;                                                                             , B9.NIVEL &#13;&#10;                                                                             , A9.ID_TP_DEDUCAO   &#13;&#10;                                                                             , A9.TEM_DEDUTORA&#13;&#10;                                                                             , SUM(A9.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                                          FROM ( &#13;&#10;                                                                                SELECT A10.ID_ESTADO &#13;&#10;                                                                                     , A10.CP_ANO &#13;&#10;                                                                                     , A10.ID_PLANO_CONTA_CP &#13;&#10;                                                                                     , A10.ID_PLANO_CONTA_PAI &#13;&#10;                                                                                     , A10.NIVEL &#13;&#10;                                                                                     , A10.ID_TP_DEDUCAO&#13;&#10;                                                                                     , A10.TEM_DEDUTORA&#13;&#10;                                                                                     , A10.VL_ARRECADADO&#13;&#10;                                                                                  FROM (&#13;&#10;                                                                                        SELECT PPLCO.ID_ESTADO &#13;&#10;                                                                                             , PPLCO.CP_ANO &#13;&#10;                                                                                             , PPLCO.ID_PLANO_CONTA_CP &#13;&#10;                                                                                             , PPLCO.ID_PLANO_CONTA_PAI &#13;&#10;                                                                                             , PPLCO.NIVEL&#13;&#10;                                                                                             , PFICH.ID_TP_DEDUCAO&#13;&#10;                                                                                             , COALESCE(PFDED.TEM_DEDUTORA, 0)  AS TEM_DEDUTORA&#13;&#10;                                                                                             , COALESCE(PFMOV.VL_ARRECADADO, 0) AS VL_ARRECADADO&#13;&#10;                                                                                          FROM PEC_FICHA PFICH&#13;&#10;                                                                                               LEFT JOIN PEC_PLANO_CONTA PPLCO  ON PPLCO.ID_PLANO_CONTA = PFICH.ID_PLANO_CONTA&#13;&#10;&#13;&#10;                                                                                               LEFT JOIN LATERAL&#13;&#10;                                                                                                         (&#13;&#10;                                                                                                           SELECT CASE WHEN COUNT(1) &#62; 0 THEN 1 ELSE 0 END AS TEM_DEDUTORA&#13;&#10;                                                                                                             FROM PEC_FICHA FICHD&#13;&#10;                                                                                                            WHERE FICHD.ID_ENTIDADE    = :ID_ENTIDADE&#13;&#10;                                                                                                              AND FICHD.CP_ANO         = :CP_ANO&#13;&#10;                                                                                                              AND FICHD.ID_TP_NATUREZA = 1&#13;&#10;                                                                                                              AND FICHD.ID_PLANO_CONTA = PFICH.ID_PLANO_CONTA&#13;&#10;                                                                                                              AND FICHD.ID_TP_DEDUCAO  IS NOT NULL&#13;&#10;                                                                                                         ) PFDED ON TRUE&#13;&#10;&#13;&#10;                                                                                               LEFT JOIN LATERAL&#13;&#10;                                                                                                         ( SELECT SUM(PCXMV.VL_MOVIMENTO) AS VL_ARRECADADO&#13;&#10;                                                                                                             FROM PEC_CAIXA_MOVIMENTO PCXMV&#13;&#10;                                                                                                                  INNER JOIN PEC_CAIXA PCAIX ON PCAIX.ID_CAIXA = PCXMV.ID_CAIXA&#13;&#10;                                                                                                            WHERE PCXMV.CP_ANO                 = :CP_ANO&#13;&#10;                                                                                                              AND PCXMV.ID_ST_LANCAMENTO_CAIXA = 1&#13;&#10;                                                                                                              AND PCAIX.CP_MES                 = :CP_MES&#13;&#10;                                                                                                              AND PCXMV.ID_FICHA               = PFICH.ID_FICHA&#13;&#10;                                                                                                         ) PFMOV  ON TRUE&#13;&#10;                                                                                                           &#13;&#10;                                                                                         WHERE PFICH.CP_ANO         = :CP_ANO &#13;&#10;                                                                                           AND PFICH.ID_TP_NATUREZA = 1 &#13;&#10;                                                                                           AND PFICH.ID_ENTIDADE    = :ID_ENTIDADE &#13;&#10;                                                                                           AND COALESCE(PPLCO.ID_TP_CALCULO_PASEP_USUARIO, PPLCO.ID_TP_CALCULO_PASEP) IN (1,2,3)&#13;&#10;                                                                                       ) A10&#13;&#10;                                                                               ) A9&#13;&#10;                                                                               LEFT  JOIN PEC_PLANO_CONTA B9 ON B9.ID_ESTADO = :ID_ESTADO&#13;&#10;                                                                                                            AND B9.CP_ANO    = :CP_ANO&#13;&#10;                                                                                                            AND ( B9.ID_PLANO_CONTA_CP = A9.ID_PLANO_CONTA_CP OR&#13;&#10;                                                                                                                ( B9.ID_PLANO_CONTA_CP = A9.ID_PLANO_CONTA_PAI AND A9.NIVEL = 10 ))&#13;&#10;                                                                         GROUP BY &#13;&#10;                                                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                                            ) A8 &#13;&#10;                                                                            LEFT  JOIN PEC_PLANO_CONTA B8 ON B8.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                                         AND B8.CP_ANO    = :CP_ANO &#13;&#10;                                                                                                         AND ( B8.ID_PLANO_CONTA_CP = A8.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                                             ( B8.ID_PLANO_CONTA_CP = A8.ID_PLANO_CONTA_PAI AND A8.NIVEL = 9 )) &#13;&#10;                                                                 GROUP BY &#13;&#10;                                                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                                    ) A7 &#13;&#10;                                                                    LEFT  JOIN PEC_PLANO_CONTA B7 ON B7.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                                 AND B7.CP_ANO    = :CP_ANO &#13;&#10;                                                                                                 AND ( B7.ID_PLANO_CONTA_CP = A7.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                                     ( B7.ID_PLANO_CONTA_CP = A7.ID_PLANO_CONTA_PAI AND A7.NIVEL = 8 )) &#13;&#10;                                                         GROUP BY &#13;&#10;                                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                            ) A6 &#13;&#10;                                                            LEFT  JOIN PEC_PLANO_CONTA B6 ON B6.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                         AND B6.CP_ANO    = :CP_ANO &#13;&#10;                                                                                         AND ( B6.ID_PLANO_CONTA_CP = A6.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                             ( B6.ID_PLANO_CONTA_CP = A6.ID_PLANO_CONTA_PAI AND A6.NIVEL = 7 )) &#13;&#10;                                                 GROUP BY &#13;&#10;                                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                    ) A5 &#13;&#10;                                                    LEFT  JOIN PEC_PLANO_CONTA B5 ON B5.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                 AND B5.CP_ANO    = :CP_ANO &#13;&#10;                                                                                 AND ( B5.ID_PLANO_CONTA_CP = A5.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                     ( B5.ID_PLANO_CONTA_CP = A5.ID_PLANO_CONTA_PAI AND A5.NIVEL = 6 )) &#13;&#10;                                         GROUP BY &#13;&#10;                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                            ) A4 &#13;&#10;                                            LEFT  JOIN PEC_PLANO_CONTA B4 ON B4.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                         AND B4.CP_ANO    = :CP_ANO &#13;&#10;                                                                         AND ( B4.ID_PLANO_CONTA_CP = A4.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                             ( B4.ID_PLANO_CONTA_CP = A4.ID_PLANO_CONTA_PAI AND A4.NIVEL = 5 )) &#13;&#10;                                 GROUP BY &#13;&#10;                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                    ) A3 &#13;&#10;                                    LEFT  JOIN PEC_PLANO_CONTA B3 ON B3.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                 AND B3.CP_ANO    = :CP_ANO &#13;&#10;                                                                 AND ( B3.ID_PLANO_CONTA_CP = A3.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                     ( B3.ID_PLANO_CONTA_CP = A3.ID_PLANO_CONTA_PAI AND A3.NIVEL = 4 )) &#13;&#10;                         GROUP BY &#13;&#10;                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                            ) A2 &#13;&#10;                            LEFT  JOIN PEC_PLANO_CONTA B2 ON B2.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                         AND B2.CP_ANO    = :CP_ANO &#13;&#10;                                                         AND ( B2.ID_PLANO_CONTA_CP = A2.ID_PLANO_CONTA_CP OR &#13;&#10;                                                             ( B2.ID_PLANO_CONTA_CP = A2.ID_PLANO_CONTA_PAI AND A2.NIVEL = 3 )) &#13;&#10;                 GROUP BY &#13;&#10;                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                    ) A1 &#13;&#10;                    LEFT  JOIN PEC_PLANO_CONTA B1 ON B1.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                 AND B1.CP_ANO    = :CP_ANO &#13;&#10;                                                 AND ( B1.ID_PLANO_CONTA_CP = A1.ID_PLANO_CONTA_CP OR &#13;&#10;                                                     ( B1.ID_PLANO_CONTA_CP = A1.ID_PLANO_CONTA_PAI AND A1.NIVEL = 2 )) &#13;&#10;                    LEFT  JOIN PEC_TP_DEDUCAO  C1 ON (C1.ID_TP_DEDUCAO  = A1.ID_TP_DEDUCAO)&#13;&#10;         WHERE B1.NIVEL IN (1,2)&#13;&#10;         GROUP BY &#13;&#10;               1, 2, 3, 4, 5, 6&#13;&#10;       ) A&#13;&#10;       LEFT JOIN SIS_SEQUENCIA B ON B.ID_SEQUENCIA &#60;= 4&#13;&#10; WHERE (A.NIVEL = 1 AND B.ID_SEQUENCIA = 1                                  )&#13;&#10;    OR (A.NIVEL = 2 AND B.ID_SEQUENCIA = 2                                  )&#13;&#10;    OR (A.NIVEL = 2 AND B.ID_SEQUENCIA = 3 AND A.TEM_DEDUTORA  = 1          )&#13;&#10;    OR (A.NIVEL = 2 AND B.ID_SEQUENCIA = 4 AND A.CD_TP_DEDUCAO IS NOT NULL  )&#13;&#10; GROUP BY&#13;&#10;       1,2,3&#13;&#10; ORDER BY &#13;&#10;       1                  " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlRetencaoPasep" UserName="SqlRetencaoPasep" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT B1.CD_PLANO_CONTA_REL&#13;&#10;     , B1.DS_PLANO_CONTA&#13;&#10;     , B1.NIVEL                            &#13;&#10;     , ROUND(SUM(A1.VL_ARRECADADO), 2)    AS VL_ARRECADADO&#13;&#10;     , ROUND(SUM(A1.VL_ARRECADADO)/100,2) AS VL_RETIDO                           &#13;&#10;  FROM (&#13;&#10;        SELECT B2.ID_ESTADO&#13;&#10;             , B2.CP_ANO&#13;&#10;             , B2.ID_PLANO_CONTA_CP&#13;&#10;             , B2.ID_PLANO_CONTA_PAI&#13;&#10;             , B2.NIVEL&#13;&#10;             , A2.ID_TP_DEDUCAO&#13;&#10;             , A2.NIVEL_EXIBICAO&#13;&#10;             , SUM(A2.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;          FROM ( &#13;&#10;                SELECT B3.ID_ESTADO&#13;&#10;                     , B3.CP_ANO&#13;&#10;                     , B3.ID_PLANO_CONTA_CP&#13;&#10;                     , B3.ID_PLANO_CONTA_PAI&#13;&#10;                     , B3.NIVEL&#13;&#10;                     , A3.ID_TP_DEDUCAO&#13;&#10;                     , A3.NIVEL_EXIBICAO&#13;&#10;                     , SUM(A3.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                  FROM ( &#13;&#10;                        SELECT B4.ID_ESTADO&#13;&#10;                             , B4.CP_ANO&#13;&#10;                             , B4.ID_PLANO_CONTA_CP&#13;&#10;                             , B4.ID_PLANO_CONTA_PAI&#13;&#10;                             , B4.NIVEL&#13;&#10;                             , A4.ID_TP_DEDUCAO&#13;&#10;                             , A4.NIVEL_EXIBICAO&#13;&#10;                             , SUM(A4.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                          FROM ( &#13;&#10;                                SELECT B5.ID_ESTADO&#13;&#10;                                     , B5.CP_ANO&#13;&#10;                                     , B5.ID_PLANO_CONTA_CP&#13;&#10;                                     , B5.ID_PLANO_CONTA_PAI&#13;&#10;                                     , B5.NIVEL&#13;&#10;                                     , A5.ID_TP_DEDUCAO&#13;&#10;                                     , a5.NIVEL_EXIBICAO&#13;&#10;                                     , SUM(A5.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                  FROM ( &#13;&#10;                                        SELECT B6.ID_ESTADO&#13;&#10;                                             , B6.CP_ANO&#13;&#10;                                             , B6.ID_PLANO_CONTA_CP&#13;&#10;                                             , B6.ID_PLANO_CONTA_PAI&#13;&#10;                                             , B6.NIVEL&#13;&#10;                                             , A6.ID_TP_DEDUCAO&#13;&#10;                                             , A6.NIVEL_EXIBICAO&#13;&#10;                                             , SUM(A6.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                          FROM ( &#13;&#10;                                                SELECT B7.ID_ESTADO&#13;&#10;                                                     , B7.CP_ANO&#13;&#10;                                                     , B7.ID_PLANO_CONTA_CP&#13;&#10;                                                     , B7.ID_PLANO_CONTA_PAI&#13;&#10;                                                     , B7.NIVEL&#13;&#10;                                                     , A7.ID_TP_DEDUCAO&#13;&#10;                                                     , A7.NIVEL_EXIBICAO&#13;&#10;                                                     , SUM(A7.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                  FROM ( &#13;&#10;                                                        SELECT B8.ID_ESTADO&#13;&#10;                                                             , B8.CP_ANO&#13;&#10;                                                             , B8.ID_PLANO_CONTA_CP&#13;&#10;                                                             , B8.ID_PLANO_CONTA_PAI&#13;&#10;                                                             , B8.NIVEL&#13;&#10;                                                             , A8.ID_TP_DEDUCAO&#13;&#10;                                                             , A8.NIVEL_EXIBICAO&#13;&#10;                                                             , SUM(A8.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                          FROM ( &#13;&#10;                                                                SELECT B9.ID_ESTADO &#13;&#10;                                                                     , B9.CP_ANO &#13;&#10;                                                                     , B9.ID_PLANO_CONTA_CP &#13;&#10;                                                                     , B9.ID_PLANO_CONTA_PAI &#13;&#10;                                                                     , B9.NIVEL &#13;&#10;                                                                     , A9.ID_TP_DEDUCAO&#13;&#10;                                                                     , A9.NIVEL_EXIBICAO&#13;&#10;                                                                     , SUM(A9.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                                  FROM ( &#13;&#10;                                                                        SELECT A10.ID_ESTADO &#13;&#10;                                                                             , A10.CP_ANO &#13;&#10;                                                                             , A10.ID_PLANO_CONTA_CP &#13;&#10;                                                                             , A10.ID_PLANO_CONTA_PAI &#13;&#10;                                                                             , A10.NIVEL &#13;&#10;                                                                             , A10.ID_TP_DEDUCAO&#13;&#10;                                                                             , A10.NIVEL_EXIBICAO&#13;&#10;                                                                             , A10.VL_ARRECADADO&#13;&#10;                                                                          FROM (SELECT PPLCO.ID_ESTADO &#13;&#10;                                                                                     , PPLCO.CP_ANO &#13;&#10;                                                                                     , PPLCO.ID_PLANO_CONTA_CP &#13;&#10;                                                                                     , PPLCO.ID_PLANO_CONTA_PAI &#13;&#10;                                                                                     , PPLCO.NIVEL&#13;&#10;                                                                                     , PFICH.ID_TP_DEDUCAO&#13;&#10;                                                                                     , CAST(7 AS INTEGER)               AS NIVEL_EXIBICAO&#13;&#10;                                                                                     , COALESCE(PFMOV.VL_ARRECADADO, 0) AS VL_ARRECADADO&#13;&#10;                                                                                  FROM PEC_FICHA PFICH&#13;&#10;                                                                                       LEFT JOIN PEC_PLANO_CONTA PPLCO  ON PPLCO.ID_PLANO_CONTA = PFICH.ID_PLANO_CONTA&#13;&#10;                                                                                       LEFT JOIN LATERAL               &#13;&#10;                                                                                                 ( &#13;&#10;                                                                                                   SELECT SUM(PCXMV.VL_MOVIMENTO) AS VL_ARRECADADO&#13;&#10;                                                                                                     FROM PEC_CAIXA_MOVIMENTO PCXMV&#13;&#10;                                                                                                          INNER JOIN PEC_CAIXA PCAIX ON PCAIX.ID_CAIXA = PCXMV.ID_CAIXA&#13;&#10;                                                                                                    WHERE PCXMV.CP_ANO                 = :CP_ANO&#13;&#10;                                                                                                      AND PCXMV.ID_ST_LANCAMENTO_CAIXA = 1&#13;&#10;                                                                                                      AND PCXMV.ID_FICHA               = PFICH.ID_FICHA                                                                                                          &#13;&#10;                                                                                                      AND PCAIX.CP_MES                 = :CP_MES                                                                                                                   &#13;&#10;                                                                                                 ) PFMOV  ON TRUE      &#13;&#10;                                                                                 WHERE PFICH.CP_ANO         = :CP_ANO &#13;&#10;                                                                                   AND PFICH.ID_TP_NATUREZA = 1 &#13;&#10;                                                                                   AND PFICH.ID_ENTIDADE    = :ID_ENTIDADE &#13;&#10;                                                                                   AND PFICH.ID_TP_DEDUCAO IS NULL&#13;&#10;                                                                                   AND COALESCE(PPLCO.ID_TP_CALCULO_PASEP_USUARIO, PPLCO.ID_TP_CALCULO_PASEP) = 3&#13;&#10;                                                                               ) A10&#13;&#10;                                                                       ) A9&#13;&#10;                                                                       LEFT  JOIN PEC_PLANO_CONTA B9 ON B9.ID_ESTADO = :ID_ESTADO&#13;&#10;                                                                                                    AND B9.CP_ANO    = :CP_ANO&#13;&#10;                                                                                                    AND ( B9.ID_PLANO_CONTA_CP = A9.ID_PLANO_CONTA_CP OR&#13;&#10;                                                                                                        ( B9.ID_PLANO_CONTA_CP = A9.ID_PLANO_CONTA_PAI AND A9.NIVEL = 10 ))&#13;&#10;                                                                 GROUP BY &#13;&#10;                                                                       1, 2, 3, 4, 5, 6, 7 &#13;&#10;                                                                    ) A8 &#13;&#10;                                                                    LEFT  JOIN PEC_PLANO_CONTA B8 ON B8.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                                 AND B8.CP_ANO    = :CP_ANO &#13;&#10;                                                                                                 AND ( B8.ID_PLANO_CONTA_CP = A8.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                                     ( B8.ID_PLANO_CONTA_CP = A8.ID_PLANO_CONTA_PAI AND A8.NIVEL = 9 )) &#13;&#10;                                                         GROUP BY &#13;&#10;                                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                            ) A7 &#13;&#10;                                                            LEFT  JOIN PEC_PLANO_CONTA B7 ON B7.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                         AND B7.CP_ANO    = :CP_ANO &#13;&#10;                                                                                         AND ( B7.ID_PLANO_CONTA_CP = A7.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                             ( B7.ID_PLANO_CONTA_CP = A7.ID_PLANO_CONTA_PAI AND A7.NIVEL = 8 )) &#13;&#10;                                                 GROUP BY &#13;&#10;                                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                    ) A6 &#13;&#10;                                                    LEFT  JOIN PEC_PLANO_CONTA B6 ON B6.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                 AND B6.CP_ANO    = :CP_ANO &#13;&#10;                                                                                 AND ( B6.ID_PLANO_CONTA_CP = A6.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                     ( B6.ID_PLANO_CONTA_CP = A6.ID_PLANO_CONTA_PAI AND A6.NIVEL = 7 )) &#13;&#10;                                         GROUP BY &#13;&#10;                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                            ) A5 &#13;&#10;                                            LEFT  JOIN PEC_PLANO_CONTA B5 ON B5.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                         AND B5.CP_ANO    = :CP_ANO &#13;&#10;                                                                         AND ( B5.ID_PLANO_CONTA_CP = A5.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                             ( B5.ID_PLANO_CONTA_CP = A5.ID_PLANO_CONTA_PAI AND A5.NIVEL = 6 )) &#13;&#10;                                 GROUP BY &#13;&#10;                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                    ) A4 &#13;&#10;                                    LEFT  JOIN PEC_PLANO_CONTA B4 ON B4.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                 AND B4.CP_ANO    = :CP_ANO &#13;&#10;                                                                 AND ( B4.ID_PLANO_CONTA_CP = A4.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                     ( B4.ID_PLANO_CONTA_CP = A4.ID_PLANO_CONTA_PAI AND A4.NIVEL = 5 )) &#13;&#10;                         GROUP BY &#13;&#10;                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                            ) A3 &#13;&#10;                            LEFT  JOIN PEC_PLANO_CONTA B3 ON B3.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                         AND B3.CP_ANO    = :CP_ANO &#13;&#10;                                                         AND ( B3.ID_PLANO_CONTA_CP = A3.ID_PLANO_CONTA_CP OR &#13;&#10;                                                             ( B3.ID_PLANO_CONTA_CP = A3.ID_PLANO_CONTA_PAI AND A3.NIVEL = 4 )) &#13;&#10;                 GROUP BY &#13;&#10;                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                    ) A2 &#13;&#10;                    LEFT  JOIN PEC_PLANO_CONTA B2 ON B2.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                 AND B2.CP_ANO    = :CP_ANO &#13;&#10;                                                 AND ( B2.ID_PLANO_CONTA_CP = A2.ID_PLANO_CONTA_CP OR &#13;&#10;                                                     ( B2.ID_PLANO_CONTA_CP = A2.ID_PLANO_CONTA_PAI AND A2.NIVEL = 3 )) &#13;&#10;         GROUP BY &#13;&#10;               1, 2, 3, 4, 5, 6, 7&#13;&#10;            ) A1 &#13;&#10;            LEFT  JOIN PEC_PLANO_CONTA B1 ON B1.ID_ESTADO = :ID_ESTADO &#13;&#10;                                         AND B1.CP_ANO    = :CP_ANO &#13;&#10;                                         AND ( B1.ID_PLANO_CONTA_CP = A1