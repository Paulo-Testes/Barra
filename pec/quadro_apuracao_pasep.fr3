10;                                                                                                         ( SELECT SUM(PCXMV.VL_MOVIMENTO *-1) AS VL_ARRECADADO&#13;&#10;                                                                                                             FROM PEC_CAIXA_MOVIMENTO PCXMV&#13;&#10;                                                                                                                  INNER JOIN PEC_CAIXA PCAIX ON PCAIX.ID_CAIXA = PCXMV.ID_CAIXA&#13;&#10;                                                                                                            WHERE PCXMV.CP_ANO                 = :CP_ANO&#13;&#10;                                                                                                              AND PCXMV.ID_ST_LANCAMENTO_CAIXA = 1&#13;&#10;                                                                                                              AND PCAIX.CP_MES                 = :CP_MES&#13;&#10;                                                                                                              AND PCXMV.ID_FICHA               = PFICH.ID_FICHA&#13;&#10;                                                                                                         ) PFMOV  ON TRUE  &#13;&#10;                                                                                                                                                                              &#13;&#10;                                                                                         WHERE PFICH.CP_ANO         = :CP_ANO &#13;&#10;                                                                                           AND PFICH.ID_TP_NATUREZA = 1 &#13;&#10;                                                                                           AND PFICH.ID_ENTIDADE    = :ID_ENTIDADE &#13;&#10;                                                                                           AND COALESCE(PPLCO.ID_TP_CALCUL