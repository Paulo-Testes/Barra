      <TableDataSource Name="Table3" Alias="SqlConciliacao" DataType="System.Int32" Enabled="true" SelectCommand="SELECT XX.ID_CONTA_BANCARIA&#13;&#10;&#13;&#10;     , SUM(XX.SD_CONTABIL)      AS SD_CONTABIL&#13;&#10;     , SUM(XX.VL_CONT_RETIRADA) AS VL_CONT_RETIRADA&#13;&#10;     , SUM(XX.VL_CONT_DEPOSITO) AS VL_CONT_DEPOSITO&#13;&#10;     , SUM(XX.VL_EXTR_RETIRADA) AS VL_EXTR_RETIRADA&#13;&#10;     , SUM(XX.VL_EXTR_DEPOSITO) AS VL_EXTR_DEPOSITO&#13;&#10;&#13;&#10;     , SUM(XX.SD_CONTABIL&#13;&#10;           - ( XX.VL_CONT_DEPOSITO )&#13;&#10;           + ( XX.VL_CONT_RETIRADA )&#13;&#10;           + ( XX.VL_EXTR_DEPOSITO )&#13;&#10;           - ( XX.VL_EXTR_RETIRADA ) ) AS SD_EXTRATO&#13;&#10;  FROM (&#13;&#10;&#13;&#10;        SELECT A2.ID_CONTA_BANCARIA&#13;&#10;             , A2.NR_FICHA&#13;&#10;             , A2.SD_INICIAL + COALESCE(B2.VL_MOVIMENTO, 0)                         AS SD_CONTABIL&#13;&#10;             , COALESCE(C2.VL_CONT_RETIRADA, 0) + COALESCE(D2.VL_CONT_RETIRADA, 0)  AS VL_CONT_RETIRADA&#13;&#10;             , COALESCE(C2.VL_CONT_DEPOSITO, 0) + COALESCE(D2.VL_CONT_DEPOSITO, 0)  AS VL_CONT_DEPOSITO&#13;&#10;             , COALESCE(D2.VL_EXTR_RETIRADA, 0)                                     AS VL_EXTR_RETIRADA&#13;&#10;             , COALESCE(D2.VL_EXTR_DEPOSITO, 0)                                     AS VL_EXTR_DEPOSITO&#13;&#10;          FROM (&#13;&#10;                SELECT FICHA.ID_CONTA_BANCARIA&#13;&#10;                     , FICHA.NR_FICHA&#13;&#10;                     , SUM(FICHA.VL_SALDO) AS SD_INICIAL&#13;&#10;                  FROM PEC_FICHA FICHA&#13;&#10;                       LEFT JOIN PEC_PLANO_CONTA PCONT ON PCONT.ID_PLANO_CONTA = FICHA.ID_PLANO_CONTA&#13;&#10;                 WHERE FICHA.ID_CONTA_BANCARIA = :ID_CONTA_BANCARIA&#13;&#10;                   AND FICHA.CP_ANO     = :CP_ANO&#13;&#10;                   AND ( FICHA.ID_FICHA = :ID_FICHA OR 0 = :CONCILIACAO_POR_FICHA )&#13;&#10;                   AND ( PCONT.CD_CONTA_CONTABIL LIKE '1.1.1%' OR (PCONT.CD_CONTA_CONTABIL LIKE '1.1.4%' AND 1 = :CONCILIA_APLICACAO_RPPS) )&#13;&#10;                 GROUP BY 1, 2&#13;&#10;               ) A2&#13;&#10;               /* MOVIMENTACOES ATE A DATA */                         &#13;&#10;               LEFT JOIN LATERAL&#13;&#10;                         (&#13;&#10;                          SELECT CXMOV.ID_CONTA_BANCARIA&#13;&#10;                               , CXMOV.NR_FICHA&#13;&#10;                               , SUM(CXMOV.VL_MOVIMENTO *-1) AS VL_MOVIMENTO&#13;&#10;                            FROM PEC_CAIXA_MOVIMENTO CXMOV&#13;&#10;                                 LEFT JOIN PEC_FICHA       FICHA ON FICHA.ID_FICHA       = CXMOV.ID_FICHA &#13;&#10;                                 LEFT JOIN PEC_PLANO_CONTA PCONT ON PCONT.ID_PLANO_CONTA = FICHA.ID_PLANO_CONTA                                             &#13;&#10;                           WHERE CXMOV.CP_ANO                    = :CP_ANO&#13;&#10;                             AND CXMOV.ID_TP_CAIXA_MOVIMENTO     = 3 &#13;&#10;                             AND CXMOV.ID_ST_LANCAMENTO_CAIXA    = 1 &#13;&#10;                             AND CXMOV.DT_MOVIMENTO             &lt;= CAST(:DT_CONCILIACAO AS DATE) &#13;&#10;                             AND CXMOV.ID_CONTA_BANCARIA         = A2.ID_CONTA_BANCARIA AND CXMOV.NR_FICHA = A2.NR_FICHA&#13;&#10;                             AND ( PCONT.CD_CONTA_CONTABIL LIKE '1.1.1%' OR (PCONT.CD_CONTA_CONTABIL LIKE '1.1.4%' AND 1 = :CONCILIA_APLICACAO_RPPS) )&#13;&#10;                           GROUP BY 1, 2&#13;&#10;                         ) B2 ON B2.ID_CONTA_BANCARIA = A2.ID_CONTA_BANCARIA&#13;&#10;                             AND B2.NR_FICHA          = A2.NR_FICHA&#13;&#10;&#13;&#10;               /* PENDENCIAS CONTABEIS DE LANCAMENTOS */&#13;&#10;               LEFT JOIN LATERAL&#13;&#10;                         (&#13;&#10;                          SELECT PDMOV.ID_CONTA_BANCARIA&#13;&#10;                               , PDMOV.NR_FICHA&#13;&#10;                               , SUM(CASE WHEN PDMOV.ID_TP_NATUREZA_CONTABIL = 1 THEN PDMOV.VL_MOVIMENTO     ELSE 0 END) AS VL_CONT_RETIRADA&#13;&#10;                               , SUM(CASE WHEN PDMOV.ID_TP_NATUREZA_CONTABIL = 2 THEN PDMOV.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_CONT_DEPOSITO&#13;&#10;                            FROM PEC_CAIXA_MOVIMENTO PDMOV&#13;&#10;                                 LEFT JOIN PEC_FICHA       FICHA ON FICHA.ID_FICHA       = PDMOV.ID_FICHA&#13;&#10;                                 LEFT JOIN PEC_PLANO_CONTA PCONT ON PCONT.ID_PLANO_CONTA = FICHA.ID_PLANO_CONTA                                             &#13;&#10;                           WHERE PDMOV.ID_TP_CAIXA_MOVIMENTO     = 3&#13;&#10;                             AND PDMOV.ID_ST_LANCAMENTO_CAIXA    = 1&#13;&#10;                             AND PDMOV.CP_ANO_CONCILIACAO       &gt;= :CP_ANO&#13;&#10;                             AND PDMOV.DT_MOVIMENTO             &lt;= CAST(:DT_CONCILIACAO AS DATE)&#13;&#10;                             AND PDMOV.ID_CONTA_BANCARIA         = A2.ID_CONTA_BANCARIA AND PDMOV.NR_FICHA = A2.NR_FICHA                             &#13;&#10;                             AND ( PDMOV.DT_COMPENSACAO         IS NULL OR PDMOV.DT_COMPENSACAO &gt; CAST(:DT_CONCILIACAO AS DATE) )&#13;&#10;                             AND ( PCONT.CD_CONTA_CONTABIL LIKE '1.1.1%' OR (PCONT.CD_CONTA_CONTABIL LIKE '1.1.4%' AND 1 = :CONCILIA_APLICACAO_RPPS) )&#13;&#10;                           GROUP BY 1, 2&#13;&#10;                         ) C2 ON C2.ID_CONTA_BANCARIA = A2.ID_CONTA_BANCARIA&#13;&#10;                             AND C2.NR_FICHA          = A2.NR_FICHA&#13;&#10;&#13;&#10;               /* PENDENCIAS CADASTRADAS */&#13;&#10;               LEFT JOIN LATERAL&#13;&#10;                         (&#13;&#10;                          SELECT PENDE.ID_CONTA_BANCARIA&#13;&#10;                               , PENDE.NR_FICHA&#13;&#10;                               , SUM(CASE WHEN PENDE.PENDENCIA_CONTABIL = 1 AND PENDE.ID_TP_NATUREZA_CONTABIL = 1 THEN PENDE.VL_MOVIMENTO     ELSE 0 END) AS VL_CONT_RETIRADA&#13;&#10;                               , SUM(CASE WHEN PENDE.PENDENCIA_CONTABIL = 1 AND PENDE.ID_TP_NATUREZA_CONTABIL = 2 THEN PENDE.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_CONT_DEPOSITO&#13;&#10;                               , SUM(CASE WHEN PENDE.PENDENCIA_CONTABIL = 0 AND PENDE.ID_TP_NATUREZA_CONTABIL = 1 THEN PENDE.VL_MOVIMENTO     ELSE 0 END) AS VL_EXTR_RETIRADA&#13;&#10;                               , SUM(CASE WHEN PENDE.PENDENCIA_CONTABIL = 0 AND PENDE.ID_TP_NATUREZA_CONTABIL = 2 THEN PENDE.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_EXTR_DEPOSITO                               &#13;&#10;                            FROM PEC_CONCILIACAO_PENDENCIA PENDE &#13;&#10;                                 LEFT JOIN PEC_FICHA       FICHA ON FICHA.ID_FICHA       = PENDE.ID_FICHA &#13;&#10;                                 LEFT JOIN PEC_PLANO_CONTA PCONT ON PCONT.ID_PLANO_CONTA = FICHA.ID_PLANO_CONTA                                                                                                  &#13;&#10;                           WHERE PENDE.CP_ANO_CONCILIACAO       &gt;= :CP_ANO&#13;&#10;                             AND PENDE.DT_MOVIMENTO             &lt;= CAST(:DT_CONCILIACAO AS DATE)&#13;&#10;                             AND PENDE.ID_CONTA_BANCARIA         = A2.ID_CONTA_BANCARIA AND PENDE.NR_FICHA = A2.NR_FICHA&#13;&#10;                             AND ( PENDE.DT_COMPENSACAO          IS NULL OR PENDE.DT_COMPENSACAO &gt; CAST(:DT_CONCILIACAO AS DATE) )&#13;&#10;                             AND ( PCONT.CD_CONTA_CONTABIL LIKE '1.1.1%' OR (PCONT.CD_CONTA_CONTABIL LIKE '1.1.4%' AND 1 = :CONCILIA_APLICACAO_RPPS) )&#13;&#10;                           GROUP BY 1, 2&#13;&#10;                         ) D2 ON D2.ID_CONTA_BANCARIA = A2.ID_CONTA_BANCARIA&#13;&#10;                             AND D2.NR_FICHA          = A2.NR_FICHA&#13;&#10;&#13;&#10;       ) XX&#13;&#10;&#13;&#10; GROUP BY 1">
