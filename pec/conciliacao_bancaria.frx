    private void agenciaBancaria_BeforePrint(object sender, EventArgs e)
    {
      string cdAgencia = Report.GetColumnValue(&quot;SqlContaBancaria.CD_AGENCIA&quot;).ToString();
      string dvAgencia = Report.GetColumnValue(&quot;SqlContaBancaria.DV_AGENCIA&quot;).ToString();

      if (!string.IsNullOrEmpty(dvAgencia))
        agenciaBancaria.Text = ((String)Report.GetColumnValue(&quot;SqlContaBancaria.cd_agencia&quot;)) + &quot;-&quot; + ((String)Report.GetColumnValue(&quot;SqlContaBancaria.dv_agencia&quot;));
      else
