      <TableDataSource Name="Table1" Alias="SqlContaBancaria" DataType="System.Int32" Enabled="true" SelectCommand="SELECT DISTINCT&#13;&#10;       CONTA.ID_CONTA_BANCARIA&#13;&#10;     , BANCO.CD_BANCO&#13;&#10;     , BANCO.DS_BANCO&#13;&#10;     , AGENC.CD_AGENCIA&#13;&#10;     , AGENC.DV_AGENCIA&#13;&#10;     , CONTA.CD_CONTA_BANCARIA&#13;&#10;     , CONTA.DV_CONTA_BANCARIA&#13;&#10;     , CONTA.DS_CONTA_BANCARIA&#13;&#10;     , CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN FICHA.ID_FICHA ELSE CAST(:ID_FICHA AS INTEGER) END AS ID_FICHA&#13;&#10;     , CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN FICHA.NR_FICHA ELSE 0                          END AS NR_FICHA&#13;&#10;     , CAST(CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN PCONT.CD_PLANO_CONTA   END AS VARCHAR(30) )   AS CD_PLANO_CONTA&#13;&#10;     , CAST(CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN PCONT.DS_PLANO_CONTA   END AS VARCHAR(250))   AS DS_PLANO_CONTA&#13;&#10;     , CAST(CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN FONTE.CD_FONTE_RECURSO END AS VARCHAR(30) )   AS CD_FONTE_RECURSO&#13;&#10;     , CAST(CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN FONTE.DS_FONTE_RECURSO END AS VARCHAR(250))   AS DS_FONTE_RECURSO&#13;&#10;     , CAST(CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN APLIC.CD_APLICACAO     END AS VARCHAR(8)  )   AS CD_APLICACAO&#13;&#10;     , CAST(CASE WHEN 1 = :CONCILIACAO_POR_FICHA THEN APLIC.DS_APLICACAO     END AS VARCHAR(250))   AS DS_APLICACAO&#13;&#10;     , CAST(:CONCILIACAO_POR_FICHA   AS INTEGER)                                                    AS CONCILIACAO_POR_FICHA&#13;&#10;     , CAST(:CONCILIA_APLICACAO_RPPS AS INTEGER)                                                    AS CONCILIA_APLICACAO_RPPS&#13;&#10;     ,CAST ( MAKE_DATE(CAST(:ANO AS INTEGER),&#13;&#10;                        CAST(:MES AS INTEGER),&#13;&#10;                        CAST(:DIA AS INTEGER))    AS DATE)                                                  AS DT_CONCILIACAO&#13;&#10;     , CAST(:CP_ANO AS INTEGER)                                                                     AS CP_ANO&#13;&#10;     , CAST(LPAD(CAST(CONTA.ID_CONTA_BANCARIA AS VARCHAR(20)), 20, '0') ||&#13;&#10;            LPAD(CAST(CASE WHEN 1 = :CONCILIACAO_POR_FICHA&#13;&#10;                           THEN FICHA.NR_FICHA ELSE 0 END AS VARCHAR(20)), 20, '0') AS VARCHAR(40)) AS AGRUPAMENTO&#13;&#10;  FROM (&#13;&#10;         SELECT SRF.ID_FILTRO&#13;&#10;           FROM SIS_RELATORIO_FILTRO SRF&#13;&#10;          WHERE SRF.ID_RELATORIO_FILTRO = :ID_RELATORIO_FILTRO&#13;&#10;       ) FILTR&#13;&#10;       RIGHT JOIN PEC_CONTA_BANCARIA CONTA ON 1                          = 1&#13;&#10;                                          /*FILTRO_CONTA*/                                                                                                                                                                                                                             &#13;&#10;       LEFT JOIN PEC_AGENCIA         AGENC ON AGENC.ID_AGENCIA           = CONTA.ID_AGENCIA&#13;&#10;       LEFT JOIN SIS_BANCO           BANCO ON BANCO.ID_BANCO             = AGENC.ID_BANCO&#13;&#10;       LEFT JOIN PEC_FICHA           FICHA ON FICHA.ID_CONTA_BANCARIA    = CONTA.ID_CONTA_BANCARIA&#13;&#10;       LEFT JOIN PEC_PLANO_CONTA     PCONT ON PCONT.ID_PLANO_CONTA       = FICHA.ID_PLANO_CONTA&#13;&#10;       LEFT JOIN SIS_FONTE_RECURSO   FONTE ON FONTE.ID_FONTE_RECURSO     = FICHA.ID_FONTE_RECURSO&#13;&#10;       LEFT JOIN SIS_APLICACAO       APLIC ON APLIC.ID_APLICACAO         = FICHA.ID_APLICACAO&#13;&#10; WHERE CONTA.ID_ENTIDADE = :ID_ENTIDADE&#13;&#10;   /*FILTRO_CONTA*/&#13;&#10;   AND ( CONTA.ID_CONTA_BANCARIA = :ID_CONTA_BANCARIA OR -99999 = :ID_CONTA_BANCARIA )&#13;&#10;   AND ( FICHA.ID_ENTIDADE = :ID_ENTIDADE )&#13;&#10;   AND ( FICHA.CP_ANO      = :CP_ANO )&#13;&#10;   AND ( FICHA.ID_FICHA    = :ID_FICHA OR -99999 = :ID_FICHA )&#13;&#10;   AND ( PCONT.CD_CONTA_CONTABIL LIKE '1.1.1%' OR (PCONT.CD_CONTA_CONTABIL LIKE '1.1.4%' AND 1 = :CONCILIA_APLICACAO_RPPS) )&#13;&#10; ORDER BY&#13;&#10;       3, 5, 6, 7, 8, 11">
