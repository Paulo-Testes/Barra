      <TableDataSource Name="Table2" Alias="SqlFichas" DataType="System.Int32" Enabled="true" SelectCommand="SELECT A1.ID_CONTA_BANCARIA&#13;&#10;     , A1.ID_FICHA&#13;&#10;     , A1.NR_FICHA&#13;&#10;     , A1.CD_PLANO_CONTA&#13;&#10;     , A1.DS_PLANO_CONTA&#13;&#10;     , A1.CD_FONTE_RECURSO&#13;&#10;     , A1.DS_FONTE_RECURSO&#13;&#10;     , A1.CD_APLICACAO&#13;&#10;     , A1.DS_APLICACAO&#13;&#10;     , A1.ATIVO&#13;&#10;     , A1.VL_SALDO&#13;&#10;     , A1.VL_SALDO +&#13;&#10;       SUM(COALESCE(B1.VL_MOVIMENTO *-1, 0)) AS VL_SALDO_FINAL&#13;&#10;  FROM (&#13;&#10;        SELECT FICHA.ID_CONTA_BANCARIA&#13;&#10;             , FICHA.ID_FICHA&#13;&#10;             , FICHA.NR_FICHA&#13;&#10;             , PCONT.CD_PLANO_CONTA&#13;&#10;             , PCONT.DS_PLANO_CONTA&#13;&#10;             , FONTE.CD_FONTE_RECURSO&#13;&#10;             , FONTE.DS_FONTE_RECURSO&#13;&#10;             , APLIC.CD_APLICACAO&#13;&#10;             , APLIC.DS_APLICACAO&#13;&#10;             , FICHA.ATIVO&#13;&#10;             , FICHA.VL_SALDO&#13;&#10;          FROM PEC_FICHA FICHA&#13;&#10;               LEFT JOIN PEC_PLANO_CONTA    PCONT ON PCONT.ID_PLANO_CONTA       = FICHA.ID_PLANO_CONTA&#13;&#10;               LEFT JOIN SIS_FONTE_RECURSO  FONTE ON FONTE.ID_FONTE_RECURSO     = FICHA.ID_FONTE_RECURSO&#13;&#10;               LEFT JOIN SIS_APLICACAO      APLIC ON APLIC.ID_APLICACAO         = FICHA.ID_APLICACAO&#13;&#10;         WHERE FICHA.ID_CONTA_BANCARIA = :ID_CONTA_BANCARIA&#13;&#10;           AND FICHA.CP_ANO = :CP_ANO&#13;&#10;           AND (FICHA.ID_FICHA = :ID_FICHA OR 0 = :CONCILIACAO_POR_FICHA)&#13;&#10;           AND (PCONT.CD_CONTA_CONTABIL LIKE '1.1.1%' OR (PCONT.CD_CONTA_CONTABIL LIKE '1.1.4%' AND 1 = :CONCILIA_APLICACAO_RPPS))&#13;&#10;       ) A1&#13;&#10;       LEFT JOIN PEC_CAIXA_MOVIMENTO B1 ON B1.ID_CONTA_BANCARIA         = A1.ID_CONTA_BANCARIA&#13;&#10;                                       AND B1.NR_FICHA                  = A1.NR_FICHA&#13;&#10;                                       AND B1.CP_ANO                    = :CP_ANO&#13;&#10;                                       AND B1.ID_ST_LANCAMENTO_CAIXA    = 1&#13;&#10;                                       AND B1.DT_MOVIMENTO             &lt;= :DT_CONCILIACAO&#13;&#10; GROUP BY&#13;&#10;       A1.ID_CONTA_BANCARIA&#13;&#10;     , A1.ID_FICHA&#13;&#10;     , A1.NR_FICHA&#13;&#10;     , A1.CD_PLANO_CONTA&#13;&#10;     , A1.DS_PLANO_CONTA&#13;&#10;     , A1.CD_FONTE_RECURSO&#13;&#10;     , A1.DS_FONTE_RECURSO&#13;&#10;     , A1.CD_APLICACAO&#13;&#10;     , A1.DS_APLICACAO&#13;&#10;     , A1.ATIVO&#13;&#10;     , A1.VL_SALDO&#13;&#10; ORDER BY&#13;&#10;       A1.NR_FICHA">
