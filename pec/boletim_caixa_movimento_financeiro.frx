        <Column Name="vl_sd_anterior" DataType="System.Double"/>
        <CommandParameter Name="IMP_MOV_FIN" DataType="9" Expression="[SqlMaster.imp_mov_fin]"/>
        <CommandParameter Name="CP_ANO" DataType="9" Expression="[cpAno]"/>
