        <Column Name="conta_bancaria" DataType="System.String"/>
        <Column Name="cd_plano_conta" DataType="System.String"/>
        <Column Name="ds_plano_conta" DataType="System.String"/>
        <Column Name="ds_adicional" DataType="System.String"/>
