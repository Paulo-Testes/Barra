        , CXMOV.NR_CHEQUE&#13;&#10;             , CAST(COALESCE(CXMOV.NOMINAL,&#13;&#10;                             SPESS.NOME_RAZAO,&#13;&#10;                             SUBSTRING(CAIXA.DS_CREDOR_DEVEDOR FROM 1 FOR 250),&#13;&#10;                             ENTID.DS_ENTIDADE                                  ) AS VARCHAR(250)) AS DS_CREDOR_DEVEDOR&#13;&#10;             , CAIXA.ESTORNO&#13;&#10;             , CXMOV.VL_MOVIMENTO * -1 AS VL_MOVIMENTO&#13;&#10;          FROM SIS_SEQUENCIA SSSSS&#13;&#10;               LEFT JOIN PEC_CAIXA                      CAIXA ON 1                                  = SSSSS.ID_SEQUENCIA&#13;&#10;               LEFT JOIN PEC_TP_LANCAMENTO_CAIXA        TLANC ON TLANC.ID_TP_LANCAMENTO_CAIXA       = CAIXA.ID_TP_LANCAMENTO_CAIXA&#13;&#10;               LEFT JOIN SIS_ENTIDADE                   ENTID ON ENTID.ID_ENTIDADE                  = CAIXA.ID_ENTIDADE&#13;&#10;               LEFT JOIN PEC_CAIXA_MOVIMENTO            CXMOV ON CXMOV.ID_CAIXA                     = CAIXA.ID_CAIXA&#13;&#10;               LEFT JOIN PEC_FICHA_VIEW                 FICHA ON FICHA.ID_FICHA                     = CXMOV.ID_FICHA&#13;&#10;                                                             AND FICHA.ID_ENTIDADE                  = CAIXA.ID_ENTIDADE&#13;&#10;                                                             AND FICHA.CP_ANO                       = :CP_ANO&#13;&#10;                                                             AND FICHA.ID_TP_NATUREZA               = 4&#13;&#10;               LEFT JOIN PEC_TP_DOCUMENTO_BANCO         TPDOC ON TPDOC.ID_TP_DOCUMENTO_BANCO        = CXMOV.ID_TP_DOCUMENTO_BANCO&#13;&#10;               LEFT JOIN SIS_PESSOA                     SPESS ON SPESS.ID_PESSOA                    = CAIXA.ID_CREDOR&#13;&#10;         WHERE SSSSS.ID_SEQUENCIA = :IMP_DETALHE_BANCARIO&#13;&#10;           AND CAIXA.CP_ANO = :CP_ANO&#13;&#10;           AND ( CAIXA.ID_ENTIDADE = :ID_ENTIDADE OR 1 = :CONSOLIDADO )&#13;&#10;           AND CXMO