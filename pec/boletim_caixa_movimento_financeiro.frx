      <TableDataSource Name="Table1" Alias="SqlNumeroBoletim" DataType="System.Int32" Enabled="true" SelectCommand="SELECT CAST(COUNT(DISTINCT CXMOV.DT_MOVIMENTO) + 1 AS INTEGER) AS NR_BOLETIM&#13;&#10;FROM PEC_CAIXA CAIXA&#13;&#10;     LEFT JOIN PEC_CAIXA_MOVIMENTO CXMOV ON CXMOV.ID_CAIXA = CAIXA.ID_CAIXA&#13;&#10;WHERE CAIXA.CP_ANO = :CP_ANO&#13;&#10;  AND (CAIXA.ID_ENTIDADE = :ID_ENTIDADE OR 1 = :CONSOLIDADO)&#13;&#10;  AND CXMOV.ID_ST_LANCAMENTO_CAIXA = 1&#13;&#10;  AND CXMOV.DT_MOVIMENTO &lt; MAKE_DATE(CAST(:ANO_FIN AS INTEGER), 1, 1)&#13;&#10;                          + CAST((CAST((CAST(:MES_FIN AS INTEGER) - 1) AS varchar) || ' MONTH') AS INTERVAL)&#13;&#10;                          + CAST((CAST((CAST(:DIA_FIN AS INTEGER) - 1) AS varchar) || ' DAY') AS INTERVAL)&#13;&#10;  AND MAKE_DATE(CAST(:ANO_INI AS INTEGER), 1, 1)&#13;&#10;      + CAST((CAST((CAST(:MES_INI AS INTEGER) - 1) AS varchar) || ' MONTH') AS INTERVAL)&#13;&#10;      + CAST((CAST((CAST(:DIA_INI AS INTEGER) - 1) AS varchar) || ' DAY') AS INTERVAL) &#13;&#10;      = &#13;&#10;      MAKE_DATE(CAST(:ANO_FIN AS INTEGER), 1, 1)&#13;&#10;      + CAST((CAST((CAST(:MES_FIN AS INTEGER) - 1) AS varchar) || ' MONTH') AS INTERVAL)&#13;&#10;      + CAST((CAST((CAST(:DIA_FIN AS INTEGER) - 1) AS varchar) || ' DAY') AS INTERVAL)">
