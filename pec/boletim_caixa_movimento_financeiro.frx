      <TableDataSource Name="Table6" Alias="SqlDetalheDeposito" DataType="System.Int32" Enabled="true" SelectCommand="SELECT CAST(CASE WHEN B1.ID_SEQUENCIA = 1 AND 1 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE            AS VARCHAR(15)), 15, '0')&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 2 AND 1 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE            AS VARCHAR(15)), 15, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.NR_FICHA               AS VARCHAR(15)), 15, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.CD_TP_DOCUMENTO_BANCO  AS VARCHAR(15)), 15, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.NR_CHEQUE              AS VARCHAR(15)), 15, '0')&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 1 AND 0 = :CONSOLIDADO THEN ''&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 2 AND 0 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE            AS VARCHAR(15)), 15, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.NR_FICHA               AS VARCHAR(15)), 15, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.CD_TP_DOCUMENTO_BANCO  AS VARCHAR(15)), 15, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.NR_CHEQUE              AS VARCHAR(15)), 15, '0')&#13;&#10;&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 3 THEN 'XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX'&#13;&#10;            END&#13;&#10;         AS VARCHAR(250))       AS CHAVE&#13;&#10;&#13;&#10;      , B1.ID_SEQUENCIA AS NIVEL&#13;&#10;&#13;&#10;      , CAST(CASE WHEN B1.ID_SEQUENCIA = 1 AND 1 = :CONSOLIDADO THEN A1.DS_ENTIDADE&#13;&#10;                  ELSE 'Não Imprime'&#13;&#10;             END&#13;&#10;         AS VARCHAR(250))       AS DS_GRUPO&#13;&#10;                                                                               