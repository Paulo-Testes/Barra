    private void Report_Start(object sender, EventArgs e)
    {
      // Substituição dinâmica do filtro em cada SQL
      string filtroFonte = Report.GetVariableValue(&quot;!filtroFonte&quot;).ToString();
      
      var sqlBoletim = Report.GetDataSource(&quot;SqlBoletim&quot;) as TableDataSource;
      if (sqlBoletim != null)
      {
        sqlBoletim.SelectCommand = sqlBoletim.SelectCommand.Replace(&quot;/*!FILTRO_FONTE*/&quot;, filtroFonte);
      }
      
      var sqlResumo = Report.GetDataSource(&quot;SqlResumo&quot;) as TableDataSource;
      if (sqlResumo != null)
      {
        sqlResumo.SelectCommand = sqlResumo.SelectCommand.Replace(&quot;/*!FILTRO_FONTE*/&quot;, filtroFonte);
      }
      
      var sqlMovimentoFinanceiro = Report.GetDataSource(&quot;SqlMovimentoFinanceiro&quot;) as TableDataSource;
      if (sqlMovimentoFinanceiro != null)
      {
        sqlMovimentoFinanceiro.SelectCommand = sqlMovimentoFinanceiro.SelectCommand.Replace(&quot;/*!FILTRO_FONTE*/&quot;, filtroFonte);
      }
      
      var sqlDetalheRetirada = Report.GetDataSource(&quot;SqlDetalheRetirada&quot;) as TableDataSource;
      if (sqlDetalheRetirada != null)
      {
        sqlDetalheRetirada.SelectCommand = sqlDetalheRetirada.SelectCommand.Replace(&quot;/*!FILTRO_FONTE*/&quot;, filtroFonte);
      }
      
      var sqlDetalheDeposito = Report.GetDataSource(&quot;SqlDetalheDeposito&quot;) as TableDataSource;
      if (sqlDetalheDeposito != null)
      {
        sqlDetalheDeposito.SelectCommand = sqlDetalheDeposito.SelectCommand.Replace(&quot;/*!FILTRO_FONTE*/&quot;, filtroFonte);
      }
      
      var sqlDetalheReceita = Report.GetDataSource(&quot;SqlDetalheReceita&quot;) as TableDataSource;
      if (sqlDetalheReceita != null)
      {
        sqlDetalheReceita.SelectCommand = sqlDetalheReceita.SelectCommand.Replace(&quot;/*!FILTRO_FONTE*/&quot;, filtroFonte);
      }
      
      var sqlDetalheDespesa = Report.GetDataSource(&quot;SqlDetalheDespesa&quot;) as TableDataSource;
      if (sqlDetalheDespesa != null)
      {
        sqlDetalheDespesa.SelectCommand = sqlDetalheDespesa.SelectCommand.Replace(&quot;/*!FILTRO_FONTE*/&quot;, filtroFonte);
      }
       
      // Configura quebras de página com base na variável &lt;QuebrarPagina&gt;
      bool quebrarPagina = Convert.ToBoolean(Report.GetVariableValue(&quot;quebrarPagina&quot;));

      cabDeposito.StartNewPage = quebrarPagina;
      cabRetirada.StartNewPage = quebrarPagina;
      cabReceita.StartNewPage = quebrarPagina;
      cabDespesa.StartNewPage = quebrarPagina;

      // Exibe o título &quot;Consolidado&quot; se a variável &lt;Consolidado&gt; == 1
      TxtConsolidado.Visible    = Convert.ToInt32(Report.GetVariableValue(&quot;consolidado&quot;)) == 1; 
      filtroConsolidado.Visible = Convert.ToInt32(Report.GetVariableValue(&quot;consolidado&quot;)) == 1; 

      // Paginação personalizada
      int paginaInicial = Convert.ToInt32(Report.GetVariableValue(&quot;paginaInicial&quot;));
      if (paginaInicial == 1)
      {
        TxtPaginacao.Text = &quot;Página [Page#] de [TotalPages#]&quot;;
      }
      else
      {
        TxtPaginacao.Text = $&quot;Página [Page# + {paginaInicial - 1}]&quot;;
      }
    }
