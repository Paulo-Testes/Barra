      <TableDataSource Name="Table2" Alias="SqlMaster" DataType="System.Int32" Enabled="true" SelectCommand="SELECT ID_SEQUENCIA AS TIPO&#13;&#10;     , MAKE_DATE(CAST(:ANO_INI AS INTEGER), 1,1)&#13;&#10;               + CAST((CAST((CAST(:MES_INI AS INTEGER) -1) AS varchar) || 'MONTH') AS interval)&#13;&#10;               + CAST((CAST((CAST(:DIA_INI AS INTEGER) -1) AS varchar) || 'DAY')   AS interval) AS dt_ini&#13;&#10;     , MAKE_DATE(CAST(:ANO_FIN AS INTEGER), 1,1)&#13;&#10;               + CAST((CAST((CAST(:MES_FIN AS INTEGER) -1) AS varchar) || 'MONTH') AS interval)&#13;&#10;               + CAST((CAST((CAST(:DIA_FIN AS INTEGER) -1) AS varchar) || 'DAY')   AS interval) AS dt_fin    &#13;&#10;     , CAST(CASE WHEN ID_SEQUENCIA = 1 THEN 1 ELSE 0 END AS INTEGER) AS IMP_BOLETIM&#13;&#10;     , CAST(CASE WHEN ID_SEQUENCIA = 2 THEN 1 ELSE 0 END AS INTEGER) AS IMP_MOV_FIN&#13;&#10;     , CAST(CASE WHEN ID_SEQUENCIA = 3 THEN :IMP_DETALHE ELSE 0 END AS INTEGER) AS IMP_DETALHE&#13;&#10;     , CAST(CASE WHEN ID_SEQUENCIA = 3 THEN :IMP_DETALHE_BANCARIO ELSE 0 END AS INTEGER) AS IMP_DETALHE_BANCARIO&#13;&#10;     , CAST(CASE WHEN ID_SEQUENCIA = 3 THEN :QUEBRAR_PAGINA ELSE 0 END AS INTEGER) AS QUEBRAR_PAGINA&#13;&#10;  FROM SIS_SEQUENCIA&#13;&#10; WHERE ( ID_SEQUENCIA  = 1 AND CAST(:TIPO AS INTEGER) = 1 )&#13;&#10;    OR ( ID_SEQUENCIA  = 2 AND CAST(:TIPO AS INTEGER) = 2 )&#13;&#10;    OR ( ID_SEQUENCIA &lt;= 2 AND CAST(:TIPO AS INTEGER) = 3 AND CAST(:IMP_DETALHE AS INTEGER) = 0 AND CAST(:IMP_DETALHE_BANCARIO AS INTEGER) = 0 )&#13;&#10;    OR ( ID_SEQUENCIA &lt;= 3 AND CAST(:TIPO AS INTEGER) = 3 AND (CAST(:IMP_DETALHE AS INTEGER) = 1 OR CAST(:IMP_DETALHE_BANCARIO AS INTEGER) = 1 ) )&#13;&#10; ORDER BY ID_SEQUENCIA">
