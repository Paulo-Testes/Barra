      <TableDataSource Name="Table8" Alias="SqlDetalheReceita" DataType="System.Int32" Enabled="true" SelectCommand="SELECT CAST(CASE WHEN B1.ID_SEQUENCIA = 1 AND 1 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE     AS VARCHAR(15)), 10, '0')&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 2 AND 1 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE     AS VARCHAR(15)), 10, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.RESTITUICAO     AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.EXTRA           AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.ID_TP_NATUREZA  AS VARCHAR(2)),   2, '0')&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 3 AND 1 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE     AS VARCHAR(15)), 10, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.RESTITUICAO     AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.EXTRA           AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.ID_TP_NATUREZA  AS VARCHAR(2)),   2, '0') || A1.CD_PLANO_CONTA&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 1 AND 0 = :CONSOLIDADO THEN ''&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 2 AND 0 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE     AS VARCHAR(15)), 10, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.RESTITUICAO     AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.EXTRA           AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.ID_TP_NATUREZA  AS VARCHAR(2)),   2, '0')&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 3 AND 0 = :CONSOLIDADO THEN LPAD(CAST(A1.ID_ENTIDADE     AS VARCHAR(15)), 10, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.RESTITUICAO     AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.EXTRA           AS VARCHAR(2)),   2, '0') ||&#13;&#10;                                                                    LPAD(CAST(A1.ID_TP_NATUREZA  AS VARCHAR(2)),   2, '0') || A1.CD_PLANO_CONTA&#13;&#10;                 WHEN B1.ID_SEQUENCIA = 4 THEN 'XXXXXXXXXXXXXXXXXXXXXXXXXXXXXX'&#13;&#10;            END&#13;&#10;         AS VARCHAR(250))       AS CHAVE&#13;&#10;&#13;&#10;      , B1.ID_SEQUENCIA AS NIVEL&#13;&#10;&#13;&#10;      , CAST(CASE WHEN B1.ID_SEQUENCIA = 1 AND 0 = :CONSOLIDADO                             THEN A1.DS_ENTIDADE&#13;&#10;                  WHEN B1.ID_SEQUENCIA = 2 AND A1.ID_TP_NATUREZA = 1 AND A1.RESTITUICAO = 0 THEN 'Receitas Orçamentárias'&#13;&#10;                  WHEN B1.ID_SEQUENCIA = 2 AND A1.ID_TP_NATUREZA = 3 AND A1.EXTRA       = 0 THEN 'Transferências Recebidas'&#13;&#10;                  WHEN B1.ID_SEQUENCIA = 2 AND A1.ID_TP_NATUREZA = 3 AND A1.EXTRA       = 1 THEN 'Receitas Extra Orçamentárias'&#13;&#10;                  WHEN B1.ID_SEQUENCIA = 2 AND A1.ID_TP_NATUREZA = 1 AND A1.RESTITUICAO = 1 THEN 'Restituições de Receitas Orçamentárias'&#13;&#10;                  ELSE 'Não Imprime'&#13;&#10;             END&#13;&#10;         AS VARCHAR(250))       AS DS_GRUPO&#13;&#10;&#13;&#10;      , CASE WHEN B1.ID_SEQUENCIA = 3 THEN A1.ID_FICHA            END AS ID_FICHA&#13;&#10;      , CASE WHEN B1.ID_SEQUENCIA = 3 THEN A1.NR_FICHA            END AS NR_FICHA&#13;&#10;      , CAST(CASE WHEN B1.ID_SEQUENCIA = 3 THEN A1.CD_PLANO_CONTA END AS VARCHAR(30)) AS CD_PLANO_CONTA&#13;&#10;      , CAST(CASE WHEN B1.ID_SEQUENCIA = 3 THEN A1.DS_PLANO_CONTA END AS VARCHAR(250)) AS DS_PLANO_CONTA&#13;&#10;      , CASE WHEN B1.ID_SEQUENCIA = 3 THEN A1.ESTORNO             END AS ESTORNO&#13;&#10;      , CAST(CASE WHEN B1.ID_SEQUENCIA = 3 THEN A1.CD_TP_DEDUCAO  END AS VARCHAR(10)) AS CD_TP_DEDUCAO&#13;&#10;&#13;&#10;      , ROUND(SUM(A1.VL_MOVIMENTO), 2) AS VL_MOVIMENTO&#13;&#10;  FROM (&#13;&#10;        SELECT ENTID.ID_ENTIDADE&#13;&#10;             , ENTID.DS_ENTIDADE&#13;&#10;             , FICHA.ID_TP_NATUREZA&#13;&#10;             , FICHA.ID_FICHA&#13;&#10;             , FICHA.NR_FICHA&#13;&#10;             , FICHA.CD_PLANO_CONTA&#13;&#10;             , FICHA.DS_PLANO_CONTA&#13;&#10;             , FICHA.CD_TP_DEDUCAO&#13;&#10;             , CAIXA.ESTORNO&#13;&#10;             , CASE WHEN FICHA.ID_TP_NATUREZA = 3 AND NTDET.INFORMA_UNIDADE       = 0 THEN 1 ELSE 0 END AS EXTRA&#13;&#10;             , CASE WHEN FICHA.ID_TP_NATUREZA = 1 AND CXMOV.ID_TP_CAIXA_MOVIMENTO = 2 THEN 1 ELSE 0 END AS RESTITUICAO&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 1                              THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        -- RESTITUICOES DE RECEITAS&#13;&#10;                        WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2 AND FICHA.ID_TP_NATUREZA = 1 THEN CXMOV.VL_MOVIMENTO&#13;&#10;                   ELSE 0 END) AS VL_MOVIMENTO&#13;&#10;          FROM SIS_SEQUENCIA SSSSS&#13;&#10;               LEFT JOIN PEC_CAIXA                  CAIXA ON 1                              = SSSSS.ID_SEQUENCIA&#13;&#10;               LEFT JOIN SIS_ENTIDADE               ENTID ON ENTID.ID_ENTIDADE              = CAIXA.ID_ENTIDADE&#13;&#10;               LEFT JOIN PEC_CAIXA_MOVIMENTO        CXMOV ON CXMOV.ID_CAIXA                 = CAIXA.ID_CAIXA&#13;&#10;               LEFT JOIN PEC_FICHA_VIEW             FICHA ON FICHA.ID_FICHA                 = CXMOV.ID_FICHA&#13;&#10;                                                         AND FICHA.ID_ENTIDADE              = CAIXA.ID_ENTIDADE&#13;&#10;                                                         AND FICHA.CP_ANO                   = :CP_ANO&#13;&#10;                                                         AND FICHA.ID_TP_NATUREZA           IN (1,3)&#13;&#10;               LEFT JOIN PEC_TP_NATUREZA_DETALHE    NTDET ON NTDET.ID_TP_NATUREZA           = FICHA.ID_TP_NATUREZA&#13;&#10;                                                         AND NTDET.ID_TP_NATUREZA_DETALHE   = FICHA.ID_TP_NATUREZA_DETALHE&#13;&#10;&#13;&#10;         WHERE SSSSS.ID_SEQUENCIA = :IMP_DETALHE&#13;&#10;           AND ( CAIXA.ID_ENTIDADE = :ID_ENTIDADE OR 1 = :CONSOLIDADO )&#13;&#10;           AND CAIXA.CP_ANO = :CP_ANO&#13;&#10;           AND ( (CXMOV.ID_TP_CAIXA_MOVIMENTO = 1) OR&#13;&#10;                 (CXMOV.ID_TP_CAIXA_MOVIMENTO = 2) AND (FICHA.ID_TP_NATUREZA = 1) )&#13;&#10;           AND CXMOV.ID_ST_LANCAMENTO_CAIXA = 1&#13;&#10;           AND CXMOV.DT_MOVIMENTO &gt;= :DT_INI&#13;&#10;           AND CXMOV.DT_MOVIMENTO &lt;= :DT_FIN&#13;&#10;           /*!FILTRO_FONTE*/&#13;&#10;         GROUP BY&#13;&#10;               ENTID.ID_ENTIDADE&#13;&#10;             , ENTID.DS_ENTIDADE&#13;&#10;             , FICHA.ID_TP_NATUREZA&#13;&#10;             , FICHA.ID_FICHA&#13;&#10;             , FICHA.NR_FICHA&#13;&#10;             , FICHA.CD_PLANO_CONTA&#13;&#10;             , FICHA.DS_PLANO_CONTA&#13;&#10;             , FICHA.CD_TP_DEDUCAO&#13;&#10;             , NTDET.INFORMA_UNIDADE&#13;&#10;             , CXMOV.ID_TP_CAIXA_MOVIMENTO&#13;&#10;             , CAIXA.ESTORNO&#13;&#10;       ) A1&#13;&#10;       LEFT JOIN SIS_SEQUENCIA B1 ON B1.ID_SEQUENCIA &lt;= 4&#13;&#10; WHERE ( (B1.ID_SEQUENCIA &lt;&gt; 1 AND 0 = :CONSOLIDADO) OR (1 = :CONSOLIDADO) )&#13;&#10; GROUP BY&#13;&#10;       1,2,3,4,5,6,7,8,9&#13;&#10; ORDER BY&#13;&#10;       1, 5">
