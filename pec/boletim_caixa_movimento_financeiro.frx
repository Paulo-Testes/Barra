      <TableDataSource Name="Table3" Alias="SqlBoletim" DataType="System.Int32" Enabled="true" SelectCommand="SELECT ROUND(SUM(A1.VL_CX_RECEITA_ORC  ), 2) AS VL_CX_RECEITA_ORC&#13;&#10;     , ROUND(SUM(A1.VL_CX_TRANSF_REC   ), 2) AS VL_CX_TRANSF_REC&#13;&#10;     , ROUND(SUM(A1.VL_CX_RECEITA_EXT  ), 2) AS VL_CX_RECEITA_EXT&#13;&#10;     , ROUND(SUM(A1.VL_CX_RECEITA_RES  ), 2) AS VL_CX_RECEITA_RES&#13;&#10;     , ROUND(SUM(A1.VL_CX_RETIRADA_BAN ), 2) AS VL_CX_RETIRADA_BAN&#13;&#10;     , ROUND(SUM(A1.VL_CX_DESPESA_ORC  ), 2) AS VL_CX_DESPESA_ORC&#13;&#10;     , ROUND(SUM(A1.VL_CX_TRANSF_CON   ), 2) AS VL_CX_TRANSF_CON&#13;&#10;     , ROUND(SUM(A1.VL_CX_DESPESA_EXT  ), 2) AS VL_CX_DESPESA_EXT&#13;&#10;     , ROUND(SUM(A1.VL_CX_DESPESA_RSP  ), 2) AS VL_CX_DESPESA_RSP&#13;&#10;     , ROUND(SUM(A1.VL_CX_DEPOSITO_BAN ), 2) AS VL_CX_DEPOSITO_BAN&#13;&#10;     , ROUND(SUM(A1.VL_BC_RECEITA_ANT  ), 2) AS VL_BC_RECEITA_ANT&#13;&#10;     , ROUND(SUM(A1.VL_BC_RECEITA_PER  ), 2) AS VL_BC_RECEITA_PER&#13;&#10;     , ROUND(SUM(A1.VL_BC_DESPESA_ANT  ), 2) AS VL_BC_DESPESA_ANT&#13;&#10;     , ROUND(SUM(A1.VL_BC_DESPESA_PER  ), 2) AS VL_BC_DESPESA_PER&#13;&#10;     , ROUND(SUM(A1.VL_SD_INICIAL_TES  ), 2) AS VL_SD_INICIAL_TES&#13;&#10;     , ROUND(SUM(A1.VL_SD_ANTERIOR_TES ), 2) AS VL_SD_ANTERIOR_TES&#13;&#10;     , ROUND(SUM(A1.VL_SD_FINAL_TES    ), 2) AS VL_SD_FINAL_TES&#13;&#10;     , ROUND(SUM(A1.VL_SD_INICIAL_BAN  ), 2) AS VL_SD_INICIAL_BAN&#13;&#10;     , ROUND(SUM(A1.VL_SD_FINAL_BAN    ), 2) AS VL_SD_FINAL_BAN&#13;&#10;  FROM (&#13;&#10;        SELECT CAST(0 AS DOUBLE PRECISION) AS VL_CX_RECEITA_ORC&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_TRANSF_REC&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_RECEITA_EXT&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_RECEITA_RES&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_RETIRADA_BAN&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_DESPESA_ORC&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_TRANSF_CON&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_DESPESA_EXT&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_DESPESA_RSP&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_CX_DEPOSITO_BAN&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_BC_RECEITA_ANT&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_BC_RECEITA_PER&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_BC_DESPESA_ANT&#13;&#10;             , CAST(0 AS DOUBLE PRECISION) AS VL_BC_DESPESA_PER&#13;&#10;             , SUM(CASE WHEN PCONT.ID_TP_NATUREZA_DETALHE  = 1&#13;&#10;                        THEN FICHA.VL_SALDO&#13;&#10;                        ELSE 0&#13;&#10;                   END                   ) AS VL_SD_INICIAL_TES&#13;&#10;             , SUM(CASE WHEN PCONT.ID_TP_NATUREZA_DETALHE  = 1&#13;&#10;                        THEN FICHA.VL_SALDO&#13;&#10;                        ELSE 0&#13;&#10;                   END                   ) AS VL_SD_ANTERIOR_TES&#13;&#10;             , SUM(CASE WHEN PCONT.ID_TP_NATUREZA_DETALHE  = 1&#13;&#10;                        THEN FICHA.VL_SALDO&#13;&#10;                        ELSE 0&#13;&#10;                   END                   ) AS VL_SD_FINAL_TES&#13;&#10;             , SUM(CASE WHEN PCONT.ID_TP_NATUREZA_DETALHE &lt;&gt; 1&#13;&#10;                        THEN FICHA.VL_SALDO&#13;&#10;                        ELSE 0&#13;&#10;                   END                   ) AS VL_SD_INICIAL_BAN&#13;&#10;             , SUM(CASE WHEN PCONT.ID_TP_NATUREZA_DETALHE &lt;&gt; 1&#13;&#10;                        THEN FICHA.VL_SALDO&#13;&#10;                        ELSE 0&#13;&#10;                   END                   ) AS VL_SD_FINAL_BAN&#13;&#10;          FROM PEC_FICHA FICHA&#13;&#10;               LEFT JOIN PEC_PLANO_CONTA PCONT ON PCONT.ID_PLANO_CONTA = FICHA.ID_PLANO_CONTA&#13;&#10;         WHERE 1 = :IMP_BOLETIM&#13;&#10;           AND FICHA.CP_ANO = :CP_ANO&#13;&#10;           AND ( FICHA.ID_ENTIDADE = :ID_ENTIDADE OR 1 = :CONSOLIDADO )&#13;&#10;           AND FICHA.ID_TP_NATUREZA = 4&#13;&#10;           /*!FILTRO_FONTE*/&#13;&#10;&#13;&#10;        UNION ALL&#13;&#10;&#13;&#10;        SELECT -- QUADRO CAIXA DA RECEITA&#13;&#10;               SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 1&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_RECEITA_ORC&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 1&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 3&#13;&#10;                         AND NTDET.INFORMA_UNIDADE          = 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_TRANSF_REC&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 1&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 3&#13;&#10;                         AND NTDET.INFORMA_UNIDADE          = 0&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_RECEITA_EXT&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 1&#13;&#10;                         AND FICHA.CP_ANO                   = :CP_ANO&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_RECEITA_RES&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 3&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND CXMOV.ID_TP_NATUREZA_CONTABIL  = 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_RETIRADA_BAN&#13;&#10;&#13;&#10;               -- QUADRO CAIXA DA DESPESA&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 2&#13;&#10;                         AND FICHA.CP_ANO                   = :CP_ANO&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_DESPESA_ORC&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 3&#13;&#10;                         AND NTDET.INFORMA_UNIDADE          = 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_TRANSF_CON&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 3&#13;&#10;                         AND NTDET.INFORMA_UNIDADE          = 0&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_DESPESA_EXT&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 2&#13;&#10;                         AND FICHA.CP_ANO                   &lt; :CP_ANO&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_DESPESA_RSP&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 3&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND CXMOV.ID_TP_NATUREZA_CONTABIL  = 2&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_CX_DEPOSITO_BAN&#13;&#10;&#13;&#10;               -- BALANCETE DA RECEITA&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 1&#13;&#10;                         AND CXMOV.DT_MOVIMENTO             &lt; :DT_INI&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        -- RESTITUICOES DE RECEITAS&#13;&#10;                        WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO             &lt; :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_BC_RECEITA_ANT&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 1&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        -- RESTITUICOES DE RECEITAS&#13;&#10;                        WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA           = 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_BC_RECEITA_PER&#13;&#10;&#13;&#10;               -- BALANCETE DA DESPESA&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO             &lt; :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA          &lt;&gt; 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_BC_DESPESA_ANT&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 2&#13;&#10;                         AND CXMOV.DT_MOVIMENTO            &gt;= :DT_INI&#13;&#10;                         AND FICHA.ID_TP_NATUREZA          &lt;&gt; 1&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_BC_DESPESA_PER&#13;&#10;&#13;&#10;               -- SALDO TESOURARIA&#13;&#10;             , CAST(0 AS DOUBLE PRECISION)                                    AS VL_SD_INICIAL_TES&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 4&#13;&#10;                         AND CXMOV.DT_MOVIMENTO             &lt; :DT_INI&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_SD_ANTERIOR_TES&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 4&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                   END                                                      ) AS VL_SD_FINAL_TES&#13;&#10;&#13;&#10;               -- SALDO BANCARIO&#13;&#10;             , CAST(0 AS DOUBLE PRECISION)                                    AS VL_SD_INICIAL_BAN&#13;&#10;&#13;&#10;             , SUM(CASE WHEN CXMOV.ID_TP_CAIXA_MOVIMENTO    = 3&#13;&#10;                        THEN CXMOV.VL_MOVIMENTO *-1&#13;&#10;                        ELSE 0&#13;&#10;                       END                                                  ) AS VL_SD_FINAL_BANCO&#13;&#10;          FROM PEC_CAIXA CAIXA&#13;&#10;               LEFT JOIN PEC_CAIXA_MOVIMENTO        CXMOV ON CXMOV.ID_CAIXA                 = CAIXA.ID_CAIXA&#13;&#10;               LEFT JOIN PEC_FICHA                  FICHA ON FICHA.ID_FICHA                 = CXMOV.ID_FICHA&#13;&#10;               LEFT JOIN PEC_PLANO_CONTA            PCONT ON PCONT.ID_PLANO_CONTA           = FICHA.ID_PLANO_CONTA&#13;&#10;               LEFT JOIN PEC_TP_NATUREZA_DETALHE    NTDET ON NTDET.ID_TP_NATUREZA           = PCONT.ID_TP_NATUREZA&#13;&#10;                                                         AND NTDET.ID_TP_NATUREZA_DETALHE   = PCONT.ID_TP_NATUREZA_DETALHE&#13;&#10;         WHERE 1 = :IMP_BOLETIM&#13;&#10;           AND CAIXA.CP_ANO = :CP_ANO&#13;&#10;           AND ( FICHA.ID_ENTIDADE = :ID_ENTIDADE OR 1 = :CONSOLIDADO )&#13;&#10;           AND CXMOV.ID_ST_LANCAMENTO_CAIXA = 1&#13;&#10;           AND CXMOV.DT_MOVIMENTO &lt;= :DT_FIN                                         &#13;&#10;           /*!FILTRO_FONTE*/&#13;&#10;       ) A1">
