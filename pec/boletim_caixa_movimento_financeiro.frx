    private void MainPage_StartPage(object sender, EventArgs e)
    {
      var data = Report.GetDataSource(&quot;SqlMaster&quot;) as TableDataSource;

      if (data != null &amp;&amp; data.RowCount &gt; 0)
      {
        data.Init(); // move para o primeiro registro

        int tipo = Convert.ToInt32(data[&quot;TIPO&quot;]);

        var repBoletim = Report.FindObject(&quot;repBoletim&quot;) as SubreportObject;
        var repMovFin = Report.FindObject(&quot;repMovFin&quot;) as SubreportObject;
        var repDetalhamento = Report.FindObject(&quot;repDetalhamento&quot;) as SubreportObject;

        if (repBoletim != null)
          repBoletim.Visible = (tipo == 1);

        if (repMovFin != null)
          repMovFin.Visible = (tipo == 2);

        if (repDetalhamento != null)
          repDetalhamento.Visible = (tipo == 3);
      }
      
    } 
