    private void Text_BeforePrint(object sender, EventArgs e)
    {
      TextObject text = sender as TextObject;
      string campo;

      if (text.Name == &quot;Text43&quot;)
      {
        campo = &quot;SqlReceita.VL_arrecadado&quot;;
      }
      else
      {
        long textNumber = long.Parse(text.Name.Substring(4)); // &quot;Text31&quot; → 31
        long mesIndex = textNumber - 30;                     // Text31 → 1
        campo = $&quot;SqlReceita.vl_arrecadado_{mesIndex.ToString(&quot;00&quot;)}&quot;;
      }

      // Define a cor do texto com base no valor
      double valor = (Double)Report.GetColumnValue(campo);
      if (valor == 0)
        text.TextColor = Color.Transparent;
      else if (valor &lt; 0)
        text.TextColor = Color.Red;
      else
        text.TextColor = Color.Black;
    }
  

