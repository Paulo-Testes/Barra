    private void bndDespesa_BeforePrint(object sender, EventArgs e)
    {
      Text63.Visible = ((Int64)Report.GetParameterValue(&quot;Consolidado&quot;)) == 1;
      Text66.Visible = ((Int64)Report.GetParameterValue(&quot;receitaPorAplicacao&quot;)) == 1;
      
      BandBase band = sender as BandBase;

      // Obtem o valor de STYLE da linha atual
      long style = Convert.ToInt32(Report.GetColumnValue(&quot;SqlReceita.STYLE&quot;));

      if (style == 1)
      {
        for (long i = 30; i &lt;= 44; i++)
        {
          TextObject Text = band.FindObject(&quot;Text&quot; + i) as TextObject;
          if (Text != null)
          {
            Text.Font = new Font(Text.Font, FontStyle.Bold | FontStyle.Italic);
          }
        }
      }
      else
      {
        // Garante que outras linhas não fiquem com estilo incorreto herdado
        for (long i = 30; i &lt;= 44; i++)
        {
          TextObject Text = band.FindObject(&quot;Text&quot; + i) as TextObject;
          if (Text != null)
          {
            Text.Font = new Font(Text.Font, FontStyle.Regular);
          }
        }
      }
           
    }
    
