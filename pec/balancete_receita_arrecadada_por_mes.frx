    private void MainPage_StartPage(object sender, EventArgs e)
    {
      Report report = Report;

      string CFiltroEntidade      = &quot;AND PFICH.ID_ENTIDADE IN ({0})&quot;;
     
      // Recebe o valor do parâmetro como string (ex: &quot;1,2,3&quot;)                            ((String)Report.GetParameterValue(&quot;!filtroEntidade&quot;))
      string filtroEntidade = ((string)Report.GetParameterValue(&quot;!FiltroEntidade&quot;)).ToString();

      TableDataSource sqlReceita = report.GetDataSource(&quot;SqlReceita&quot;) as TableDataSource;

      sqlReceita.SelectCommand = sqlReceita.SelectCommand.Replace(&quot;AND PFICH.ID_ENTIDADE IN ({0})&quot;, 
        string.Format(CFiltroEntidade, filtroEntidade));
    } 
    
