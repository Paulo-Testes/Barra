      <TableDataSource Name="Table" Alias="SqlReceita" DataType="System.Int32" Enabled="true" SelectCommand="SELECT CASE WHEN B.ID_SEQUENCIA &lt; 3 THEN UDF_DIGITS(A.CD_PLANO_CONTA)&#13;&#10;            WHEN B.ID_SEQUENCIA = 3 THEN UDF_DIGITS(A.CD_PLANO_CONTA) || '0.0'&#13;&#10;            WHEN B.ID_SEQUENCIA = 4 THEN UDF_DIGITS(A.CD_PLANO_CONTA) || COALESCE(A.CD_TP_DEDUCAO, '0.0')&#13;&#10;       END  AS CHAVE&#13;&#10;&#13;&#10;     , B.ID_SEQUENCIA AS NIVEL&#13;&#10;&#13;&#10;     , CASE WHEN B.ID_SEQUENCIA = 1 THEN A.CD_PLANO_CONTA_REL || ' - ' || A.DS_PLANO_CONTA&#13;&#10;            WHEN B.ID_SEQUENCIA = 2 THEN '     ' || A.CD_PLANO_CONTA_REL || ' - ' || A.DS_PLANO_CONTA&#13;&#10;            WHEN B.ID_SEQUENCIA = 3 THEN '          0.0 - Receita Bruta'&#13;&#10;            WHEN B.ID_SEQUENCIA = 4 THEN '          ' || A.CD_TP_DEDUCAO || ' - ' || A.DS_TP_DEDUCAO_RELATORIO&#13;&#10;       END  AS DESCRICAO&#13;&#10;&#13;&#10;     , ROUND(SUM(A.VL_ARRECADADO), 2) AS VL_ARRECADADO&#13;&#10;  FROM (SELECT C1.CD_TP_DEDUCAO&#13;&#10;             , CAST(C1.DS_TP_DEDUCAO_RELATORIO AS VARCHAR(100) )    AS DS_TP_DEDUCAO_RELATORIO    &#13;&#10;             , B1.CD_PLANO_CONTA&#13;&#10;             , B1.CD_PLANO_CONTA_REL&#13;&#10;             , B1.DS_PLANO_CONTA&#13;&#10;             , B1.NIVEL                            &#13;&#10;             , MAX(A1.TEM_DEDUTORA)                                 AS TEM_DEDUTORA&#13;&#10;             , SUM(A1.VL_ARRECADADO)                                AS VL_ARRECADADO&#13;&#10;          FROM (&#13;&#10;                SELECT B2.ID_ESTADO&#13;&#10;                     , B2.CP_ANO&#13;&#10;                     , B2.ID_PLANO_CONTA_CP&#13;&#10;                     , B2.ID_PLANO_CONTA_PAI&#13;&#10;                     , B2.NIVEL&#13;&#10;                     , A2.ID_TP_DEDUCAO   &#13;&#10;                     , A2.TEM_DEDUTORA&#13;&#10;                     , SUM(A2.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                  FROM ( &#13;&#10;                        SELECT B3.ID_ESTADO&#13;&#10;                             , B3.CP_ANO&#13;&#10;                             , B3.ID_PLANO_CONTA_CP&#13;&#10;                             , B3.ID_PLANO_CONTA_PAI&#13;&#10;                             , B3.NIVEL&#13;&#10;                             , A3.ID_TP_DEDUCAO    &#13;&#10;                             , A3.TEM_DEDUTORA&#13;&#10;                             , SUM(A3.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                          FROM ( &#13;&#10;                                SELECT B4.ID_ESTADO&#13;&#10;                                     , B4.CP_ANO&#13;&#10;                                     , B4.ID_PLANO_CONTA_CP&#13;&#10;                                     , B4.ID_PLANO_CONTA_PAI&#13;&#10;                                     , B4.NIVEL&#13;&#10;                                     , A4.ID_TP_DEDUCAO    &#13;&#10;                                     , A4.TEM_DEDUTORA&#13;&#10;                                     , SUM(A4.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                  FROM ( &#13;&#10;                                        SELECT B5.ID_ESTADO&#13;&#10;                                             , B5.CP_ANO&#13;&#10;                                             , B5.ID_PLANO_CONTA_CP&#13;&#10;                                             , B5.ID_PLANO_CONTA_PAI&#13;&#10;                                             , B5.NIVEL&#13;&#10;                                             , A5.ID_TP_DEDUCAO       &#13;&#10;                                             , A5.TEM_DEDUTORA&#13;&#10;                                             , SUM(A5.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                          FROM ( &#13;&#10;                                                SELECT B6.ID_ESTADO&#13;&#10;                                                     , B6.CP_ANO&#13;&#10;                                                     , B6.ID_PLANO_CONTA_CP&#13;&#10;                                                     , B6.ID_PLANO_CONTA_PAI&#13;&#10;                                                     , B6.NIVEL&#13;&#10;                                                     , A6.ID_TP_DEDUCAO      &#13;&#10;                                                     , A6.TEM_DEDUTORA&#13;&#10;                                                     , SUM(A6.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                  FROM ( &#13;&#10;                                                        SELECT B7.ID_ESTADO&#13;&#10;                                                             , B7.CP_ANO&#13;&#10;                                                             , B7.ID_PLANO_CONTA_CP&#13;&#10;                                                             , B7.ID_PLANO_CONTA_PAI&#13;&#10;                                                             , B7.NIVEL&#13;&#10;                                                             , A7.ID_TP_DEDUCAO  &#13;&#10;                                                             , A7.TEM_DEDUTORA&#13;&#10;                                                             , SUM(A7.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                          FROM ( &#13;&#10;                                                                SELECT B8.ID_ESTADO&#13;&#10;                                                                     , B8.CP_ANO&#13;&#10;                                                                     , B8.ID_PLANO_CONTA_CP&#13;&#10;                                                                     , B8.ID_PLANO_CONTA_PAI&#13;&#10;                                                                     , B8.NIVEL&#13;&#10;                                                                     , A8.ID_TP_DEDUCAO  &#13;&#10;                                                                     , A8.TEM_DEDUTORA&#13;&#10;                                                                     , SUM(A8.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                                  FROM ( &#13;&#10;                                                                        SELECT B9.ID_ESTADO &#13;&#10;                                                                             , B9.CP_ANO &#13;&#10;                                                                             , B9.ID_PLANO_CONTA_CP &#13;&#10;                                                                             , B9.ID_PLANO_CONTA_PAI &#13;&#10;                                                                             , B9.NIVEL &#13;&#10;                                                                             , A9.ID_TP_DEDUCAO   &#13;&#10;                                                                             , A9.TEM_DEDUTORA&#13;&#10;                                                                             , SUM(A9.VL_ARRECADADO ) AS VL_ARRECADADO&#13;&#10;                                                                          FROM ( &#13;&#10;                                                                                SELECT A10.ID_ESTADO &#13;&#10;                                                                                     , A10.CP_ANO &#13;&#10;                                                                                     , A10.ID_PLANO_CONTA_CP &#13;&#10;                                                                                     , A10.ID_PLANO_CONTA_PAI &#13;&#10;                                                                                     , A10.NIVEL &#13;&#10;                                                                                     , A10.ID_TP_DEDUCAO&#13;&#10;                                                                                     , A10.TEM_DEDUTORA&#13;&#10;                                                                                     , A10.VL_ARRECADADO&#13;&#10;                                                                                  FROM (&#13;&#10;                                                                                        SELECT PPLCO.ID_ESTADO &#13;&#10;                                                                                             , PPLCO.CP_ANO &#13;&#10;                                                                                             , PPLCO.ID_PLANO_CONTA_CP &#13;&#10;                                                                                             , PPLCO.ID_PLANO_CONTA_PAI &#13;&#10;                                                                                             , PPLCO.NIVEL&#13;&#10;                                                                                             , PFICH.ID_TP_DEDUCAO&#13;&#10;                                                                                             , COALESCE(PFDED.TEM_DEDUTORA, 0)  AS TEM_DEDUTORA&#13;&#10;                                                                                             , COALESCE(PFMOV.VL_ARRECADADO, 0) AS VL_ARRECADADO&#13;&#10;                                                                                          FROM PEC_FICHA PFICH&#13;&#10;                                                                                               LEFT JOIN PEC_PLANO_CONTA PPLCO  ON PPLCO.ID_PLANO_CONTA = PFICH.ID_PLANO_CONTA&#13;&#10;&#13;&#10;                                                                                               LEFT JOIN LATERAL&#13;&#10;                                                                                                         (&#13;&#10;                                                                                                           SELECT CASE WHEN COUNT(1) &gt; 0 THEN 1 ELSE 0 END AS TEM_DEDUTORA&#13;&#10;                                                                                              