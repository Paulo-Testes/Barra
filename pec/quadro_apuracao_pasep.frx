UP BY &#13;&#10;                                                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                                            ) A8 &#13;&#10;                                                                            LEFT  JOIN PEC_PLANO_CONTA B8 ON B8.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                                         AND B8.CP_ANO    = :CP_ANO &#13;&#10;                                                                                                         AND ( B8.ID_PLANO_CONTA_CP = A8.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                                             ( B8.ID_PLANO_CONTA_CP = A8.ID_PLANO_CONTA_PAI AND A8.NIVEL = 9 )) &#13;&#10;                                                                 GROUP BY &#13;&#10;                                                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                                    ) A7 &#13;&#10;                                                                    LEFT  JOIN PEC_PLANO_CONTA B7 ON B7.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                                 AND B7.CP_ANO    = :CP_ANO &#13;&#10;                                                                                                 AND ( B7.ID_PLANO_CONTA_CP = A7.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                                     ( B7.ID_PLANO_CONTA_CP = A7.ID_PLANO_CONTA_PAI AND A7.NIVEL = 8 )) &#13;&#10;                                                         GROUP BY &#13;&#10;                                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                            ) A6 &#13;&#10;                                                            LEFT  JOIN PEC_PLANO_CONTA B6 ON B6.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                         AND B6.CP_ANO    = :CP_ANO &#13;&#10;                                                                                         AND ( B6.ID_PLANO_CONTA_CP = A6.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                             ( B6.ID_PLANO_CONTA_CP = A6.ID_PLANO_CONTA_PAI AND A6.NIVEL = 7 )) &#13;&#10;                                                 GROUP BY &#13;&#10;                                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                                    ) A5 &#13;&#10;                                                    LEFT  JOIN PEC_PLANO_CONTA B5 ON B5.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                                 AND B5.CP_ANO    = :CP_ANO &#13;&#10;                                                                                 AND ( B5.ID_PLANO_CONTA_CP = A5.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                                     ( B5.ID_PLANO_CONTA_CP = A5.ID_PLANO_CONTA_PAI AND A5.NIVEL = 6 )) &#13;&#10;                                         GROUP BY &#13;&#10;                                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                                            ) A4 &#13;&#10;                                            LEFT  JOIN PEC_PLANO_CONTA B4 ON B4.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                         AND B4.CP_ANO    = :CP_ANO &#13;&#10;                                                                         AND ( B4.ID_PLANO_CONTA_CP = A4.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                             ( B4.ID_PLANO_CONTA_CP = A4.ID_PLANO_CONTA_PAI AND A4.NIVEL = 5 )) &#13;&#10;                                 GROUP BY &#13;&#10;                                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                                    ) A3 &#13;&#10;                                    LEFT  JOIN PEC_PLANO_CONTA B3 ON B3.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                                 AND B3.CP_ANO    = :CP_ANO &#13;&#10;                                                                 AND ( B3.ID_PLANO_CONTA_CP = A3.ID_PLANO_CONTA_CP OR &#13;&#10;                                                                     ( B3.ID_PLANO_CONTA_CP = A3.ID_PLANO_CONTA_PAI AND A3.NIVEL = 4 )) &#13;&#10;                         GROUP BY &#13;&#10;                               1, 2, 3, 4, 5, 6, 7&#13;&#10;                            ) A2 &#13;&#10;                            LEFT  JOIN PEC_PLANO_CONTA B2 ON B2.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                         AND B2.CP_ANO    = :CP_ANO &#13;&#10;                                                         AND ( B2.ID_PLANO_CONTA_CP = A2.ID_PLANO_CONTA_CP OR &#13;&#10;                                                             ( B2.ID_PLANO_CONTA_CP = A2.ID_PLANO_CONTA_PAI AND A2.NIVEL = 3 )) &#13;&#10;                 GROUP BY &#13;&#10;                       1, 2, 3, 4, 5, 6, 7&#13;&#10;                    ) A1 &#13;&#10;                    LEFT  JOIN PEC_PLANO_CONTA B1 ON B1.ID_ESTADO = :ID_ESTADO &#13;&#10;                                                 AND B1.CP_ANO    = :CP_ANO &#13;&#10;                                                 AND ( B1.ID_PLANO_CONTA_CP = A1.ID_PLANO_CONTA_CP OR &#13;&#10;                                                     ( B1.ID_PLANO_CONTA_CP = A1.ID_PLANO_CONTA_PAI AND A1.NIVEL = 2 )) &#13;&#10;                    LEFT  JOIN PEC_TP_DEDUCAO  C1 ON (C1.ID_TP_DEDUCAO  = A1.ID_TP_DEDUCAO)&#13;&#10;         WHERE B1.NIVEL IN (1,2)&#13;&#10;         GROUP BY &#13;&#10;               1,