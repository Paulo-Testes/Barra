                                         AS VL_INICIAL&#13;&#10;             , CAST(0 AS DOUBLE PRECISION)                                                                                     AS VL_ALTERACAO&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 01    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_01&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 02    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_02&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 03    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_03&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 04    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_04&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 05    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_05&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 06    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_06&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 07    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_07&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 08    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_08&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 09    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_09&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 10    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_10&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 11    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_11&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA AND PFIMV.CP_MES = 12    THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO_12&#13;&#10;             , SUM(CASE WHEN 1 = :FASE_DESPESA                          THEN PFIMV.VL_DOCUMENTO      ELSE 0 END) AS VL_EMPENHADO&#13;&#10;          FROM SIS_SEQUENCIA SEQUE&#13;&#10;               LEFT JOIN PEC_FICHA                        FICHA ON 1 = SEQUE.ID_SEQUENCIA&#13;&#10;               LEFT JOIN PEC_PLANO_CONTA                  PCONT ON PCONT.ID_PLANO_CONTA               = FICHA.ID_PLANO_CONTA&#13;&#10;               LEFT JOIN PEC_FICHA_MOVIMENTO              PFIMV ON PFIMV.ID_FICHA                     = FICHA.ID_FICHA&#13;&#10;                                                               AND PFIMV.CP_ANO                       = :CP_ANO&#13;&#10;                                                               AND PFIMV.ID_TP_FICHA_MOVIMENTO        = 4&#13;&#10;                                                               AND PFIMV.LANCADO                      = 1&#13;&#10;               LEFT JOIN PEC_EMPENHO                      EMPEN ON EMPEN.ID_EMPENHO                   = PFIMV.ID_EMPENHO&#13;&#10;               LEFT JOIN PEC_PLANO_CONTA                  SUBEL ON SUBEL.ID_PLANO_CONTA               = EMPEN.ID_PLANO_CONTA&#13;&#10;               LEFT JOIN PEC_FICHA_POR_ID(FICHA.ID_FICHA) PFICH ON 1 = 1               &#13;&#10;         WHERE SEQUE.ID_SEQUENCIA   = :DETALHAR_SUBELEMENTO&#13;&#10;           AND FICHA.CP_ANO         = :CP_ANO&#13;&#10;           AND FICHA.ID_TP_NATUREZA = 2&#13;&#10;           /*!FILTRO_ENTIDADE_FICHA*/&#13;&#10;           AND FICHA.ID_PLANO_CONTA &lt;&gt; EMPEN.ID_PLANO_CONTA&#13;&#10;         GROUP BY&#13;&#10;               PFICH.CD_ORGAO&#13;&#10;             , PFICH.ORGAO&#13;&#10;             , PFICH.CD_UO&#13;&#10;             , PFICH.UO&#13;&#10;             , PFICH.CD_UE&#13;&#10;             , PFICH.UE&#13;&#10;             , PFICH.CD_FUNCAO&#13;&#10;             , PFICH.FUNCAO&#13;&#10;             , PFICH.CD_SUBFUNCAO&#13;&#10;             , PFICH.SUBFUNCAO&#13;&#10;             , PFICH.CD_PROGRAMA&#13;&#10;             , PFICH.PROGRAMA&#13;&#10;             , PFICH.CD_ACAO&#13;&#10;             , PFICH.AC