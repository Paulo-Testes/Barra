    private void MainPage_StartPage(object sender, EventArgs e)
    {
      Report report = Report;

      string CEntidadeFicha      = &quot;AND PFICH.ID_ENTIDADE IN ({0})&quot;;
      string CEntidadeLiquidacao = &quot;AND LIQUD.ID_ENTIDADE IN ({0})&quot;;
      string CEntidadePagamento  = &quot;AND PCAIX.ID_ENTIDADE IN ({0})&quot;;

      // Recebe o valor do parâmetro como string (ex: &quot;1,2,3&quot;)
      string filtroEntidade = Report.GetParameterValue(&quot;!FiltroEntidade&quot;).ToString();

      TableDataSource sqlDespesa = report.GetDataSource(&quot;SqlDespesa&quot;) as TableDataSource;

      sqlDespesa.SelectCommand = sqlDespesa.SelectCommand.Replace(&quot;AND PFICH.ID_ENTIDADE IN ({0})&quot;, 
        string.Format(CEntidadeFicha, filtroEntidade));

      sqlDespesa.SelectCommand = sqlDespesa.SelectCommand.Replace(&quot;AND LIQUD.ID_ENTIDADE IN ({0})&quot;, 
        string.Format(CEntidadeLiquidacao, filtroEntidade));

      sqlDespesa.SelectCommand = sqlDespesa.SelectCommand.Replace(&quot;AND PCAIX.ID_ENTIDADE IN ({0})&quot;, 
        string.Format(CEntidadePagamento, filtroEntidade));
    }  
    
