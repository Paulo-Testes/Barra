    private void Text30_BeforePrint(object sender, EventArgs e)
    {
      TextObject text = sender as TextObject;

      if (((Int32)Report.GetColumnValue(&quot;SqlDespesa.nivel&quot;)) == 4)
      {
        //text.Padding = &quot;10;0;2;0&quot;;
        Text30.Text = &quot;      &quot; + ((string)Report.GetColumnValue(&quot;SqlDespesa.descricao&quot;));
      } 
      else if (((Int32)Report.GetColumnValue(&quot;SqlDespesa.nivel&quot;)) == 5)
      {
        Text30.Text = &quot;           &quot; + ((string)Report.GetColumnValue(&quot;SqlDespesa.descricao&quot;));
      }
      else if (((Int32)Report.GetColumnValue(&quot;SqlDespesa.nivel&quot;)) == 6)
        {
         Text30.Text = &quot;               &quot; + ((string)Report.GetColumnValue(&quot;SqlDespesa.descricao&quot;));
        }
      else if (((Int32)Report.GetColumnValue(&quot;SqlDespesa.nivel&quot;)) == 8)
      {
        Text30.Text = &quot;                  &quot; + ((string)Report.GetColumnValue(&quot;SqlDespesa.descricao&quot;));
      }
    }
    
