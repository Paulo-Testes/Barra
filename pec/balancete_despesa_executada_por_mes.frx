    private void Text15_BeforePrint(object sender, EventArgs e)
    {
      if ((long)Report.GetParameterValue(&quot;FaseDespesa&quot;) == 1)
        Text15.Text = &quot;DESPESA EMPENHADA&quot;;
      else if ((long)Report.GetParameterValue(&quot;FaseDespesa&quot;) == 2)
        Text15.Text = &quot;DESPESA LIQUIDADA&quot;;
      else if ((long)Report.GetParameterValue(&quot;FaseDespesa&quot;) == 3)
        Text15.Text = &quot;DESPESA PAGA&quot;;  
    }
         
