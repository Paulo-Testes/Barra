      <TableDataSource Name="Table1" Alias="SqlDespesa" DataType="System.Int32" Enabled="true" SelectCommand="SELECT CAST(CASE WHEN SSEQU.ID_SEQUENCIA =  1 THEN PEC_MV_FC.CD_ORGAO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  2 THEN PEC_MV_FC.CD_UO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  3 THEN PEC_MV_FC.CD_UE&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  4 THEN PEC_MV_FC.CD_UE || PEC_MV_FC.CD_FUNCAO || PEC_MV_FC.CD_SUBFUNCAO || PEC_MV_FC.CD_PROGRAMA || PEC_MV_FC.CD_ACAO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  5 THEN PEC_MV_FC.CD_UE || PEC_MV_FC.CD_FUNCAO || PEC_MV_FC.CD_SUBFUNCAO || PEC_MV_FC.CD_PROGRAMA || PEC_MV_FC.CD_ACAO || PEC_MV_FC.CD_FONTE_RECURSO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  6 THEN PEC_MV_FC.CD_UE || PEC_MV_FC.CD_FUNCAO || PEC_MV_FC.CD_SUBFUNCAO || PEC_MV_FC.CD_PROGRAMA || PEC_MV_FC.CD_ACAO || PEC_MV_FC.CD_FONTE_RECURSO || COALESCE(PEC_MV_FC.CD_APLICACAO,'000.0000')&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  7 THEN PEC_MV_FC.CD_UE || PEC_MV_FC.CD_FUNCAO || PEC_MV_FC.CD_SUBFUNCAO || PEC_MV_FC.CD_PROGRAMA || PEC_MV_FC.CD_ACAO || PEC_MV_FC.CD_FONTE_RECURSO || COALESCE(PEC_MV_FC.CD_APLICACAO,'000.0000') || PEC_MV_FC.CD_PLANO_CONTA&#13;&#10;            END AS VARCHAR(250) ) AS CHAVE&#13;&#10;     , CAST(CASE WHEN SSEQU.ID_SEQUENCIA =  1 THEN PEC_MV_FC.ORGAO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  2 THEN PEC_MV_FC.UO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  3 THEN PEC_MV_FC.UE&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  4 THEN PEC_MV_FC.PROGRAMA_ACAO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  5 THEN PEC_MV_FC.FONTE_RECURSO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  6 THEN PEC_MV_FC.APLICACAO&#13;&#10;                 WHEN SSEQU.ID_SEQUENCIA =  7 THEN PEC_MV_FC.PLANO_CONTA&#13;&#10;            END AS VARCHAR(250) ) AS DESCRICAO&#13;&#10;     , CAST(0 AS INTEGER)                                                             AS NIVEL_CONTA&#1