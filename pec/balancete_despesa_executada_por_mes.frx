HEN PLIMV.CP_MES = 11 THEN PLIMV.VL_MOVIMENTO ELSE 0 END) AS VL_LIQUIDADO_11&#13;&#10;                         , SUM(CASE WHEN PLIMV.CP_MES = 12 THEN PLIMV.VL_MOVIMENTO ELSE 0 END) AS VL_LIQUIDADO_12&#13;&#10;                         , SUM(COALESCE(PLIMV.VL_MOVIMENTO, 0)                               ) AS VL_LIQUIDADO&#13;&#10;                       FROM PEC_LIQUIDACAO LIQUD&#13;&#10;                            LEFT JOIN PEC_LIQUIDACAO_MOVIMENTO  PLIMV ON PLIMV.ID_LIQUIDACAO                = LIQUD.ID_LIQUIDACAO&#13;&#10;                                                                     AND PLIMV.CP_ANO                       = :CP_ANO&#13;&#10;                                                                     AND PLIMV.ID_TP_LIQUIDACAO_MOVIMENTO  &gt;= 4&#13;&#10;                                                                     AND PLIMV.ID_TP_LIQUIDACAO_MOVIMENTO  &lt;= 6&#13;&#10;                     WHERE 1                     = :DETALHAR_SUBELEMENTO&#13;&#10;                       AND 2                     = :FASE_DESPESA&#13;&#10;                       AND LIQUD.CP_ANO         &gt;= :CP_ANO&#13;&#10;                       AND LIQUD.ANO_LIQUIDACAO  = :CP_ANO&#13;&#10;                       /*!FILTRO_ENTIDADE_LIQUIDACAO*/&#13;&#10;                     GROUP BY&#13;&#10;                           LIQUD.ID_EMPENHO ) SUB_MV_LQ ON SUB_MV_LQ.ID_EMPENHO = SUB_MV_FC.ID_EMPENHO&#13;&#10;&#13;&#10;       LEFT JOIN (  SELECT PCXMV.ID_EMPENHO&#13;&#10;                         , SUM(CASE WHEN EXTRACT(MONTH FROM PCXMV.DT_MOVIMENTO) = 01 THEN PCXMV.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_PAGO_01&#13;&#10;                         , SUM(CASE WHEN EXTRACT(MONTH FROM PCXMV.DT_MOVIMENTO) = 02 THEN PCXMV.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_PAGO_02&#13;&#10;                         , SUM(CASE WHEN EXTRACT(MONTH FROM PCXMV.DT_MOVIMENTO) = 03 THEN PCXMV.VL_MOVIMENTO *-1 ELSE 0 END) AS VL_PAGO_03&#13;&#10;                         , SUM(CASE WHEN EXTRACT(MONTH FROM PCXMV.DT_MOVIMENTO) = 