<TfrxReport Version="6.4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1535" PreviewOptions.Zoom="1" PrintOptions.Printer="Padrão" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="45288,390378044" ReportOptions.Description.Text="" ReportOptions.LastChange="45384,3942922569" ScriptLanguage="PascalScript" ScriptText.Text="var&#13;&#10;  SomaTotal : real;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SomaTotal := SomaTotal+ &#60;SqlRepasse.&#34;vl_total&#34;&#62;   ;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;end." StoreInDFM="False">
