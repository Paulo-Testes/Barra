  <TfrxDataPage Name="Data" Height="1000" Left="0" Top="0" Width="1000">
    <TfrxADOQuery Name="SqlCliente" UserName="SqlCliente" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT TRIM(A.NOME)           AS NOME                       &#13;&#10;     , TRIM(A.NOME_REDUZIDO)  AS NOME_REDUZIDO       &#13;&#10;     , TRIM(A.ENDERECO)       AS ENDERECO       &#13;&#10;     , TRIM(A.BAIRRO)         AS BAIRRO       &#13;&#10;     , A.CODIGO_MUNICIPIO  &#13;&#10;     , TRIM(B.NOME_MUNICIPIO) AS NOME_MUNICIPIO&#13;&#10;     , TRIM(B.UF)             AS UF       &#13;&#10;     , TRIM(A.CEP)            AS CEP       &#13;&#10;     , TRIM(A.TELEFONE)       AS TELEFONE       &#13;&#10;     , TRIM(A.FAX)            AS FAX       &#13;&#10;     , TRIM(A.EMAIL)          AS EMAIL       &#13;&#10;     , TRIM(A.SITE)           AS SITE       &#13;&#10;     , TRIM(A.CGC)            AS CNPJ                                      &#13;&#10;     , TRIM(A.SLOGAN)         AS SLOGAN       &#13;&#10;     , A.LOGOTIPO&#13;&#10;  FROM AAC_CLIENTES A&#13;&#10;       LEFT JOIN AAC_MUNICIPIOS B ON B.CODIGO_MUNICIPIO = A.CODIGO_MUNICIPIO&#13;&#10;  ROWS 1  " CommandTimeout="30" LockType="ltReadOnly" PropData="05704C65667402440470546F7002240A506172616D65746572730100"/>
    <TfrxADOQuery Name="SqlRepasse" UserName="SqlRepasse" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT&#13;&#10;    COALESCE(gaa.ds_area_atuacao, 'Não informada') AS ds_area_atuacao,&#13;&#10;    sp.id_pessoa,&#13;&#10;    sp.nome_razao,&#13;&#10;    sp.documento_formatado,&#13;&#10;    SUM(gce.vl_execucao + COALESCE(gce.vl_estornado, 0)) AS vl_total,&#13;&#10;   SUM(SUM(gce.vl_execucao + COALESCE(gce.vl_estornado, 0))) OVER () AS totalRepasse&#13;&#10;FROM&#13;&#10;    gcc_contrato gc&#13;&#10;    LEFT OUTER JOIN gcc_area_atuacao gaa ON gaa.id_area_atuacao = gc.id_area_atuacao&#13;&#10;    LEFT OUTER JOIN gcc_contrato_execucao gce ON gce.id_contrato = gc.id_contrato&#13;&#10;    LEFT OUTER JOIN sis_pessoa sp ON sp.id_pessoa = gc.id_pessoa&#13;&#10;WHERE&#13;&#10;    gc.id_tp_contrato = 2&#13;&#10;    AND gc.id_tp_categoria = 2&#13;&#10;    AND gce.id_tp_fase_execucao = 3&#13;&#10;    AND gce.estorno = 0&#13;&#10;    AND gc.id_entidade = :id_entidade&#13;&#10;    AND gce.dt_execucao BETWEEN :dt_inicio AND :dt_final&#13;&#10;    AND (&#13;&#10;        (gc.id_area_atuacao = COALESCE(:area_atuacao, 0) OR COALESCE(:area_atuacao, 0) = 0)&#13;&#10;        OR (COALESCE(:area_atuacao, 0) = 0 AND COALESCE(:beneficiario, '0') = '0')&#13;&#10;    )&#13;&#10;    AND (&#13;&#10;        UNACCENT(sp.nome_razao) LIKE UNACCENT('%' || COALESCE(:beneficiario, '0') || '%')&#13;&#10;        OR (COALESCE(:area_atuacao, 0) = 0 AND COALESCE(:beneficiario, '0') = '0')&#13;&#10;    )&#13;&#10;GROUP BY&#13;&#10;    gaa.ds_area_atuacao,&#13;&#10;    sp.id_pessoa,&#13;&#10;    sp.nome_razao,&#13;&#10;    sp.documento_formatado&#13;&#10;ORDER BY&#13;&#10;    sp.nome_razao;      " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
