      <TableDataSource Name="Table" Alias="SqlInstrumento" DataType="System.Int32" Enabled="true" SelectCommand="SELECT&#13;&#10;    gec.id_edital_chamamento,&#13;&#10;    gec.nr_edital_chamamento,&#13;&#10;    gec.ano_edital_chamamento,&#13;&#10;    gec.dt_limite,&#13;&#10;    gec.dt_publicacao,&#13;&#10;    gec.objeto,&#13;&#10;    gaa.ds_area_atuacao,&#13;&#10;    z.ds_anexo,&#13;&#10;    COUNT(gea.id_edital_chamamento) as qtd_anexos&#13;&#10;FROM&#13;&#10;    gcc_edital_chamamento gec&#13;&#10;    LEFT OUTER JOIN gcc_area_atuacao gaa ON gaa.id_area_atuacao = gec.id_area_atuacao&#13;&#10;    LEFT OUTER JOIN gcc_edital_anexo gea ON gea.id_edital_chamamento = gec.id_edital_chamamento AND gea.publica = 1&#13;&#10;    LEFT JOIN LATERAL(&#13;&#10;    	select &#13;&#10;             string_agg(sa.ds_anexo, ' | ')     as ds_anexo&#13;&#10;        from gcc_edital_anexo  gea2 &#13;&#10;        left join gcc_anexo    ga on ga.id_anexo = gea2.id_anexo&#13;&#10;        left join gcc_tp_anexo gta on gta.id_tp_anexo = ga.id_tp_anexo&#13;&#10;        left join sis_anexo    sa on sa.id_anexo = gea2.id_anexo&#13;&#10;        where &#13;&#10;            gea2.id_edital_chamamento = gea.id_edital_chamamento and &#13;&#10;            gea2.publica = 1&#13;&#10;    ) z on true&#13;&#10;WHERE&#13;&#10;    gec.id_edital_chamamento = :id_edital_chamamento     &#13;&#10;    AND gec.dt_publicacao &lt;= CURRENT_DATE      &#13;&#10;GROUP BY&#13;&#10;    gec.id_edital_chamamento,&#13;&#10;    gec.nr_edital_chamamento,&#13;&#10;    gec.ano_edital_chamamento,&#13;&#10;    gec.dt_limite,&#13;&#10;    gec.dt_publicacao,&#13;&#10;    gec.objeto,&#13;&#10;    gaa.ds_area_atuacao,&#13;&#10;    z.ds_anexo&#13;&#10;ORDER BY&#13;&#10;    gec.nr_edital_chamamento,&#13;&#10;    gec.ano_edital_chamamento;">
