      <TableDataSource Name="Table5" Alias="SqlCRYPT" DataType="System.Int32" Enabled="true" SelectCommand="SELECT CODIGO, REFCERTIDAO &#13;&#10;FROM ADE_CRYPT(&#13;&#10;    p_referencial := :referencial,&#13;&#10;    p_tipodocumento := (&#13;&#10;        SELECT CASE &#13;&#10;               	 WHEN wcd.tipocert = 0  THEN 'CN' &#13;&#10;              	 WHEN wcd.tipocert = 1  THEN 'CP' &#13;&#10;                 WHEN wcd.tipocert = 2  THEN 'CPN' &#13;&#10;                 ELSE '' &#13;&#10;        	   END&#13;&#10;        FROM WEB_CERTIDAO_DEB('IPTU', :inscricao, -1, CURRENT_DATE) AS wcd&#13;&#10;    ),&#13;&#10;    p_tipoemissao := 1,&#13;&#10;    p_modulo := 'IPTU',&#13;&#10;    p_usuario := -1,&#13;&#10;    p_info_veracidade := ''&#13;&#10;);">
