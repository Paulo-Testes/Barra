      <TableDataSource Name="SqlDadosCertidao" DataType="System.Int32" PropName="Table6" Enabled="true" SelectCommand="WITH FormataDates AS (&#13;&#10;    SELECT &#13;&#10;        TO_CHAR(ADE.data, 'DD/MM/YYYY') AS formata_data,&#13;&#10;        TO_CHAR(ADE.data, 'HH24:MI:SS') AS formata_hora,&#13;&#10;        TO_CHAR(udf_incday(ADE.DATA, ADE.DIAS_VALIDADE), 'DD/MM/YYYY') AS formata_validade,&#13;&#10;        ADE.NUMERO,&#13;&#10;        ADE.ANO,&#13;&#10;        ADE.DIAS_VALIDADE,&#13;&#10;        ADE.REFERENCIAL &#13;&#10;    FROM &#13;&#10;        AAC_DOCUMENTOS_EMISSAO ADE&#13;&#10;    WHERE &#13;&#10;        ADE.REFERENCIAL = :IdReferencial&#13;&#10;)&#13;&#10;SELECT &#13;&#10;    formata_data AS DATA,&#13;&#10;    formata_hora AS HORA,&#13;&#10;    formata_validade AS VALIDADE,&#13;&#10;    NUMERO,&#13;&#10;    ANO,&#13;&#10;    DIAS_VALIDADE,&#13;&#10;    REFERENCIAL &#13;&#10;FROM &#13;&#10;    FormataDates;&#13;&#10;">
