    private void Cabecalho_BeforePrint(object sender, EventArgs e)
    {
      
      string titulo1 = (string)Report.GetColumnValue(&quot;SqlModulo.titulo_1&quot;);
      string titulo2 = (string)Report.GetColumnValue(&quot;SqlModulo.titulo_2&quot;);

      if (string.IsNullOrWhiteSpace(titulo1) &amp;&amp; string.IsNullOrWhiteSpace(titulo2))
      {
        TxtTituloRelatorio.Top = TxtTitulo1.Top;
      }
      else if (string.IsNullOrWhiteSpace(titulo1))
      {
        TxtTituloRelatorio.Top = TxtTitulo2.Top;
        TxtTitulo2.Top = TxtTitulo1.Top;
      }
      else if (string.IsNullOrWhiteSpace(titulo2))
      {
        TxtTituloRelatorio.Top = TxtTitulo2.Top;
      }
    }
 
    
    private void TxtCompetencia_BeforePrint(object sender, EventArgs e)
    {
     TxtCompetencia.Visible = !string.IsNullOrWhiteSpace(Convert.ToString(Report.GetParameterValue(&quot;competencia&quot;))); 
    }
    
    private void Filtro_BeforePrint(object sender, EventArgs e)
    {                                  
      Filtro.Visible = !string.IsNullOrWhiteSpace(Convert.ToString(Report.GetParameterValue(&quot;filtro&quot;)));
    } 
    
    private void Rodape_BeforePrint(object sender, EventArgs e)
    {
      TxtRodape.Text = Convert.ToString(Report.GetColumnValue(&quot;SqlCliente.nome_reduzido&quot;));

      int imprimirUsuario = Convert.ToInt32(Report.GetColumnValue(&quot;SqlModulo.imprimir_usuario&quot;));

      // Replace &lt;Personalizado&gt; with the actual variable or property holding the value
      long personalizadoValue = Convert.ToInt64(Report.GetParameterValue(&quot;personalizado&quot;));

      if (imprimirUsuario == 1)
      {
        TxtRodape.Text += &quot; | &quot; + Convert.ToString(Report.GetColumnValue(&quot;SqlUsuario.ds_usuario&quot;));
      }

      if (personalizadoValue == 1)
      {
        TxtRodape.Text += &quot; | PERSONALIZADO&quot;;
      }
      else if (personalizadoValue == 2)
      {
        TxtRodape.Text += &quot; | GSR - Construtor de Relatórios&quot;;
      }

      // Adicionar a data atual
      TxtRodape.Text += &quot; | &quot; + ((DateTime)Report.GetVariableValue(&quot;Date&quot;));

      string cnpj = Convert.ToString(Report.GetColumnValue(&quot;SqlCliente.cnpj&quot;));
      string cnpjFormatado = FormatarCnpj(cnpj);

      string cep = Convert.ToString(Report.GetColumnValue(&quot;SqlCliente.cep&quot;));
      string cepFormatado = FormatarCep(cep);

      // Armazenar o CEP formatado como um parâmetro
      Report.SetParameterValue(&quot;cep_formatado&quot;, cepFormatado);

      // Armazenar o CNPJ formatado como um parâmetro
      Report.SetParameterValue(&quot;cnpj_formatado&quot;, cnpjFormatado);
    }
    
    static string FormatarCnpj(string cnpj)
    {
      if (cnpj.Length != 14)
      {
        throw new ArgumentException(&quot;O CNPJ deve conter 14 dígitos.&quot;);
      }

      return cnpj.Substring(0, 2) + &quot;.&quot; + cnpj.Substring(2, 3) + &quot;.&quot; + cnpj.Substring(5, 3) + &quot;/&quot; + cnpj.Substring(8, 4) + &quot;-&quot; + cnpj.Substring(12);
    }
    static string FormatarCep(string cep)
    {                                                                                                                                           
      if (cep.Length != 8)
      {
        throw new ArgumentException(&quot;O CEP deve conter 8 dígitos.&quot;);
      }

      return cep.Substring(0, 5) + &quot;-&quot; + cep.Substring(5);
    }
                                                         
  }
}
