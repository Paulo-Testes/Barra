      <TableDataSource Name="Table3" Alias="SqlInfo" DataType="System.Int32" Enabled="true" SelectCommand="SELECT gc.id_pessoa,&#13;&#10;       sp.nome_razao AS nome,&#13;&#10;       sp.documento_formatado AS documento,&#13;&#10;       sp.documento AS documento_pesquisa,&#13;&#10;       COUNT(1) AS qtd_convenio&#13;&#10;FROM gcc_contrato gc&#13;&#10;LEFT OUTER JOIN sis_pessoa sp ON sp.id_pessoa = gc.id_pessoa&#13;&#10;WHERE gc.id_entidade = :id_entidade&#13;&#10;  AND gc.id_tp_contrato = 2&#13;&#10;  AND gc.id_tp_categoria = 2&#13;&#10;  AND 1 = 1&#13;&#10;GROUP BY gc.id_pessoa,&#13;&#10;         sp.nome_razao,&#13;&#10;         sp.documento_formatado,&#13;&#10;         sp.documento&#13;&#10;ORDER BY sp.nome_razao">
