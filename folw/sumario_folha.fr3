    <TfrxADOQuery Name="SqlSumario" UserName="SqlSumario" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="select B.NUMERO_CONTA&#13;&#10;     , B.DESCRICAO&#13;&#10;     , B.TIPO&#13;&#10;     , CAST(COUNT(C.C001) AS INTEGER) AS QUANTIDADE&#13;&#10;     , ROUND(SUM(CASE WHEN B.TIPO = 3 THEN A.VALOR ELSE 0 END), 2) AS VL_VENCIMENTO&#13;&#10;     , ROUND(SUM(CASE WHEN B.TIPO = 2 THEN A.VALOR ELSE 0 END), 2) AS VL_DESCONTO&#13;&#10;     , ROUND(SUM(CASE WHEN B.TIPO = 1 THEN A.VALOR ELSE 0 END), 2) AS VL_CALCULO     &#13;&#10;from grh_movimentos a&#13;&#10;inner join grh_plano_contas b on a.conta = b.numero_conta AND B.RESUMO = 'S'&#13;&#10;inner join grh_dados_mes c on a.chapa = c.c000 and a.competencia = c.competencia and a.suplementar = c.suplementar and c.id_estabelecimento = :id_estabelecimento                                                                                                  &#13;&#10;where a.competencia &#62;= :competencia_inicial and a.competencia &#60;= :competencia_final&#13;&#10;and a.suplementar = coalesce(:suplementar, 0)&#13;&#10;AND ROUND(A.VALOR, 2) &#60;&#62; 0&#13;&#10;AND A.VALOR IS NOT NULL&#13;&#10;AND B.NUMERO_CONTA &#62; 0&#13;&#10; GROUP BY&#13;&#10;       B.NUMERO_CONTA&#13;&#10;     , B.DESCRICAO&#13;&#10;     , B.TIPO&#13;&#10;ORDER BY&#13;&#10;       B.TIPO DESC&#13;&#10;     , B.NUMERO_CONTA     &#13;&#10;  " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlSuplementar" UserName="SqlSuplementar" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT TRIM(A.TITULO) AS TITULO                          &#13;&#10;  FROM AAC_SUPLEMENTAR A&#13;&#10; WHERE A.MODULO = 'FOLW'&#13;&#10;   AND A.COMPETENCIA &#62;= CAST(:CP_ANO AS VARCHAR(4)) || LPAD(CAST(:CP_MES_INICIAL AS VARCHAR(2)), 2, '0')&#13;&#10;   AND A.COMPETENCIA &#60;= CAST(:CP_ANO AS VARCHAR(4)) || LPAD(CAST(:CP_MES_FINAL   AS VARCHAR(2)), 2, '0')&#13;&#10;   AND A.NUM_SUPL     = COALESCE(CAST(:SUPLEMENTAR AS INTEGER), 0)&#13;&#10; ORDER BY&#13;&#10;       A.COMPETENCIA&#13;&#10;  ROWS 1  " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
