<TfrxReport Version="6.4.12" DotMatrixReport="False" IniFile="\Software\Fast Reports" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1535" PreviewOptions.Zoom="1" PrintOptions.Printer="PadrÃ£o" PrintOptions.PrintOnSheet="0" ReportOptions.CreateDate="44706,6855262037" ReportOptions.Description.Text="" ReportOptions.LastChange="45805,4401263426" ScriptLanguage="PascalScript" ScriptText.Text="&#13;&#10;{procedure Page1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;   p_competencia, ano_atual, sub_select_quadra : String;&#13;&#10;begin&#13;&#10;   ano_atual := copy(datetostr(now()),7,4);&#13;&#10;   p_competencia := copy(&#60;competencia&#62;,1,4);&#13;&#10;&#13;&#10;   if (ano_atual = p_competencia) then&#13;&#10;   begin&#13;&#10;      SqlCabecalho.Sql.Text :=&#13;&#10;         'SELECT                                                                                      '&#13;&#10;        +'    a.c000 AS REGISTRO,                                                                     '&#13;&#10;        +'    d.c008 as letra_referencia_salarial,                                                     '&#13;&#10;        +'    udf_copy(a.c001,1,35) as NOME ,                                                         '&#13;&#10;        +'    (SELECT udf_copy(b.descricao,1,35)                                                      '&#13;&#10;        +'       FROM grh_cargos b                                                                    '&#13;&#10;        +'      WHERE b.codigo_cargo = a.c005) AS CARGO,                                              '&#13;&#10;        +'    a.c030 AS RG,                                                                           '&#13;&#10;        +'    a.c031 AS CPF,                                                                          '&#13;&#10;        +'    udf_copy(Y.Descricao,1,20) AS LOCAL_TRABALHO,                                           '&#13;&#10;        +'    COALESCE(f.DESCRICAO,                                                                   '&#13;&#10;        +'            (SELECT udf_copy(b.descricao,1,35)                                              '&#13;&#10;        +'               FROM grh_cargos b                                                            '&#13;&#10;        +'              WHERE b.codigo_cargo = a.c005)) AS FUNCAO,                                                          '&#13;&#10;        +'    udf_strzero(extract(day from CAST(a.c002 AS TIMESTAMP)), 2) || ''/'' ||                 '&#13;&#10;        +'    udf_strzero(extract(month from CAST(a.c002 AS TIMESTAMP)), 2) ||''/'' ||                '&#13;&#10;        +'    udf_strzero(udf_int(extract(year from CAST(a.c002 AS TIMESTAMP))), 4) as DATA_ADMISSAO, '&#13;&#10;        +'    a.c032 AS PIS                                                                           '&#13;&#10;        +'FROM grh_cadastro_funcionarios a                                                            '&#13;&#10;        +'LEFT JOIN grh_dados_mes d ON                                                                '&#13;&#10;        +'    (d.c000 = a.c000                                                                        '&#13;&#10;        +'    AND d.competencia = :competencia                                                        '&#13;&#10;        +'    AND d.suplementar = 0)                                                                  '&#13;&#10;        +'LEFT JOIN grh_cargos f ON                                                                   '&#13;&#10;        +'    (f.codigo_cargo = COALESCE(d.c059,                                                      '&#13;&#10;        +'    a.c059))                                                                                '&#13;&#10;        +'LEFT JOIN GRH_LOCAL_TRABALHO Y ON                                                           '&#13;&#10;        +'    (Y.CODIGO = COALESCE(d.C065,                                                            '&#13;&#10;        +'    a.c065))                                                                                '&#13;&#10;        +'WHERE                                                                                       '&#13;&#10;        +'    a.c080 IS NOT NULL                                                                      '&#13;&#10;        +'    AND a.c000 = :matricula                                                                 '&#13;&#10;        +'ORDER BY                                                                                    '&#13;&#10;        +'    d.competencia DESC                                                                      ';&#13;&#10;   end&#13;&#10;   else&#13;&#10;   begin&#13;&#10;      SqlCabecalho.Sql.Text :=&#13;&#10;         '  SELECT                                                                                    '&#13;&#10;        +'     a.c000 AS REGISTRO,                                                                    '&#13;&#10;        +'    a.c008 as letra_referencia_salarial,                                                     '&#13;&#10;        +'     udf_copy(a.c001,1,35) AS NOME ,                                                        '&#13;&#10;        +'     COALESCE(f.DESCRICAO,                                                                  '&#13;&#10;        +'     (SELECT udf_copy(b.descricao,1,35)                                                     '&#13;&#10;        +'        FROM grh_cargos b                                                                   '&#13;&#10;        +'       WHERE b.codigo_cargo = a.c005)) AS FUNCAO,                                           '&#13;&#10;        +'     (SELECT udf_copy(b.descricao,1,35)                                                     '&#13;&#10;        +'        FROM grh_cargos b                                                                   '&#13;&#10;        +'       WHERE b.codigo_cargo = a.c005) AS CARGO,                                             '&#13;&#10;        +'     a.c030 AS RG,                                                                          '&#13;&#10;        +'     a.c031 AS CPF,                                                                         '&#13;&#10;        +'     udf_copy(Y.Descricao,1,20) AS LOCAL_TRABALHO,                                          '&#13;&#10;        +'     udf_strzero(extract(day from CAST(a.c002 AS TIMESTAMP)), 2) || ''/'' ||                '&#13;&#10;        +'     udf_strzero(extract(month from CAST(a.c002 AS TIMESTAMP)), 2) ||''/'' ||               '&#13;&#10;        +'     udf_strzero(udf_int(extract(year from CAST(a.c002 AS TIMESTAMP))), 4) as DATA_ADMISSAO,'&#13;&#10;        +'     a.c032 AS PIS                                                                          '&#13;&#10;        +' FROM grh_dados_mes a                                                                       '&#13;&#10;        +' LEFT JOIN grh_cargos f ON                                                                  '&#13;&#10;        +'     (f.codigo_cargo = a.c059)                                                              '&#13;&#10;        +' LEFT JOIN GRH_LOCAL_TRABALHO Y ON                                                          '&#13;&#10;        +'     (Y.CODIGO = a.c065)                                                                    '&#13;&#10;        +' WHERE                                                                                      '&#13;&#10;        +'     a.c000 = :matricula                                                                    '&#13;&#10;        +'     AND a.competencia = :competencia                                                       '&#13;&#10;        +'     AND a.suplementar = :suplementar                                                       '&#13;&#10;        +' ORDER BY                                                                                   '&#13;&#10;        +'     a.competencia DESC                                                                     '&#13;&#10;&#13;&#10;   end;&#13;&#10;&#13;&#10;end;       }&#13;&#10;&#13;&#10;procedure ReportSummary1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SqlSalarioBase.Open;&#13;&#10;  SqlCrypt.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure MasterData1OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SqlCompetencia.Open;&#13;&#10;  SqlCabecalho.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;&#13;&#10;procedure Memo50OnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if(trim(&#60;SqlCabecalho.&#34;nome_social&#34;&#62;) = '') then&#13;&#10;  begin&#13;&#10;    //Move a linha para cima&#13;&#10;    Line5.Height := Line4.Height;&#13;&#10;&#13;&#10;    //Oculta os campos do Nome Social e move os campos do cargo em seu lugar.&#13;&#10;    Memo50.Visible := False;&#13;&#10;    Memo51.Visible := False;&#13;&#10;    Memo58.Left := Memo50.Left;&#13;&#10;    Memo60.Left := Memo51.Left;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;end." StoreInDFM="False">
