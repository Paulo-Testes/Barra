        <Column Name="cpf_cnpj_razao_banco" DataType="System.String"/>
        <Column Name="cpf_cnpj_resp_banco" DataType="System.String"/>
        <Column Name="ie" DataType="System.String"/>
        <Column Name="tipocertidao" DataType="System.String"/>
        <Column Name="tipocertidaonp" DataType="System.String"/>
        <Column Name="situacao" DataType="System.String"/>
