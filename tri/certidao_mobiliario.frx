      <TableDataSource Name="Table1" Alias="SqlDadosCertidao" DataType="System.Int32" Enabled="true" SelectCommand="SELECT&#13;&#10;    LPAD(EXTRACT(DAY FROM ADE.data)::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(MONTH FROM ADE.data)::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(YEAR FROM ADE.data)::TEXT, 4, '0') AS DATA,&#13;&#10;&#13;&#10;    LPAD(EXTRACT(HOUR FROM ADE.data)::TEXT, 2, '0') || ':' ||&#13;&#10;    LPAD(EXTRACT(MINUTE FROM ADE.data)::TEXT, 2, '0') || ':' ||&#13;&#10;    LPAD(EXTRACT(SECOND FROM ADE.data)::INT::TEXT, 2, '0') AS HORA,&#13;&#10;    &#13;&#10;    LPAD(EXTRACT(DAY FROM   (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(MONTH FROM (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(YEAR FROM  (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 4, '0') AS VALIDADE,&#13;&#10;&#13;&#10;    ADE.numero,&#13;&#10;    ADE.ano,&#13;&#10;    ADE.dias_validade,&#13;&#10;    ADE.referencial&#13;&#10;&#13;&#10;FROM AAC_DOCUMENTOS_EMISSAO ADE&#13;&#10;WHERE ADE.referencial = :IdReferencial;">
        <Column Name="data" DataType="System.String"/>
        <Column Name="hora" DataType="System.String"/>
        <Column Name="validade" DataType="System.String"/>
        <Column Name="numero" DataType="System.Int32"/>
        <Column Name="ano" DataType="System.String"/>
        <Column Name="dias_validade" DataType="System.Int32"/>
        <Column Name="referencial" DataType="System.Int32"/>
        <CommandParameter Name="IdReferencial" DataType="9" Expression="[SqlCrypt.refcertidao]" DefaultValue="0"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="SqlCertidao" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;   cast(:inscricao as varchar(30)) as inscricao,&#13;&#10;   cast(:ip_acessado as varchar(30)) as ip_acessado,       &#13;&#10;   TRIM(WCD.PREFIXO) AS PREFIXO,                                                                                     &#13;&#10;   TRIM(WCD.LOGRA) as LOGRA,&#13;&#10;   TRIM(WCD.N_IMOVEL) AS NUM_IMOVEL,&#13;&#10;   TRIM(WCD.BAIRRO) AS BAIRRO,&#13;&#10;   TRIM(WCD.QUADRA) AS QUADRA,&#13;&#10;   TRIM(WCD.LOTE) AS LOTE,&#13;&#10;   TRIM(WCD.COMPLEM) AS COMPLEM,&#13;&#10;   TRIM(WCD.N_IMOVEL) AS N_IMOVEL,&#13;&#10;   TRIM(WCD.ATIVIDADE) AS ATIVIDADE,&#13;&#10;   TRIM(WCD.COMPR) AS COMPR,&#13;&#10;   TRIM(WCD.PROP) AS PROP,&#13;&#10;   TRIM(WCD.CEP) AS CEP,                                                           &#13;&#10;   TRIM(I.CIDADE) AS CIDADE,&#13;&#10;   TRIM(I.ESTADO) AS ESTADO,       &#13;&#10;   coalesce(P1.CPF_CNPJ) as CPF_CNPJ_RAZAO_BANCO,&#13;&#10;   coalesce(P2.CPF_CNPJ) as CPF_CNPJ_RESP_BANCO,&#13;&#10;   (select DOCUMENTO from sis_pessoa_documento sp where id_tp_documento = 1 and sp.id_pessoa = p1.id_pessoa limit 1) as rg,&#13;&#10;   (select DOCUMENTO from sis_pessoa_documento sp where id_tp_documento = 3 and sp.id_pessoa = p1.id_pessoa limit 1) as ie,&#13;&#10;	RTRIM( case when wcd.tipocert = 0 then 'NÃO CONSTAM' &#13;&#10;	      when wcd.tipocert = 1 then 'CONSTAM' &#13;&#10;	      when wcd.tipocert = 2 then 'CONSTAM PARCELAS DE ACORDO EM DIA DE' else '' end) as tipocertidao,&#13;&#10;	RTRIM( case when wcd.tipocert = 0 then 'Negativa' &#13;&#10;	      when wcd.tipocert = 1 then 'Positiva' &#13;&#10;	      when wcd.tipocert = 2 then 'Positiva com Efeito Negativa' else '' end) as tipocertidaonp,          &#13;&#10;	   (select case A.M005 when 'S' then 'ATIVA' else 'INATIVA' end&#13;&#10;	     	from entidade001.CTEC_MOBILIARIO A&#13;&#10;	     where a.referencial = :referencial) as situacao                       &#13;&#10;from      WEB_CERTIDAO_DEB('ISSW',CAST(:inscricao as varchar), -1, CAST(CURRENT_TIMESTAMP AS DATE)) WCD&#13;&#10;left join PROC_CTEC_ENDERECOS(WCD.COD_LOG) I on 0=0     &#13;&#10;left join SIS_PESSOA         P1                        on P1.ID_PESSOA = WCD.COD_COMPR -- COMPROMISSÁRIO&#13;&#10;left join SIS_PESSOA           P2                        on P2.ID_PESSOA = WCD.COD_PROP    -- PROPRIETÁRIO&#13;&#10;left join sis_pessoa_documento SPD                       on SPD.Id_pessoa = P1.id_pessoa -- MANTEMOS A LÓGICA ANTERIOR PARA A CHAMADA DO RG/IE - ERA SIS_PESSOA_FISICA PF1 E IS_PESSOA_JURIDICA PJ1 E AGORA FICOU PRA P1&#13;&#10;&#13;&#10;&#13;&#10;">
        <Column Name="inscricao" DataType="System.String"/>
        <Column Name="ip_acessado" DataType="System.String"/>
        <Column Name="prefixo" DataType="System.String"/>
        <Column Name="logra" DataType="System.String"/>
        <Column Name="num_imovel" DataType="System.String"/>
