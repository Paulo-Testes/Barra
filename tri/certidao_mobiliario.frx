      <TableDataSource Name="Table" Alias="SqlCrypt" DataType="System.Int32" Enabled="true" SelectCommand="select CODIGO,REFCERTIDAO from ADE_CRYPT(&#13;&#10;:referencial,&#13;&#10;(select case when wcd.tipocert = 0 then 'CN' &#13;&#10;      when wcd.tipocert = 1 then 'CP' &#13;&#10;      when wcd.tipocert = 2 then 'CPN' else '' end                                                               &#13;&#10;  from WEB_CERTIDAO_DEB('ISSW',:inscricao, -1, CAST(CURRENT_TIMESTAMP AS DATE)) WCD),&#13;&#10;1,&#13;&#10;'ISSW',&#13;&#10;-1,&#13;&#10;'')">
