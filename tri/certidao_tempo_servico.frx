      <TableDataSource Name="SqlUsuario" Restrictions="DontMove, DontResize, DontModify, DontDelete" DataType="System.Int32" Enabled="true" SelectCommand="select log.nome&#13;&#10;     , case when usu.ds_usuario is not null then ' (' || trim(usu.ds_usuario) || ')' end as ds_usuario &#13;&#10;  from sis_login log &#13;&#10;  left join sis_usuario usu on usu.id_usuario = :id_usuario&#13;&#10; where log.id_login = :id_login&#13;&#10; limit 1">
        <Column Name="ds_usuario" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="nome" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <CommandParameter Name="id_login" DataType="9" Expression="[idLogin]"/>
        <CommandParameter Name="id_usuario" DataType="9" Expression="[idUsuario]"/>
      </TableDataSource>
      <TableDataSource Name="Table" Alias="SqlCrypt" DataType="System.Int32" Enabled="true" SelectCommand="select CODIGO,REFCERTIDAO from ADE_CRYPT(&#13;&#10;:referencial,&#13;&#10;(select case when wcd.tipocert = 0 then 'CN' &#13;&#10;      when wcd.tipocert = 1 then 'CP' &#13;&#10;      when wcd.tipocert = 2 then 'CPN' else '' end                                                               &#13;&#10;  from	WEB_CERTIDAO_DEB('IPTU',:inscricao, -1, CAST(CURRENT_TIMESTAMP AS DATE)) WCD),&#13;&#10;1,&#13;&#10;'ISSW',&#13;&#10;-1,&#13;&#10;'')">
        <Column Name="codigo" DataType="System.String"/>
        <Column Name="refcertidao" DataType="System.Int32"/>
        <CommandParameter Name="referencial" DataType="9" Expression="[SqlCertidao.referencial]"/>
        <CommandParameter Name="inscricao" DataType="22" Expression="[inscricao]"/>
      </TableDataSource>
      <TableDataSource Name="Table1" Alias="SqlDadosCertidao" DataType="System.Int32" Enabled="true" SelectCommand="SELECT&#13;&#10;    LPAD(EXTRACT(DAY FROM ADE.data)::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(MONTH FROM ADE.data)::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(YEAR FROM ADE.data)::TEXT, 4, '0') AS DATA,&#13;&#10;&#13;&#10;    LPAD(EXTRACT(HOUR FROM ADE.data)::TEXT, 2, '0') || ':' ||&#13;&#10;    LPAD(EXTRACT(MINUTE FROM ADE.data)::TEXT, 2, '0') || ':' ||&#13;&#10;    LPAD(EXTRACT(SECOND FROM ADE.data)::INT::TEXT, 2, '0') AS HORA,&#13;&#10;&#13;&#10;    LPAD(EXTRACT(DAY FROM (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(MONTH FROM (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(YEAR FROM (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 4, '0') AS VALIDADE,&#13;&#10;&#13;&#10;    ADE.numero,&#13;&#10;    ADE.ano,&#13;&#10;    ADE.dias_validade,&#13;&#10;    ADE.referencial&#13;&#10;&#13;&#10;FROM AAC_DOCUMENTOS_EMISSAO ADE&#13;&#10;WHERE ADE.referencial = :IdReferencial;&#13;&#10;">
        <Column Name="data" DataType="System.String"/>
        <Column Name="hora" DataType="System.String"/>
        <Column Name="validade" DataType="System.String"/>
        <Column Name="numero" DataType="System.Int32"/>
        <Column Name="ano" DataType="System.String"/>
        <Column Name="dias_validade" DataType="System.Int32"/>
        <Column Name="referencial" DataType="System.Int32"/>
        <CommandParameter Name="IdReferencial" DataType="9" Expression="[SqlCrypt.refcertidao]"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="SqlCertidao" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;	 CAST(:ip_acessado  as varchar(30)) as ip_acessado&#13;&#10;	,trim(M.M000) as inscricao&#13;&#10;	,m.referencial                      as referencial&#13;&#10;	,trim(p.nome_razao)                 as nome_razao&#13;&#10;	,CASE &#13;&#10;	  WHEN    &#13;&#10;	          length(coalesce(PF.CPF, PJ.CNPJ)) = 11 THEN &#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 1, 3) || '.' ||&#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 4, 3) || '.' ||&#13;&#10;	       SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 7, 3) || '-' ||&#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 10, 2)&#13;&#10;				&#13;&#10;      WHEN &#13;&#10;              length(coalesce(PF.CPF, PJ.CNPJ)) = 14 THEN &#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 1, 2) || '.' ||&#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 3, 3) || '.' ||&#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 6, 3) || '/' ||&#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 9, 4) || '-' ||&#13;&#10;		   SUBSTRING(coalesce(PF.CPF, PJ.CNPJ), 13, 2)&#13;&#10;		   &#13;&#10;	   ELSE coalesce(PF.CPF, PJ.CNPJ)&#13;&#10;	  END AS cpf_cnpj&#13;&#10;	 ,SA.S002 as atividade&#13;&#10;	 ,ma.a009 as atividade_ativda&#13;&#10;	 ,trim(trim(upper(PCE.DES_PREFIXO)) || ' ' || trim(PCE.DES_TITULO)) || ' ' || trim(PCE.NOME) || ' ' ||&#13;&#10;	  trim(M.M019) || ' - ' || trim(PCE.BAIRRO) || ' - ' || trim(PCE.CIDADE) || '/' || trim(PCE.ESTADO) as ENDERECO1&#13;&#10;	  &#13;&#10;	 ,trim(trim(UPPER(WCD.PREFIXO)) || ' ' || trim(UPPER(WCD.LOGRA))) || ' - Nº: ' || trim(UPPER(WCD.N_IMOVEL)) || ' - BAIRRO: ' ||&#13;&#10;	  trim(UPPER(WCD.BAIRRO)) || ' - CEP: ' || trim(WCD.CEP) || ' - ' || trim(PCE.CIDADE) || '/' || trim(PCE.ESTADO) as ENDERECO&#13;&#10;	  &#13;&#10;	 ,iif(WCD.TIPOCERT = 0,'Nada Deve','Deve') as CONSTA_DEBITOS&#13;&#10;	 ,coalesce(MA.A003, m.m011) as DATA_INICIO --Início da Atividade                                                                                                                                                                                                                                       &#13;&#10;     ,coalesce(MA.A004, m.m043) as DATA_FIM    --Encerramento da Atividade  &#13;&#10;&#13;&#10;&#13;&#10;from ctec_mobiliario M&#13;&#10; left join ctec_mob_atividade                                          ma      on ma.referencial_mob    = m.referencial          &#13;&#10;                                                                              and ma.a000 = m.m004 &#13;&#10; left join issw_sub_atividades                                         sa      on sa.s000               = m.m004 &#13;&#10; left join sis_pessoa                                                  p       on p.id_pessoa           = m.m001&#13;&#10; left join sis_pessoa_fisica                                           pf      on pf.id_pessoa_fisica   = p.id_pessoa_fisica &#13;&#10; left join sis_pessoa_juridica                                         pj      on pj.id_pessoa_juridica = p.id_pessoa_juridica &#13;&#10; left join web_certidao_deb('ISSW', :inscricao, -1, cast('today' as date)) WCD on 1=1 &#13;&#10; left join proc_ctec_enderecos(WCD.COD_LOG) /*REF.LOGRADOURO  m.m015*/              PCE on 0=0&#13;&#10;  where&#13;&#10;   M.M000 = :inscricao and&#13;&#10;   MA.REFERENCIAL_MOB = M.REFERENCIAL and&#13;&#10;   SA.COMPETENCIA = (select max(COMPETENCIA) from AAC_COMPETENCIAS where MODULO = 'ISSW')">
        <Column Name="ip_acessado" DataType="System.String"/>
        <Column Name="nome_razao" DataType="System.String"/>
        <Column Name="cpf_cnpj" DataType="System.String"/>
        <Column Name="atividade" DataType="System.String"/>
        <Column Name="atividade_ativda" DataType="System.String"/>
        <Column Name="endereco" DataType="System.String"/>
        <Column Name="consta_debitos" DataType="System.String"/>
        <Column Name="data_inicio" DataType="System.DateTime"/>
        <Column Name="data_fim" DataType="System.DateTime"/>
        <Column Name="referencial" DataType="System.Int32"/>
        <Column Name="endereco1" DataType="System.String"/>
        <Column Name="inscricao" DataType="System.String"/>
        <CommandParameter Name="inscricao" DataType="22" Expression="[inscricao]"/>
        <CommandParameter Name="ip_acessado" DataType="9" Expression="[ipAcessado]"/>
        <CommandParameter Name="inscricao" DataType="22" Expression="[inscricao]"/>
      </TableDataSource>
    </PostgresDataConnection>
