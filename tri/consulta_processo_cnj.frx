      <TableDataSource Name="Table2" Alias="SqlParcAcordo" DataType="System.Int32" Enabled="true" SelectCommand="select &#13;&#10;	 gp.parcela&#13;&#10;    ,gp.vencimento&#13;&#10;    ,gp.valor&#13;&#10;    ,gp.data_pagto&#13;&#10;    ,coalesce(gp.vlr_pago, 0.0) as vlr_pago                                   &#13;&#10;    ,case &#13;&#10;    	when gp.status = '0'  then 'Sem Pagamento'&#13;&#10;        when gp.status = '1'  then 'Pendente'&#13;&#10;        when gp.status = '2'  then 'Anulada'&#13;&#10;        when gp.status = '3'  then 'Pagamento Cancelado'&#13;&#10;        when gp.status = '4'  then 'Baixa Manual'&#13;&#10;        when gp.status = '5'  then 'Baixa Automática'&#13;&#10;        when gp.status = '6'  then 'Restituída Parcial'&#13;&#10;        when gp.status = '7'  then 'Restituída Total'&#13;&#10;        when gp.status = '8'  then 'Carnê Cancelado'&#13;&#10;        when gp.status = '9'  then 'Carnê Pago em Única'&#13;&#10;        when gp.status = '10' then 'Carnê Pago em Parcelas '&#13;&#10;        when gp.status = '11' then 'Dívida Ativa'&#13;&#10;        when gp.status = '12' then 'Baixa Após Dívida'&#13;&#10;        when gp.status = '13' then 'Parcela Suspensa'&#13;&#10;    end as status&#13;&#10;    &#13;&#10;from geda_parcelas gp&#13;&#10;where gp.referencial_carne = :ref_acordo&#13;&#10;order by gp.parcela">
        <Column Name="parcela" DataType="System.Int32"/>
        <Column Name="vencimento" DataType="System.DateTime"/>
        <Column Name="valor" DataType="System.Double"/>
        <Column Name="data_pagto" DataType="System.DateTime"/>
        <Column Name="vlr_pago" DataType="System.Double"/>
        <Column Name="status" DataType="System.String"/>
        <CommandParameter Name="ref_acordo" DataType="9" DefaultValue="-1"/>
      </TableDataSource>
      <TableDataSource Name="Table3" Alias="TotalParc" DataType="System.Int32" Enabled="true" SelectCommand="SELECT &#13;&#10;    SUM(COALESCE(gp.vlr_pago, 0.0)) AS total_vlr_pago,    -- Soma de todos os valores pagos&#13;&#10;    SUM(gp.valor) AS total_valor                          -- Soma de todos os valores das parcelas&#13;&#10;FROM geda_parcelas gp&#13;&#10;WHERE gp.referencial_carne = :ref_acordo">
        <Column Name="total_vlr_pago" DataType="System.Double"/>
        <Column Name="total_valor" DataType="System.Double"/>
        <CommandParameter Name="ref_acordo" DataType="9" Expression="[SqlProcesso.d012]"/>
      </TableDataSource>
      <TableDataSource Name="Table4" Alias="Custas" DataType="System.Int32" Enabled="true" SelectCommand="select b.processo_cnj, b.id_execucao, sum(b.acrescimo20 + b.acrescimo2 + b.caarj + b.citacao_por_correios + b.atos_distribuidores + b.atos_escrivaes + b.fundperj + b.funperj + b.funarpen + b.taxa_bancaria + b.taxa_judiciaria) from (&#13;&#10;SELECT distinct  &#13;&#10;    det.processo_cnj,&#13;&#10;    det.id_execucao,&#13;&#10;    --s.id_sequencia,&#13;&#10;&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 11), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)- pg.pago_acrescim                                                                                               AS ACRESCIMO20,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 14), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_acrescim2                                                                                               AS ACRESCIMO2,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 3), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_caarj                                                                                               AS CAARJ,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 5), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_correio                                                                                               AS CITACAO_POR_CORREIOS,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 7), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_distrib                                                                                               AS ATOS_DISTRIBUIDORES,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 6), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_escrivao                                                                                               AS ATOS_ESCRIVAES,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 9), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_fundperj                                                                                               AS FUNDPERJ,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 8), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_funperj                                                                                               AS FUNPERJ,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 103), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)-pg.pago_funarpen                                                                                               AS FUNARPEN,&#13;&#10;    CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 15), ',',&#13;&#10;                 '.') AS DOUBLE PRECISION)                                                                                               AS TAXA_BANCARIA,              &#13;&#10;    least(CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel =  4), ',', '.') AS DOUBLE PRECISION), -- AS TAXA_JUDICIARIA_MAX,&#13;&#10;    greatest(round((d.vl_div*1.1)*0.04,2), CAST(REPLACE((SELECT A.conteudo FROM AAC_VARIAVEIS A WHERE A.modulo = 'GEDA' AND A.id_variavel = 10), ',', '.') AS DOUBLE PRECISION)))as Taxa_judiciaria&#13;&#10;&#13;&#10;FROM GEDA_execucao DET&#13;&#10;&#13;&#10;    left join sis_sequencia s on s.id_sequencia&lt;61&#13;&#10;    left join lateral (select string_agg(cast(d000 as varchar),',')as div,sum(d.d010)as vl_div from geda_ldivida D where d.id_execucao = det.id_execucao AND d.d029 NOT IN (3, 4))d on true&#13;&#10;    left join proc_calcula_refis (cast(d.div as varchar),s.id_sequencia)r on TRUE&#13;&#10;    left join proc_pago_custas(det.id_execucao)pg on true&#13;&#10;&#13;&#10;WHERE det.id_mestre = :id_execucao and det.status in (0,1)&#13;&#10;&#13;&#10;) b&#13;&#10;group by 1,2">
        <Column Name="processo_cnj" DataType="System.String"/>
        <Column Name="id_execucao" DataType="System.Int32"/>
        <Column Name="sum" DataType="System.Double"/>
        <CommandParameter Name="id_execucao" DataType="9" Expression="[SqlProcesso.id_mestre]"/>
      </TableDataSource>
    </PostgresDataConnection>
