  <Dictionary>
    <PostgresDataConnection Name="Connection" ConnectionString="rijcmlqiSaocAniJQNoKNFeaTFzMe98Srvu/H0B5rG99iH0rlBGEpz4pTYWSRBdb4fQEIusIPd2TwctcDPBb5WOcH80F6acXIkfo06CPlktUp15TPeummrtQ1jzaU4CBKc3sfJxOM+aoYxh9roKs5XlDt+GOA==">
      <TableDataSource Name="SqlEntidade" DataType="System.Int32" Enabled="true" SelectCommand="select coalesce(enti.ds_entidade, lice.ds_licenciado) as nome&#13;&#10;     , pess.documento_formatado as cnpj&#13;&#10;     , lice.imagem as brasao&#13;&#10;     , coalesce(enti.sigla, lice.sigla) as sigla&#13;&#10;     , enti.imagem as logo&#13;&#10;     , case when enti.id_entidade is not null then ende.endereco end as endereco&#13;&#10;     , fone.contato as telefone&#13;&#10;     , mail.contato as email&#13;&#10;     , site.contato as site&#13;&#10;  from sis_licenciado            lice&#13;&#10;  left join sis_entidade         enti on enti.id_entidade        = :id_entidade&#13;&#10;  left join sis_pessoa           pess on pess.id_pessoa_juridica = enti.id_pessoa_juridica&#13;&#10;  left join sis_pessoa_endereco  ende on ende.id_pessoa          = pess.id_pessoa&#13;&#10;                                     and ende.principal          = 1&#13;&#10;  left join sis_pessoa_contato   fone on fone.id_pessoa          = pess.id_pessoa&#13;&#10;                                     and fone.id_tp_contato      = 2&#13;&#10;                                     and fone.principal          = true&#13;&#10;  left join sis_pessoa_contato   mail on mail.id_pessoa          = pess.id_pessoa&#13;&#10;                                     and mail.id_tp_contato      = 1&#13;&#10;                                     and mail.principal          = true&#13;&#10;  left join sis_pessoa_contato   site on site.id_pessoa          = pess.id_pessoa&#13;&#10;                                     and site.id_tp_contato      = 4&#13;&#10;                                     and site.principal          = true&#13;&#10; limit 1">
        <Column Name="nome" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="endereco" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="telefone" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="cnpj" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="brasao" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.Byte[]" BindableControl="Picture"/>
        <Column Name="sigla" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="logo" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.Byte[]" BindableControl="Picture"/>
        <Column Name="email" DataType="System.String"/>
        <Column Name="site" DataType="System.String"/>
        <CommandParameter Name="id_entidade" DataType="9" Expression="[idEntidade]"/>
      </TableDataSource>
      <TableDataSource Name="SqlModulo" Restrictions="DontMove, DontResize, DontModify, DontDelete" DataType="System.Int32" Enabled="true" SelectCommand="select a.titulo_1&#13;&#10;     , a.titulo_2&#13;&#10;  from sis_modulo a&#13;&#10; where a.id_modulo = :id_modulo">
        <Column Name="titulo_1" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="titulo_2" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <CommandParameter Name="id_modulo" DataType="9" Expression="[idModulo]"/>
      </TableDataSource>
      <TableDataSource Name="SqlUsuario" Restrictions="DontMove, DontResize, DontModify, DontDelete" DataType="System.Int32" Enabled="true" SelectCommand="select log.nome&#13;&#10;     , case when usu.ds_usuario is not null then ' (' || trim(usu.ds_usuario) || ')' end as ds_usuario &#13;&#10;  from sis_login log &#13;&#10;  left join sis_usuario usu on usu.id_usuario = :id_usuario&#13;&#10; where log.id_login = :id_login&#13;&#10; limit 1">
        <Column Name="ds_usuario" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <Column Name="nome" Restrictions="DontMove, DontResize, DontModify, DontEdit, DontDelete" DataType="System.String"/>
        <CommandParameter Name="id_login" DataType="9" Expression="[idLogin]"/>
        <CommandParameter Name="id_usuario" DataType="9" Expression="[idUsuario]"/>
      </TableDataSource>
      <TableDataSource Name="Table" Alias="SqlCrypt" DataType="System.Int32" Enabled="true" SelectCommand="select CODIGO,REFCERTIDAO from ADE_CRYPT(&#13;&#10;:referencial,&#13;&#10;(select case when wcd.tipocert = 0 then 'CN' &#13;&#10;      when wcd.tipocert = 1 then 'CP' &#13;&#10;      when wcd.tipocert = 2 then 'CPN' else '' end                                                               &#13;&#10;  from	WEB_CERTIDAO_DEB('IPTU',:inscricao, -1, CAST(CURRENT_TIMESTAMP AS DATE)) WCD),&#13;&#10;1,&#13;&#10;'IPTU',&#13;&#10;-1,&#13;&#10;'')">
        <Column Name="codigo" DataType="System.String"/>
        <Column Name="refcertidao" DataType="System.Int32"/>
        <CommandParameter Name="inscricao" DataType="22" Expression="[inscricao]"/>
        <CommandParameter Name="referencial" DataType="9" Expression="[referencial]"/>
      </TableDataSource>
      <TableDataSource Name="Table1" Alias="SqlDadosCertidao" DataType="System.Int32" Enabled="true" SelectCommand="SELECT&#13;&#10;    LPAD(EXTRACT(DAY FROM ADE.data)::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(MONTH FROM ADE.data)::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(YEAR FROM ADE.data)::TEXT, 4, '0') AS DATA,&#13;&#10;&#13;&#10;    LPAD(EXTRACT(HOUR FROM ADE.data)::TEXT, 2, '0') || ':' ||&#13;&#10;    LPAD(EXTRACT(MINUTE FROM ADE.data)::TEXT, 2, '0') || ':' ||&#13;&#10;    LPAD(EXTRACT(SECOND FROM ADE.data)::INT::TEXT, 2, '0') AS HORA,&#13;&#10;&#13;&#10;    LPAD(EXTRACT(DAY FROM (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(MONTH FROM (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 2, '0') || '/' ||&#13;&#10;    LPAD(EXTRACT(YEAR FROM (ADE.data + (ADE.dias_validade || ' days')::INTERVAL))::TEXT, 4, '0') AS VALIDADE,&#13;&#10;&#13;&#10;    ADE.numero,&#13;&#10;    ADE.ano,&#13;&#10;    ADE.dias_validade,&#13;&#10;    ADE.referencial&#13;&#10;&#13;&#10;FROM AAC_DOCUMENTOS_EMISSAO ADE&#13;&#10;WHERE ADE.referencial = :IdReferencial;&#13;&#10;">
        <Column Name="data" DataType="System.String"/>
        <Column Name="hora" DataType="System.String"/>
        <Column Name="validade" DataType="System.String"/>
        <Column Name="numero" DataType="System.Int32"/>
        <Column Name="ano" DataType="System.String"/>
        <Column Name="dias_validade" DataType="System.Int32"/>
        <Column Name="referencial" DataType="System.Int32"/>
        <CommandParameter Name="IdReferencial" DataType="9" Expression="[SqlCrypt.refcertidao]"/>
      </TableDataSource>
      <TableDataSource Name="Table2" Alias="SqlCertidao" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;  cast(:inscricao as varchar(30))   as inscricao,&#13;&#10;  cast(:ip_acessado as varchar(30)) as ip_acessado,     &#13;&#10;  TRIM(WCD.PROP)                    as  PROP,&#13;&#10;  TRIM(WCD.PREFIXO)                 AS PREFIXO,                                                                                     &#13;&#10;  TRIM(WCD.LOGRA)                   as LOGRADOURO,&#13;&#10;  TRIM(WCD.N_IMOVEL)                AS NUM_IMOVEL,&#13;&#10;  TRIM(WCD.BAIRRO)                  AS BAIRRO,&#13;&#10;  TRIM(WCD.QUADRA)                  AS QUADRA,&#13;&#10;  TRIM(WCD.LOTE)                    AS LOTE,&#13;&#10;  TRIM(WCD.COMPLEM)                 AS COMPLEM,&#13;&#10;  TRIM(WCD.CEP)                     AS CEP,                                                          &#13;&#10;  TRIM(I.CIDADE)                    AS CIDADE,&#13;&#10;  TRIM(I.ESTADO)                    AS ESTADO,&#13;&#10;  TRIM(P1.documento)            as CPF_CNPJ_COMPR_BANCO,&#13;&#10;  TRIM(P2.documento)            as CPF_CNPJ_PROP_BANCO,&#13;&#10;  TRIM(case when wcd.tipocert = 0 then 'NÃO CONSTAM' &#13;&#10;                 when wcd.tipocert = 1 then 'CONSTAM' &#13;&#10;                 when wcd.tipocert = 2 then 'CONSTAM PARCELAS DE ACORDO EM DIA DE' else '' end) as tipocertidao,&#13;&#10;  TRIM(case when wcd.tipocert = 0 then 'Negativa' &#13;&#10;                 when wcd.tipocert = 1 then 'Positiva' &#13;&#10;                 when wcd.tipocert = 2 then 'Positiva com Efeito Negativa' else '' end) as tipocertidaonp,          &#13;&#10;  (select case A.I012 when 'S' then 'ATIVA' else 'INATIVA' end&#13;&#10;   	from CTEC_IMOVEIS A&#13;&#10;    where A.referencial = :referencial) as situacao                                                               &#13;&#10;from WEB_CERTIDAO_DEB('IPTU',:inscricao, -1, CAST(CURRENT_TIMESTAMP AS DATE)) WCD&#13;&#10;    left join PROC_CTEC_ENDERECOS(WCD.COD_LOG) I on 0=0   &#13;&#10;    left join SIS_PESSOA           P1			 on P1.ID_PESSOA = WCD.COD_COMPR -- COMPROMISSÁRIO&#13;&#10;    left join SIS_PESSOA 		   P2			 on P2.ID_PESSOA = WCD.COD_PROP  -- PROPRIETÁRIO      &#13;&#10; &#13;&#10;&#13;&#10;&#13;&#10;&#13;&#10;">
        <Column Name="inscricao" DataType="System.String"/>
        <Column Name="ip_acessado" DataType="System.String"/>
        <Column Name="prop" DataType="System.String"/>
        <Column Name="prefixo" DataType="System.String"/>
        <Column Name="logradouro" DataType="System.String"/>
        <Column Name="num_imovel" DataType="System.String"/>
        <Column Name="bairro" DataType="System.String"/>
        <Column Name="quadra" DataType="System.String"/>
        <Column Name="lote" DataType="System.String"/>
        <Column Name="complem" DataType="System.String"/>
        <Column Name="cep" DataType="System.String"/>
        <Column Name="cidade" DataType="System.String"/>
        <Column Name="estado" DataType="System.String"/>
        <Column Name="cpf_cnpj_prop_banco" DataType="System.String"/>
        <Column Name="cpf_cnpj_compr_banco" DataType="System.String"/>
        <Column Name="tipocertidao" DataType="System.String"/>
        <Column Name="tipocertidaonp" DataType="System.String"/>
        <Column Name="situacao" DataType="System.String"/>
        <CommandParameter Name="inscricao" DataType="22" Expression="[inscricao]"/>
        <CommandParameter Name="referencial" DataType="9" Expression="[referencial]"/>
        <CommandParameter Name="ip_acessado" DataType="22" Expression="[ipacessado]"/>
      </TableDataSource>
    </PostgresDataConnection>
