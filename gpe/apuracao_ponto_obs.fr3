    <TfrxADOQuery Name="SqlFuncionario" UserName="SqlFuncionario" CloseDataSource="True" FieldAliases.Text="id_funcionario=id_funcionario&#13;&#10;matricula=matricula&#13;&#10;nome=nome&#13;&#10;cpf=cpf&#13;&#10;pis=pis&#13;&#10;cargo=cargo&#13;&#10;funcao=funcao&#13;&#10;ds_local_trabalho=ds_local_trabalho&#13;&#10;ds_area_apuracao=ds_area_apuracao&#13;&#10;cp_ano=cp_ano&#13;&#10;cp_mes=cp_mes&#13;&#10;dt_inicial=dt_inicial&#13;&#10;dt_final=dt_final&#13;&#10;id_entidade=id_entidade&#13;&#10;ds_entidade=ds_entidade&#13;&#10;doc_entidade=doc_entidade&#13;&#10;id_horario_contratual=id_horario_contratual&#13;&#10;cd_horario_contratual=cd_horario_contratual&#13;&#10;ds_horario_contratual=ds_horario_contratual&#13;&#10;id_tp_horario_contratual=id_tp_horario_contratual" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT A1.ID_FUNCIONARIO&#13;&#10;     , A1.MATRICULA&#13;&#10;     , A1.NOME&#13;&#10;     , A1.CPF&#13;&#10;     , A1.PIS&#13;&#10;     , A1.CARGO&#13;&#10;     , A1.FUNCAO&#13;&#10;     , A1.DS_LOCAL_TRABALHO&#13;&#10;     , A1.DS_AREA_APURACAO&#13;&#10;     , A1.CP_ANO&#13;&#10;     , A1.CP_MES&#13;&#10;     , A1.DT_INICIAL&#13;&#10;     , A1.DT_FINAL&#13;&#10;     , A1.ID_ENTIDADE&#13;&#10;     , A1.DS_ENTIDADE&#13;&#10;     , A1.DOC_ENTIDADE&#13;&#10;     , A1.ID_HORARIO_CONTRATUAL&#13;&#10;     , B1.CD_HORARIO_CONTRATUAL&#13;&#10;     , B1.DS_HORARIO_CONTRATUAL&#13;&#10;     , B1.ID_TP_HORARIO_CONTRATUAL&#13;&#10;  FROM (&#13;&#10;        SELECT G.ID_FUNCIONARIO&#13;&#10;             , G.MATRICULA_SEQ AS MATRICULA                                    &#13;&#10;             , G.NOME&#13;&#10;             , H.DOCUMENTO_FORMATADO AS CPF&#13;&#10;             , G.PIS&#13;&#10;             , G.CARGO&#13;&#10;             , G.FUNCAO&#13;&#10;             , I.DS_LOCAL_TRABALHO&#13;&#10;             , F.DS_AREA_APURACAO&#13;&#10;             , B.CP_ANO&#13;&#10;             , B.CP_MES&#13;&#10;             , B.DT_INICIAL&#13;&#10;             , B.DT_FINAL&#13;&#10;             , K.ID_ENTIDADE&#13;&#10;             , L.NOME_RAZAO AS DS_ENTIDADE&#13;&#10;             , L.DOCUMENTO_FORMATADO AS DOC_ENTIDADE           &#13;&#10;             , (SELECT XX.ID_HORARIO_CONTRATUAL&#13;&#10;                  FROM (&#13;&#10;                        SELECT FF.ID_HORARIO_CONTRATUAL AS ID_HORARIO_CONTRATUAL&#13;&#10;                             , FF.DT_INICIO_HORARIO&#13;&#10;                          FROM GPE_FUNCIONARIO_HORARIO FF&#13;&#10;                         WHERE 0                     = C.INTEGRADO_RH&#13;&#10;                           AND FF.ID_FUNCIONARIO     = A.ID_FILTRO&#13;&#10;                           AND FF.DT_INICIO_HORARIO &#60;= B.DT_FINAL&#13;&#10;                         UNION&#13;&#10;                        SELECT FF.ID_HORARIO_CONTRATUAL_RH AS ID_HORARIO_CONTRATUAL&#13;&#10;                             , FF.DT_INICIO_HORARIO&#13;&#10;                          FROM GPE_FUNCIONARIO_HORARIO FF&#13;&#10;                         WHERE 1                     = C.INTEGRADO_RH&#13;&#10;                           AND FF.ID_FUNCIONARIO_RH  = A.ID_FILTRO&#13;&#10;                           AND FF.DT_INICIO_HORARIO &#60;= B.DT_FINAL&#13;&#10;                         ORDER BY 2 DESC&#13;&#10;                          ROWS 1&#13;&#10;                       ) XX ) AS ID_HORARIO_CONTRATUAL&#13;&#10;          FROM SIS_RELATORIO_FILTRO A&#13;&#10;               LEFT JOIN GPE_PERIODO_APURACAO                B ON B.ID_PERIODO_APURACAO                            = :ID_PERIODO_APURACAO&#13;&#10;               LEFT JOIN GPE_CONFIGURACAO                    C ON C.ID_CONFIGURACAO                                = 1&#13;&#10;        &#13;&#10;               LEFT JOIN GPE_AREA_FUNCIONARIO                E ON E.ID_AREA_APURACAO                               = B.ID_AREA_APURACAO&#13;&#10;                                                              AND ( (  E.ID_FUNCIONARIO_RH  = A.ID_FILTRO   AND 1  = C.INTEGRADO_RH) OR&#13;&#10;                                                                    (  E.ID_FUNCIONARIO     = A.ID_FILTRO   AND 0 &#60;&#62; C.INTEGRADO_RH)   )&#13;&#10;               LEFT JOIN GPE_AREA_APURACAO                   F ON F.ID_AREA_APURACAO                               = B.ID_AREA_APURACAO&#13;&#10;               LEFT JOIN GPE_FUNCIONARIO_POR_ID(A.ID_FILTRO) G ON 1 = 1&#13;&#10;               LEFT JOIN SIS_FORMATA_DOCUMENTO(2, G.CPF)     H ON 1 = 1&#13;&#10;               LEFT JOIN GPE_LOCAL_TRABALHO_VIEW             I ON I.ID_LOCAL_TRABALHO                              = G.ID_LOCAL_TRABALHO&#13;&#10;               LEFT JOIN SIS_ENTIDADE                        K ON K.ID_ENTIDADE                                    = G.ID_ENTIDADE&#13;&#10;               LEFT JOIN SIS_PESSOA                          L ON L.ID_PESSOA                                      = K.ID_PESSOA_JURIDICA&#13;&#10;         WHERE A.ID_RELATORIO_FILTRO = :ID_RELATORIO_FILTRO                                                        &#13;&#10;       ) A1&#13;&#10;       LEFT JOIN GPE_HORARIO_CONTRATUAL_VIEW B1 ON B1.ID_HORARIO_CONTRATUAL = A1.ID_HORARIO_CONTRATUAL&#13;&#10; ORDER BY&#13;&#10;       A1.NOME&#13;&#10;     , A1.MATRICULA  " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlPonto" UserName="SqlPonto" CloseDataSource="True" FieldAliases.Text="DT_PONTO=DT_PONTO&#13;&#10;DIA_SEMANA=DIA_SEMANA&#13;&#10;ID_APURACAO=ID_APURACAO&#13;&#10;ID_TP_DIA_PONTO=ID_TP_DIA_PONTO&#13;&#10;DS_TP_DIA_PONTO=DS_TP_DIA_PONTO&#13;&#10;APURACAO_CARGA_HORARIA=APURACAO_CARGA_HORARIA&#13;&#10;HORARIO_IS_CARGA_HORARIA=HORARIO_IS_CARGA_HORARIA&#13;&#10;ID_TP_CARGA_HORARIA=ID_TP_CARGA_HORARIA&#13;&#10;APURACAO_DIA_SEGUINTE=APURACAO_DIA_SEGUINTE&#13;&#10;EXIBIR_INTERVALO_PRE_ASSINALADO=EXIBIR_INTERVALO_PRE_ASSINALADO&#13;&#10;OBS=OBS&#13;&#10;ENTRADA_01=ENTRADA_01&#13;&#10;SAIDA_01=SAIDA_01&#13;&#10;ENTRADA_02=ENTRADA_02&#13;&#10;SAIDA_02=SAIDA_02&#13;&#10;ENTRADA_01_DIA=ENTRADA_01_DIA&#13;&#10;SAIDA_01_DIA=SAIDA_01_DIA&#13;&#10;ENTRADA_02_DIA=ENTRADA_02_DIA&#13;&#10;SAIDA_02_DIA=SAIDA_02_DIA&#13;&#10;ID_PONTO_REGISTRADO_ENTRADA_01=ID_PONTO_REGISTRADO_ENTRADA_01&#13;&#10;ID_PONTO_REGISTRADO_SAIDA_01=ID_PONTO_REGISTRADO_SAIDA_01&#13;&#10;ID_PONTO_REGISTRADO_ENTRADA_02=ID_PONTO_REGISTRADO_ENTRADA_02&#13;&#10;ID_PONTO_REGISTRADO_SAIDA_02=ID_PONTO_REGISTRADO_SAIDA_02&#13;&#10;TEM_OUTROS_REGISTROS=TEM_OUTROS_REGISTROS&#13;&#10;HR_NORMAL=HR_NORMAL&#13;&#10;HR_ATRASO=HR_ATRASO&#13;&#10;HR_SAIDA_ANTECIPADA=HR_SAIDA_ANTECIPADA&#13;&#10;HR_FALTA=HR_FALTA&#13;&#10;HR_EXTRA_1=HR_EXTRA_1&#13;&#10;HR_EXTRA_2=HR_EXTRA_2&#13;&#10;HR_EXTRA_3=HR_EXTRA_3&#13;&#10;HR_EXTRA_4=HR_EXTRA_4&#13;&#10;HR_EXTRA_5=HR_EXTRA_5&#13;&#10;HR_EXTRA_6=HR_EXTRA_6&#13;&#10;HR_EXTRA_NAO_AUTORIZADA=HR_EXTRA_NAO_AUTORIZADA&#13;&#10;HR_ADICIONAL_NOTURNO=HR_ADICIONAL_NOTURNO&#13;&#10;ID_APURACAO_USUARIO=ID_APURACAO_USUARIO&#13;&#10;COR=COR&#13;&#10;ISENTO=ISENTO&#13;&#10;ID_FUNCIONARIO_P=ID_FUNCIONARIO_P&#13;&#10;ID_FUNCIONARIO=ID_FUNCIONARIO" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="                 SELECT A3.DT_PONTO&#13;&#10;     , A3.DIA_SEMANA&#13;&#10;     , A3.ID_APURACAO&#13;&#10;     , A3.ID_TP_DIA_PONTO&#13;&#10;&#13;&#10;     , CAST(CASE WHEN A3.ID_TP_DIA_PONTO = 1 AND A3.ISENTO = 1 THEN A3.DS_TP_DIA_PONTO || ' (Isento)'&#13;&#10;                 WHEN A3.ID_TP_DIA_PONTO = 1 AND A3.ID_PONTO_REGISTRADO_ENTRADA_01 IS NULL THEN A3.DS_TP_DIA_PONTO || ' (Falta)'&#13;&#10;                 ELSE A3.DS_TP_DIA_PONTO&#13;&#10;            END&#13;&#10;       AS VARCHAR(120)) AS DS_TP_DIA_PONTO&#13;&#10;&#13;&#10;     , A3.APURACAO_CARGA_HORARIA&#13;&#10;     , A3.HORARIO_IS_CARGA_HORARIA&#13;&#10;     , A3.ID_TP_CARGA_HORARIA&#13;&#10;     , A3.APURACAO_DIA_SEGUINTE&#13;&#10;     , A3.EXIBIR_INTERVALO_PRE_ASSINALADO&#13;&#10;     , A3.OBS&#13;&#10;&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA    = 1  THEN ''&#13;&#10;                 WHEN A3.ENTRADA_01             IS NULL THEN '--:--'&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_01 = 2  THEN '*' || A3.ENTRADA_01   -- INCLUIDO POR DIGITACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_01 = 3  THEN '^' || A3.ENTRADA_01   -- PRE ASSINALADO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_01 = 4  THEN 'A.M.'                 -- AUSENCIA DE MARCACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_01 = 5  THEN 'H.P.'                 -- HORARIO PADRAO&#13;&#10;                 ELSE A3.ENTRADA_01&#13;&#10;             END AS VARCHAR(10)) AS ENTRADA_01&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA    = 1  THEN ''&#13;&#10;                 WHEN A3.SAIDA_01               IS NULL THEN '--:--'&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_01   = 2  THEN '*' || A3.SAIDA_01     -- INCLUIDO POR DIGITACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_01   = 3  THEN '^' || A3.SAIDA_01     -- PRE ASSINALADO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_01   = 4  THEN 'A.M.'                 -- AUSENCIA DE MARCACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_01   = 5  THEN 'H.P.'                 -- HORARIO PADRAO&#13;&#10;                 ELSE A3.SAIDA_01&#13;&#10;             END AS VARCHAR(10)) AS SAIDA_01&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA    = 1  THEN ''&#13;&#10;                 WHEN A3.ENTRADA_02             IS NULL THEN '--:--'&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_02 = 2  THEN '*' || A3.ENTRADA_02   -- INCLUIDO POR DIGITACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_02 = 3  THEN '^' || A3.ENTRADA_02   -- PRE ASSINALADO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_02 = 4  THEN 'A.M.'                 -- AUSENCIA DE MARCACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_ENTRADA_02 = 5  THEN 'H.P.'                 -- HORARIO PADRAO&#13;&#10;                 ELSE A3.ENTRADA_02&#13;&#10;             END AS VARCHAR(10)) AS ENTRADA_02&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA    = 1  THEN ''&#13;&#10;                 WHEN A3.SAIDA_02               IS NULL THEN '--:--'&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_02   = 2  THEN '*' || A3.SAIDA_02     -- INCLUIDO POR DIGITACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_02   = 3  THEN '^' || A3.SAIDA_02     -- PRE ASSINALADO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_02   = 4  THEN 'A.M.'                 -- AUSENCIA DE MARCACAO&#13;&#10;                 WHEN A3.ID_TP_REGISTRO_SAIDA_02   = 5  THEN 'H.P.'                 -- HORARIO PADRAO&#13;&#10;                 ELSE A3.SAIDA_02&#13;&#10;             END AS VARCHAR(10)) AS SAIDA_02&#13;&#10;&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA &#60;&#62; 1 AND A3.ID_TP_REGISTRO_ENTRADA_01 &#60;&#62; 4 AND A3.ID_TP_REGISTRO_ENTRADA_01 &#60;&#62; 5 THEN A3.ENTRADA_01_DIA END AS VARCHAR(5)) AS ENTRADA_01_DIA&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA &#60;&#62; 1 AND A3.ID_TP_REGISTRO_SAIDA_01   &#60;&#62; 4 AND A3.ID_TP_REGISTRO_SAIDA_01   &#60;&#62; 5 THEN A3.SAIDA_01_DIA   END AS VARCHAR(5)) AS SAIDA_01_DIA&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA &#60;&#62; 1 AND A3.ID_TP_REGISTRO_ENTRADA_02 &#60;&#62; 4 AND A3.ID_TP_REGISTRO_ENTRADA_02 &#60;&#62; 5 THEN A3.ENTRADA_02_DIA END AS VARCHAR(5)) AS ENTRADA_02_DIA&#13;&#10;     , CAST(CASE WHEN A3.APURACAO_CARGA_HORARIA &#60;&#62; 1 AND A3.ID_TP_REGISTRO_SAIDA_02   &#60;&#62; 4 AND A3.ID_TP_REGISTRO_SAIDA_02   &#60;&#62; 5 THEN A3.SAIDA_02_DIA   END AS VARCHAR(5)) AS SAIDA_02_DIA&#13;&#10;&#13;&#10;     , A3.ID_PONTO_REGISTRADO_ENTRADA_01&#13;&#10;     , A3.ID_PONTO_REGISTRADO_SAIDA_01&#13;&#10;     , A3.ID_PONTO_REGISTRADO_ENTRADA_02&#13;&#10;     , A3.ID_PONTO_REGISTRADO_SAIDA_02&#13;&#10;     , A3.TEM_OUTROS_REGISTROS&#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_NORMAL, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_NORMAL &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_NORMAL_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_NORMAL_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_NORMAL_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                      AS HR_NORMAL&#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_ATRASO, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_ATRASO &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_ATRASO_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_ATRASO_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_ATRASO_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                      AS HR_ATRASO     &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_SAIDA_ANTECIPADA, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_SAIDA_ANTECIPADA &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_SAIDA_ANTECIPADA_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_SAIDA_ANTECIPADA_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_SAIDA_ANTECIPADA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                            AS HR_SAIDA_ANTECIPADA     &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_FALTA, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_FALTA &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_FALTA_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_FALTA_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_FALTA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                       AS HR_FALTA          &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_EXTRA_1, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_EXTRA_1 &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_1_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXTRA_1_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_1_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                     AS HR_EXTRA_1&#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_EXTRA_2, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_EXTRA_2 &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_2_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXTRA_2_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_2_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                     AS HR_EXTRA_2&#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_EXTRA_3, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_EXTRA_3 &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_3_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXTRA_3_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_3_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                     AS HR_EXTRA_3     &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_EXTRA_4, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_EXTRA_4 &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_4_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXTRA_4_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_4_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                     AS HR_EXTRA_4     &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_EXTRA_5, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_EXTRA_5 &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_5_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXTRA_5_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_5_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                     AS HR_EXTRA_5          &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_EXTRA_6, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_EXTRA_6 &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_6_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXTRA_6_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_6_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                     AS HR_EXTRA_6          &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_EXTRA_NAO_AUTORIZADA, 0) = 0 THEN NULL&#13;&#10;                 WHEN A3.HR_EXTRA_NAO_AUTORIZADA &#60; 0              THEN '*' ELSE '' END ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_NAO_AUTORIZADA_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXTRA_NAO_AUTORIZADA_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_EXTRA_NAO_AUTORIZADA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                        AS HR_EXTRA_NAO_AUTORIZADA          &#13;&#10;&#13;&#10;     , CAST(CASE WHEN COALESCE(A3.HR_ADICIONAL_NOTURNO, 0) = 0 THEN NULL&#13;&#10;                 ELSE LPAD(CAST(A3.HR_ADICIONAL_NOTURNO_H AS VARCHAR(4)),&#13;&#10;                           CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_ADICIONAL_NOTURNO_H AS VARCHAR(4))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;                      LPAD(CAST(A3.HR_ADICIONAL_NOTURNO_M AS VARCHAR(2)), 2, '0') END AS VARCHAR(10))                                       AS HR_ADICIONAL_NOTURNO&#13;&#10;&#13;&#10;     , A3.ID_APURACAO_USUARIO&#13;&#10;     , A3.COR&#13;&#10;     , A3.ISENTO&#13;&#10;     , A3.ID_FUNCIONARIO_P&#13;&#10;     , A3.ID_FUNCIONARIO&#13;&#10;  FROM (&#13;&#10;        SELECT A2.DT_PONTO&#13;&#10;             , CAST(CASE UDF_WEEKDAY(CAST(A2.DT_PONTO AS DATE))&#13;&#10;                         WHEN 0 THEN 'dom'&#13;&#10;                         WHEN 1 THEN 'seg'&#13;&#10;                         WHEN 2 THEN 'ter'&#13;&#10;                         WHEN 3 THEN 'qua'&#13;&#10;                         WHEN 4 THEN 'qui'&#13;&#10;                         WHEN 5 THEN 'sex'&#13;&#10;                         WHEN 6 THEN 'sáb'&#13;&#10;                     END AS VARCHAR(3)) AS DIA_SEMANA&#13;&#10;        &#13;&#10;             , A2.ID_APURACAO&#13;&#10;             , A2.ID_TP_DIA_PONTO&#13;&#10;             , A2.DS_TP_DIA_PONTO&#13;&#10;             , A2.APURACAO_CARGA_HORARIA&#13;&#10;             , A2.HORARIO_IS_CARGA_HORARIA&#13;&#10;             , A2.ID_TP_CARGA_HORARIA&#13;&#10;             , A2.APURACAO_DIA_SEGUINTE&#13;&#10;             , A2.EXIBIR_INTERVALO_PRE_ASSINALADO&#13;&#10;             , A2.OBS&#13;&#10;&#13;&#10;             , CAST(CASE WHEN A2.DT_HR_REGISTRO_ENTRADA_01 IS NOT NULL THEN LPAD(CAST(UDF_HOUR(A2.DT_HR_REGISTRO_ENTRADA_01)   AS VARCHAR(2)), 2, '0') || ':' ||&#13;&#10;                                                                            LPAD(CAST(UDF_MINUTE(A2.DT_HR_REGISTRO_ENTRADA_01) AS VARCHAR(2)), 2, '0') END AS VARCHAR(10) ) AS ENTRADA_01&#13;&#10;             , CAST(CASE WHEN A2.DT_HR_REGISTRO_SAIDA_01   IS NOT NULL THEN LPAD(CAST(UDF_HOUR(A2.DT_HR_REGISTRO_SAIDA_01)     AS VARCHAR(2)), 2, '0') || ':' ||&#13;&#10;                                                                            LPAD(CAST(UDF_MINUTE(A2.DT_HR_REGISTRO_SAIDA_01)   AS VARCHAR(2)), 2, '0') END AS VARCHAR(10) ) AS SAIDA_01&#13;&#10;             , CAST(CASE WHEN A2.DT_HR_REGISTRO_ENTRADA_02 IS NOT NULL THEN LPAD(CAST(UDF_HOUR(A2.DT_HR_REGISTRO_ENTRADA_02)   AS VARCHAR(2)), 2, '0') || ':' ||&#13;&#10;                                                                            LPAD(CAST(UDF_MINUTE(A2.DT_HR_REGISTRO_ENTRADA_02) AS VARCHAR(2)), 2, '0') END AS VARCHAR(10) ) AS ENTRADA_02&#13;&#10;             , CAST(CASE WHEN A2.DT_HR_REGISTRO_SAIDA_02   IS NOT NULL THEN LPAD(CAST(UDF_HOUR(A2.DT_HR_REGISTRO_SAIDA_02)     AS VARCHAR(2)), 2, '0') || ':' ||&#13;&#10;                                                                            LPAD(CAST(UDF_MINUTE(A2.DT_HR_REGISTRO_SAIDA_02)   AS VARCHAR(2)), 2, '0') END AS VARCHAR(10) ) AS SAIDA_02&#13;&#10;&#13;&#10;             , CAST(CASE WHEN A2.DT_HR_REGISTRO_ENTRADA_01 IS NOT NULL AND UDF_ENCDATE(UDF_YEAR(A2.DT_HR_REGISTRO_ENTRADA_01), UDF_MONTH(A2.DT_HR_REGISTRO_ENTRADA_01), UDF_DAY(A2.DT_HR_REGISTRO_ENTRADA_01)) &#60;&#62; A2.DT_PONTO THEN&#13;&#10;                         CAST(CASE WHEN A2.APUR