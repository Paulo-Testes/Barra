;&#13;&#10;  memHE2Carga.Width := memHE2.CalcWidth;&#13;&#10;  memHE3Carga.Width := memHE3.CalcWidth;&#13;&#10;  memHE4Carga.Width := memHE4.CalcWidth;&#13;&#10;  memHE5Carga.Width := memHE5.CalcWidth;&#13;&#10;  memHE6Carga.Width := memHE6.CalcWidth;&#13;&#10;  memObsCarga.Left := 1000;&#13;&#10;&#13;&#10;  bndApuracaoCargaHoraria.Visible := &#60;SqlPonto.&#34;APURACAO_CARGA_HORARIA&#34;&#62; = 1;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndOutrosRegistrosOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bndOutrosRegistros.Visible := &#60;SqlPonto.&#34;TEM_OUTROS_REGISTROS&#34;&#62; = 1;&#13;&#10;  if &#60;ColorirTipoDia&#62; = 1 then&#13;&#10;  begin&#13;&#10;    memDiaOutrosRegistros.Color := &#60;SqlPonto.&#34;COR&#34;&#62;;&#13;&#10;    memDiaOutrosRegistros.Highlight.ApplyFill := False;&#13;&#10;    memTipoDiaOutrosRegistros.Color := &#60;SqlPonto.&#34;COR&#34;&#62;;&#13;&#10;    memTipoDiaOutrosRegistros.Highlight.ApplyFill := False;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndOutrosRegistrosOnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  memOutrosRegistros5.Left := memOutrosRegistros4.Left + memOutrosRegistros4.Width;&#13;&#10;  memOutrosRegistros5.Width := memHE1.CalcWidth;&#13;&#10;&#13;&#10;  memOutrosRegistros6.Left := memOutrosRegistros5.Left + memOutrosRegistros5.Width;&#13;&#10;  memOutrosRegistros6.Width := memHE2.CalcWidth;&#13;&#10;&#13;&#10;  memOutrosRegistros7.Left := memOutrosRegistros6.Left + memOutrosRegistros6.Width;&#13;&#10;  memOutrosRegistros7.Width := memHE3.CalcWidth;&#13;&#10;&#13;&#10;  memOutrosRegistros8.Left := memOutrosRegistros7.Left + memOutrosRegistros7.Width;&#13;&#10;  memOutrosRegistros8.Width := memHE4.CalcWidth;&#13;&#10;&#13;&#10;  memOutrosRegistros9.Left := memOutrosRegistros8.Left + memOutrosRegistros8.Width;&#13;&#10;  memOutrosRegistros9.Width := memHE5.CalcWidth;&#13;&#10;&#13;&#10;  memOutrosRegistros10.Left := memOutrosRegistros9.Left + memOutrosRegistros9.Width;&#13;&#10;  memOutrosRegistros10.Width := memHE6.CalcWidth;&#13;&#10;&#13;&#10;  memOutrosRegistros11.Left := memOutrosRegistros10.Left + memOutrosRegistros10.Width;&#13;&#10;  memOutrosRegistros12.Left := memOutrosRegistros11.Left + memOutrosRegistros11.Width;&#13;&#10;&#13;&#10;  memOutrosRegistros13.Left := memOutrosRegistros12.Left + memOutrosRegistros12.Width;&#13;&#10;  memOutrosRegistros13.Width := bndOutrosRegistros.Width - memOutrosRegistros13.Left;&#13;&#10;&#13;&#10;  memDiaOutrosRegistros.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memTipoDiaOutrosRegistros.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros1.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros2.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros3.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros4.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros5.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros6.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros7.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros8.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros9.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros10.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros11.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros12.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;  memOutrosRegistros13.Height := 10 * (SqlOutrosRegistros.RecordCount div 4);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndSomaPontoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  memHE1Total.Width := memHE1.CalcWidth;&#13;&#10;  memHE2Total.Width := memHE2.CalcWidth;&#13;&#10;  memHE3Total.Width := memHE3.CalcWidth;&#13;&#10;  memHE4Total.Width := memHE4.CalcWidth;&#13;&#10;  memHE5Total.Width := memHE5.CalcWidth;&#13;&#10;  memHE6Total.Width := memHE6.CalcWidth;&#13;&#10;  memObsTotal.Left := 1000;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  if &#60;dbms&#62; = 'PostgreSQL' then&#13;&#10;  begin&#13;&#10;    SqlPonto.SQL.Text := StringReplace(SqlPonto.SQL.Text, 'A.DT_FINAL - A.DT_INICIAL + 1', 'EXTRACT(DAYS FROM A.DT_FINAL - A.DT_INICIAL) + 1');&#13;&#10;    SqlSomaPonto.SQL.Text := StringReplace(SqlSomaPonto.SQL.Text, 'A.DT_FINAL - A.DT_INICIAL + 1', 'EXTRACT(DAYS FROM A.DT_FINAL - A.DT_INICIAL) + 1');&#13;&#10;    SqlOutrosRegistros.SQL.Text := SqlOutrosRegistros.SQL.Text + #13 + 'OFFSET 4';&#13;&#10;  end&#13;&#10;  else&#13;&#10;    SqlOutrosRegistros.SQL.Text := SqlOutrosRegistros.SQL.Text + #13 + 'ROWS 5 TO 1000';&#13;&#10;&#13;&#10;  SqlHE.Open;&#13;&#10;  memHE1.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE1&#34;&#62;;&#13;&#10;  memHE2.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE2&#34;&#62;;&#13;&#10;  memHE3.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE3&#34;&#62;;&#13;&#10;  memHE4.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE4&#34;&#62;;&#13;&#10;  memHE5.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE5&#34;&#62;;&#13;&#10;  memHE6.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE6&#34;&#62;;&#13;&#10;  SqlHE.Close;&#13;&#10;end." StoreInDFM="False">
