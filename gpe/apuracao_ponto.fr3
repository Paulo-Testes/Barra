13;&#10;&#13;&#10;  SqlHE.Open;&#13;&#10;  memHE1.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE1&#34;&#62;;&#13;&#10;  memHE2.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE2&#34;&#62;;&#13;&#10;  memHE3.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE3&#34;&#62;;&#13;&#10;  memHE4.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE4&#34;&#62;;&#13;&#10;  memHE5.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE5&#34;&#62;;&#13;&#10;  memHE6.Memo.Text := &#60;SqlHE.&#34;DS_TP_HE6&#34;&#62;;&#13;&#10;  SqlHE.Close;&#13;&#10;&#13;&#10;  cabJustificativaObservacao.StartNewPage := &#60;ImprimeJustificativa&#62; = 1;&#13;&#10;end." StoreInDFM="False">
