;&#10;                                 THEN COALESCE(D1.HR_SAIDA_ANTECIPADA_PAGAMENTO, B1.HR_SAIDA_ANTECIPADA_PAGAMENTO, 0) ELSE 0 END) AS HR_SAIDA_ANTECIPADA_PAGAMENTO&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_FALTA_PAGAMENTO, B1.HR_FALTA_PAGAMENTO, 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_FALTA_PAGAMENTO, B1.HR_FALTA_PAGAMENTO, 0) ELSE 0 END )                      AS HR_FALTA_PAGAMENTO&#13;&#10;                     &#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_1, B1.HR_EXTRA_1 - COALESCE(B1.HR_EXTRA_1_BANCO, 0), 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_1, B1.HR_EXTRA_1 - COALESCE(B1.HR_EXTRA_1_BANCO, 0), 0) ELSE 0 END )   AS HR_EXTRA_1&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_2, B1.HR_EXTRA_2 - COALESCE(B1.HR_EXTRA_2_BANCO, 0), 0) &#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_2, B1.HR_EXTRA_2 - COALESCE(B1.HR_EXTRA_2_BANCO, 0), 0) ELSE 0 END )   AS HR_EXTRA_2&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_3, B1.HR_EXTRA_3 - COALESCE(B1.HR_EXTRA_3_BANCO, 0), 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_3, B1.HR_EXTRA_3 - COALESCE(B1.HR_EXTRA_3_BANCO, 0), 0) ELSE 0 END )   AS HR_EXTRA_3                     &#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_4, B1.HR_EXTRA_4 - COALESCE(B1.HR_EXTRA_4_BANCO, 0), 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_4, B1.HR_EXTRA_4 - COALESCE(B1.HR_EXTRA_4_BANCO, 0), 0) ELSE 0 END )   AS HR_EXTRA_4                     &#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_5, B1.HR_EXTRA_5 - COALESCE(B1.HR_EXTRA_5_BANCO, 0), 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_5, B1.HR_EXTRA_5 - COALESCE(B1.HR_EXTRA_5_BANCO, 0), 0) ELSE 0 END )   AS HR_EXTRA_5&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_6, B1.HR_EXTRA_6 - COALESCE(B1.HR_EXTRA_6_BANCO, 0), 0)   &#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_6, B1.HR_EXTRA_6 - COALESCE(B1.HR_EXTRA_6_BANCO, 0), 0) ELSE 0 END )   AS HR_EXTRA_6&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_NAO_AUTORIZADA, B1.HR_EXTRA_NAO_AUTORIZADA, 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_EXTRA_NAO_AUTORIZADA, B1.HR_EXTRA_NAO_AUTORIZADA, 0) ELSE 0 END )            AS HR_EXTRA_NAO_AUTORIZADA&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(B1.HR_ADICIONAL_NOTURNO, 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(B1.HR_ADICIONAL_NOTURNO, 0) ELSE 0 END )                                           AS HR_ADICIONAL_NOTURNO&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(B1.HR_ADICIONAL_BANCO, 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(B1.HR_ADICIONAL_BANCO, 0) ELSE 0 END )                                             AS HR_ADICIONAL_BANCO&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(B1.HR_ADICIONAL_BANCO, 0) + CASE WHEN E1.BANCO_HORAS = 1 AND E1.INCLUIR_ADICIONAL_NOTURNO_BH = 1 THEN COALESCE(B1.HR_ADICIONAL_NOTURNO, 0) ELSE 0 END&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(B1.HR_ADICIONAL_BANCO, 0) + CASE WHEN E1.BANCO_HORAS = 1 AND E1.INCLUIR_ADICIONAL_NOTURNO_BH = 1 THEN COALESCE(B1.HR_ADICIONAL_NOTURNO, 0) ELSE 0 END ELSE 0 END) AS HR_ADICIONAL_BANCO_TOTAL&#13;&#10;&#13;&#10;&#13;&#10;                     , SUM( CASE WHEN E1.ID_TP_APURACAO          &#60;&#62; 4&#13;&#10;                                 THEN COALESCE(D1.HR_PAGAMENTO_BANCO, 0)&#13;&#10;                                 WHEN B1.APURACAO_CARGA_HORARIA   = 1&#13;&#10;                                 THEN COALESCE(D1.HR_PAGAMENTO_BANCO, 0) ELSE 0 END )                                             AS HR_PAGAMENTO_BANCO&#13;&#10;        &#13;&#10;                     , SUM( CASE WHEN (E1.ID_TP_APURACAO &#60;&#62; 4) OR (E1.ID_TP_APURACAO = 4 AND B1.APURACAO_CARGA_HORARIA = 1)&#13;&#10;                                 THEN COALESCE(D1.HR_EXCEDENTE, B1.HR_EXCEDENTE, 0) +&#13;&#10;                                      COALESCE(D1.HR_EXCEDENTE_NOTURNA, B1.HR_EXCEDENTE_NOTURNA, 0) +&#13;&#10;                                      COALESCE(D1.HR_NORMAL_COMO_EXC_DOM, B1.HR_NORMAL_COMO_EXC_DOM, 0) +&#13;&#10;                                      COALESCE(D1.HR_NORMAL_COMO_EXC_SAB, B1.HR_NORMAL_COMO_EXC_SAB, 0) +&#13;&#10;                                      COALESCE(D1.HR_NORMAL_COMO_EXC_FER, B1.HR_NORMAL_COMO_EXC_FER, 0) -   &#13;&#10;                                      COALESCE(D1.HR_EXTRA_1, B1.HR_EXTRA_1 - COALESCE(B1.HR_EXTRA_1_BANCO, 0), 0) -&#13;&#10;                                      COALESCE(D1.HR_EXTRA_2, B1.HR_EXTRA_2 - COALESCE(B1.HR_EXTRA_2_BANCO, 0), 0) -&#13;&#10;                                      COALESCE(D1.HR_EXTRA_3, B1.HR_EXTRA_3 - COALESCE(B1.HR_EXTRA_3_BANCO, 0), 0) -&#13;&#10;                                      COALESCE(D1.HR_EXTRA_4, B1.HR_EXTRA_4 - COALESCE(B1.HR_EXTRA_4_BANCO, 0), 0) -&#13;&#10;                                      COALESCE(D1.HR_EXTRA_5, B1.HR_EXTRA_5 - COALESCE(B1.HR_EXTRA_5_BANCO, 0), 0) -&#13;&#10;                                      COALESCE(D1.HR_EXTRA_6, B1.HR_EXTRA_6 - COALESCE(B1.HR_EXTRA_6_BANCO, 0), 0) -&#13;&#10;                                      COALESCE(D1.HR_EXTRA_NAO_AUTORIZADA, B1.HR_EXTRA_NAO_AUTORIZADA, 0) ELSE 0 END )            AS HR_ACRESCIMOS_BH&#13;&#10;        &#13;&#10;                     , SUM( CASE WHEN (E1.ID_TP_APURACAO &#60;&#62; 4) OR (E1.ID_TP_APURACAO = 4 AND B1.APURACAO_CARGA_HORARIA = 1)&#13;&#10;                                 THEN COALESCE(D1.HR_ATRASO, B1.HR_ATRASO, 0) +&#13;&#10;                                      COALESCE(D1.HR_SAIDA_ANTECIPADA, B1.HR_SAIDA_ANTECIPADA, 0) +&#13;&#10;                                      COALESCE(D1.HR_FALTA, B1.HR_FALTA, 0) -&#13;&#10;                                      (&#13;&#10;                                           COALESCE(D1.HR_ATRASO_ABONADO, B1.HR_ATRASO_ABONADO, 0) +&#13;&#10;                                           COALESCE(D1.HR_SAIDA_ANTECIPADA_ABONADA, B1.HR_SAIDA_ANTECIPADA_ABONADA, 0) +&#13;&#10;                                           COALESCE(D1.HR_FALTA_ABONADA, B1.HR_FALTA_ABONADA, 0)                              ) -&#13;&#10;                                      (&#13;&#10;                                            COALESCE(D1.HR_ATRASO_PAGAMENTO, B1.HR_ATRASO_PAGAMENTO, 0) +&#13;&#10;                                            COALESCE(D1.HR_SAIDA_ANTECIPADA_PAGAMENTO, B1.HR_SAIDA_ANTECIPADA_PAGAMENTO, 0) +&#13;&#10;                                            COALESCE(D1.HR_FALTA_PAGAMENTO, B1.HR_FALTA_PAGAMENTO, 0)                         ) ELSE 0 END ) AS HR_DECRESCIMOS_BH&#13;&#10;        &#13;&#10;                  FROM (&#13;&#10;                        SELECT A.ID_PERIODO_APURACAO&#13;&#10;                             , A.DT_INICIAL&#13;&#10;                             , A.DT_FINAL&#13;&#10;                             , UDF_INCDAY(A.DT_INICIAL, (B.ID_SEQUENCIA -1)) AS DT_PONTO                             &#13;&#10;                             , C.INTEGRADO_RH&#13;&#10;                          FROM GPE_PERIODO_APURACAO A&#13;&#10;                               LEFT JOIN SIS_SEQUENCIA    B ON B.ID_SEQUENCIA &#60;= A.DT_FINAL - A.DT_INICIAL + 1&#13;&#10;                               LEFT JOIN GPE_CONFIGURACAO C ON C.ID_CONFIGURACAO = 1&#13;&#10;                         WHERE A.ID_PERIODO_APURACAO = :ID_PERIODO_APURACAO                                      &#13;&#10;                       ) A1                                                                              &#13;&#10;                       LEFT JOIN GPE_APURACAO                   X1 ON X1.ID_PERIODO_APURACAO    = A1.ID_PERIODO_APURACAO&#13;&#10;                                                                  AND X1.DT_PONTO               = A1.DT_PONTO&#13;&#10;                                                                  AND X1.ID_FUNCIONARIO         = :ID_FUNCIONARIO&#13;&#10;                                                                  AND 1                        &#60;&#62; A1.INTEGRADO_RH&#13;&#10;                       LEFT JOIN GPE_APURACAO                   X2 ON X2.ID_PERIODO_APURACAO    = A1.ID_PERIODO_APURACAO&#13;&#10;                                                                  AND X2.DT_PONTO               = A1.DT_PONTO&#13;&#10;                                                                  AND X2.ID_FUNCIONARIO_RH      = :ID_FUNCIONARIO&#13;&#10;                                                                  AND 1                         = A1.INTEGRADO_RH&#13;&#10;                       LEFT JOIN GPE_APURACAO                   B1 ON B1.ID_APURACAO            = COALESCE(X1.ID_APURACAO, X2.ID_APURACAO)&#13;&#10;                &#13;&#10;                       LEFT JOIN GPE_APURACAO_USUARIO           Z1 ON Z1.ID_FUNCIONARIO         = COALESCE(B1.ID_FUNCIONARIO, B1.ID_FUNCIONARIO_RH)&#13;&#10;                                                                  AND Z1.DT_PONTO               = B1.DT_PONTO&#13;&#10;                                                                  AND Z1.APURACAO_CARGA_HORARIA = B1.APURACAO_CARGA_HORARIA&#13;&#10;                                                                  AND 1                        &#60;&#62; A1.INTEGRADO_RH&#13;&#10;                       LEFT JOIN GPE_APURACAO_USUARIO           Z2 ON Z2.ID_FUNCIONARIO_RH      = COALESCE(B1.ID_FUNCIONARIO, B1.ID_FUNCIONARIO_RH)&#13;&#10;                                                                  AND Z2.DT_PONTO               = B1.DT_PONTO&#13;&#10;                                                                  AND Z2.APURACAO_CARGA_HORARIA = B1.APURACAO_CARGA_HORARIA&#13;&#10;                                                                  AND 1                         = A1.INTEGRADO_RH&#13;&#10;                       LEFT JOIN GPE_APURACAO_USUARIO           D1 ON D1.ID_APURACAO_USUARIO    = COALESCE(Z1.ID_APURACAO_USUARIO, Z2.ID_APURACAO_USUARIO)&#13;&#10;                                                                  AND B1.ID_TP_DIA_PONTO       &#60;&#62; 9                  &#13;&#10;                  &#13;&#10;                       LEFT JOIN GPE_APONTAMENTO                E1 ON E1.ID_APONTAMENTO         = B1.ID_APONTAMENTO&#13;&#10;                       LEFT JOIN GPE_HORARIO                    H1 ON H1.ID_HORARIO             = B1.ID_HORARIO&#13;&#10;&#13;&#10;               ) A2&#13;&#10;&#13;&#10;       ) A3&#13;&#10;&#13;&#10;   ROWS 1                            " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlHE" UserName="SqlHE" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT MAX(CAST(CASE WHEN A.ID_TP_HE = 1 THEN COALESCE(A.DESCRICAO, A.DS_TP_HE) END AS VARCHAR(50))) AS DS_TP_HE1&#13;&#10;     , MAX(CAST(CASE WHEN A.ID_TP_HE = 2 THEN COALESCE(A.DESCRICAO, A.DS_TP_HE) END AS VARCHAR(50))) AS DS_TP_HE2&#13;&#10;     , MAX(CAST(CASE WHEN A.ID_TP_HE = 3 THEN COALESCE(A.DESCRICAO, A.DS_TP_HE) END AS VARCHAR(50))) AS DS_TP_HE3&#13;&#10;     , MAX(CAST(CASE WHEN A.ID_TP_HE = 4 THEN COALESCE(A.DESCRICAO, A.DS_TP_HE) END AS VARCHAR(50))) AS DS_TP_HE4&#13;&#10;     , MAX(CAST(CASE WHEN A.ID_TP_HE = 5 THEN COALESCE(A.DESCRICAO, A.DS_TP_HE) END AS VARCHAR(50))) AS DS_TP_HE5&#13;&#10;     , MAX(CAST(CASE WHEN A.ID_TP_HE = 6 THEN COALESCE(A.DESCRICAO, A.DS_TP_HE) END AS VARCHAR(50))) AS DS_TP_HE6&#13;&#10;  FROM GPE_TP_HE A  " CommandTimeout="30" LockType="ltReadOnly" PropData="05704C65667403BA000470546F7003FD000A506172616D65746572730100"/>
    <TfrxADOQuery Name="SqlJustificativaObservacao" UserName="SqlJustificativaObservacao" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT B1.DT_PONTO&#13;&#10;     , CAST(CASE UDF_WEEKDAY(CAST(B1.DT_PONTO AS DATE))&#13;&#10;                 WHEN 0 THEN 'dom'&#13;&#10;                 WHEN 1 THEN 'seg'&#13;&#10;                 WHEN 2 THEN 'ter'&#13;&#10;                 WHEN 3 THEN 'qua'&#13;&#10;                 WHEN 4 THEN 'qui'&#13;&#10;                 WHEN 5 THEN 'sex'&#13;&#10;                 WHEN 6 THEN 'sáb'&#13;&#10;             END AS VARCHAR(3)) AS DIA_SEMANA&#13;&#10;&#13;&#10;     , CAST(CASE WHEN B1.OBS_TP_DIA_PONTO IS NOT NULL&#13;&#10;                 THEN B1.OBS_TP_DIA_PONTO || CASE WHEN COALESCE(D1.OBS_FALTA_ATRASO_SAIDA_ANTECIP, B1.OBS_FALTA_ATRASO_SAIDA_ANTECIP) IS NOT NULL THEN ' | ' ELSE '' END&#13;&#10;                 ELSE '' END ||&#13;&#10;            CASE WHEN COALESCE(D1.OBS_FALTA_ATRASO_SAIDA_ANTECIP, B1.OBS_FALTA_ATRASO_SAIDA_ANTECIP) IS NOT NULL&#13;&#10;                 THEN COALESCE(D1.OBS_FALTA_ATRASO_SAIDA_ANTECIP, B1.OBS_FALTA_ATRASO_SAIDA_ANTECIP) || CASE WHEN D1.OBS IS NOT NULL THEN ' | ' ELSE '' END&#13;&#10;                 ELSE '' END ||&#13;&#10;            CASE WHEN D1.OBS IS NOT NULL&#13;&#10;                 THEN D1.OBS&#13;&#10;                 ELSE '' END  AS BLOB SUB_TYPE 1) AS OBS&#13;&#10;     , B1.OBS_TP_DIA_PONTO&#13;&#10;     , B1.OBS_FALTA_ATRASO_SAIDA_ANTECIP&#13;&#10;     , CAST(:ID_FUNCIONARIO AS INTEGER) AS ID_FUNCIONARIO                                                                                                                                 &#13;&#10;  FROM (&#13;&#10;        SELECT A.ID_PERIODO_APURACAO&#13;&#10;             , C.INTEGRADO_RH&#13;&#10;          FROM GPE_PERIODO_APURACAO A&#13;&#10;               LEFT JOIN GPE_CONFIGURACAO C ON C.ID_CONFIGURACAO = 1&#13;&#10;         WHERE A.ID_PERIODO_APURACAO = :ID_PERIODO_APURACAO&#13;&#10;       ) A1                                                                              &#13;&#10;       LEFT JOIN GPE_APURACAO           X1 ON X1.ID_PERIODO_APURACAO    = A1.ID_PERIODO_APURACAO&#13;&#10;                                          AND X1.ID_FUNCIONARIO         = :ID_FUNCIONARIO&#13;&#10;                                          AND 1                        &#60;&#62; A1.INTEGRADO_RH&#13;&#10;       LEFT JOIN GPE_APURACAO           X2 ON X2.ID_PERIODO_APURACAO    = A1.ID_PERIODO_APURACAO&#13;&#10;                                          AND X2.ID_FUNCIONARIO_RH      = :ID_FUNCIONARIO&#13;&#10;                                          AND 1                         = A1.INTEGRADO_RH&#13;&#10;       LEFT JOIN GPE_APURACAO           B1 ON B1.ID_APURACAO            = COALESCE(X1.ID_APURACAO, X2.ID_APURACAO)&#13;&#10;&#13;&#10;       LEFT JOIN GPE_APURACAO_USUARIO   Z1 ON Z1.ID_FUNCIONARIO         = COALESCE(B1.ID_FUNCIONARIO, B1.ID_FUNCIONARIO_RH)&#13;&#10;                                          AND Z1.DT_PONTO               = B1.DT_PONTO&#13;&#10;                                          AND Z1.APURACAO_CARGA_HORARIA = B1.APURACAO_CARGA_HORARIA&#13;&#10;                                          AND 1                        &#60;&#62; A1.INTEGRADO_RH&#13;&#10;       LEFT JOIN GPE_APURACAO_USUARIO   Z2 ON Z2.ID_FUNCIONARIO_RH      = COALESCE(B1.ID_FUNCIONARIO, B1.ID_FUNCIONARIO_RH)&#13;&#10;                                          AND Z2.DT_PONTO               = B1.DT_PONTO&#13;&#10;                                          AND Z2.APURACAO_CARGA_HORARIA = B1.APURACAO_CARGA_HORARIA&#13;&#10;                                          AND 1                         = A1.INTEGRADO_RH&#13;&#10;       LEFT JOIN GPE_APURACAO_USUARIO   D1 ON D1.ID_APURACAO_USUARIO    = COALESCE(Z1.ID_APURACAO_USUARIO, Z2.ID_APURACAO_USUARIO)&#13;&#10;                                          AND B1.ID_TP_DIA_PONTO       &#60;&#62; 9                  &#13;&#10;  &#13;&#10; WHERE CAST(NULLIF(TRIM(CASE WHEN B1.OBS_TP_DIA_PONTO IS NOT NULL&#13;&#10;                             THEN B1.OBS_TP_DIA_PONTO || CASE WHEN COALESCE(D1.OBS_FALTA_ATRASO_SAIDA_ANTECIP, B1.OBS_FALTA_ATRASO_SAIDA_ANTECIP) IS NOT NULL THEN ' | ' ELSE '' END&#13;&#10;                             ELSE '' END ||&#13;&#10;                        CASE WHEN COALESCE(D1.OBS_FALTA_ATRASO_SAIDA_ANTECIP, B1.OBS_FALTA_ATRASO_SAIDA_ANTECIP) IS NOT NULL&#13;&#10;                             THEN COALESCE(D1.OBS_FALTA_ATRASO_SAIDA_ANTECIP, B1.OBS_FALTA_ATRASO_SAIDA_ANTECIP) || CASE WHEN D1.OBS IS NOT NULL THEN ' | ' ELSE '' END&#13;&#10;                             ELSE '' END ||&#13;&#10;                        CASE WHEN D1.OBS IS NOT NULL&#13;&#10;                             THEN D1.OBS&#13;&#10;                             ELSE '' END), '') AS BLOB SUB_TYPE 1) IS NOT NULL&#13;&#10;ORDER BY&#13;&#10;      B1.DT_PONTO  " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlAM" UserName="SqlAM" CloseDataSource="True" FieldAliases.Text="QTD_AM=QTD_AM" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT COUNT(1) AS QTD_AM&#13;&#10;  FROM (SELECT A.ID_PONTO_REGISTRADO&#13;&#10;        FROM GPE_PONTO_REGISTRADO A&#13;&#10;        LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;       WHERE A.ID_FUNCIONARIO = :ID_FUNCIONARIO&#13;&#10;         AND A.ID_PERIODO_APURACAO  = :ID_PERIODO_APURACAO&#13;&#10;         AND A.ID_TP_MARCACAO &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;         AND A.ID_TP_REGISTRO  = 4 -- AM&#13;&#10;         AND B.INTEGRADO_RH   = 0&#13;&#10;&#13;&#10;       UNION&#13;&#10;&#13;&#10;      SELECT A.ID_PONTO_REGISTRADO&#13;&#10;        FROM GPE_PONTO_REGISTRADO A&#13;&#10;        LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;       WHERE A.ID_FUNCIONARIO_RH = :ID_FUNCIONARIO&#13;&#10;         AND A.ID_PERIODO_APURACAO  = :ID_PERIODO_APURACAO&#13;&#10;         AND A.ID_TP_MARCACAO &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;         AND A.ID_TP_REGISTRO  = 4 -- AM&#13;&#10;         AND B.INTEGRADO_RH      = 1&#13;&#10;) A1    " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlHP" UserName="SqlHP" CloseDataSource="True" FieldAliases.Text="QTD_HP=QTD_HP" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT COUNT(1) AS QTD_HP&#13;&#10;  FROM (SELECT A.ID_PONTO_REGISTRADO&#13;&#10;        FROM GPE_PONTO_REGISTRADO A&#13;&#10;        LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;       WHERE A.ID_FUNCIONARIO = :ID_FUNCIONARIO&#13;&#10;         AND A.ID_PERIODO_APURACAO  = :ID_PERIODO_APURACAO&#13;&#10;         AND A.ID_TP_MARCACAO &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;         AND A.ID_TP_REGISTRO  = 5 -- HP&#13;&#10;         AND B.INTEGRADO_RH   = 0&#13;&#10;&#13;&#10;       UNION&#13;&#10;&#13;&#10;      SELECT A.ID_PONTO_REGISTRADO&#13;&#10;        FROM GPE_PONTO_REGISTRADO A&#13;&#10;        LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;       WHERE A.ID_FUNCIONARIO_RH = :ID_FUNCIONARIO&#13;&#10;         AND A.ID_PERIODO_APURACAO  = :ID_PERIODO_APURACAO&#13;&#10;         AND A.ID_TP_MARCACAO &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;         AND A.ID_TP_REGISTRO  = 5 -- HP&#13;&#10;         AND B.INTEGRADO_RH      = 1&#13;&#10;) A1    " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
