) AS INTEGER) AS D03_CARGA_HORARIA&#13;&#10;             , CAST(DIV(D04.CARGA_HORARIA, 60) AS INTEGER) AS D04_CARGA_HORARIA&#13;&#10;             , CAST(DIV(D05.CARGA_HORARIA, 60) AS INTEGER) AS D05_CARGA_HORARIA&#13;&#10;             , CAST(DIV(D06.CARGA_HORARIA, 60) AS INTEGER) AS D06_CARGA_HORARIA&#13;&#10;             , CAST(DIV(D07.CARGA_HORARIA, 60) AS INTEGER) AS D07_CARGA_HORARIA&#13;&#10;&#13;&#10;             , CAST(MOD(D01.CARGA_HORARIA, 60) AS INTEGER) AS D01_CARGA_HORARIA_MIN&#13;&#10;             , CAST(MOD(D02.CARGA_HORARIA, 60) AS INTEGER) AS D02_CARGA_HORARIA_MIN&#13;&#10;             , CAST(MOD(D03.CARGA_HORARIA, 60) AS INTEGER) AS D03_CARGA_HORARIA_MIN&#13;&#10;             , CAST(MOD(D04.CARGA_HORARIA, 60) AS INTEGER) AS D04_CARGA_HORARIA_MIN&#13;&#10;             , CAST(MOD(D05.CARGA_HORARIA, 60) AS INTEGER) AS D05_CARGA_HORARIA_MIN&#13;&#10;             , CAST(MOD(D06.CARGA_HORARIA, 60) AS INTEGER) AS D06_CARGA_HORARIA_MIN&#13;&#10;             , CAST(MOD(D07.CARGA_HORARIA, 60) AS INTEGER) AS D07_CARGA_HORARIA_MIN&#13;&#10;&#13;&#10;             , TCG.ID_TP_CARGA_HORARIA&#13;&#10;             , TCG.DS_TP_CARGA_HORARIA&#13;&#10;          FROM GPE_HORARIO_CONTRATUAL_VIEW  HOR&#13;&#10;               LEFT JOIN GPE_HORARIO            D01 ON D01.ID_HORARIO           = HOR.ID_HORARIO_01&#13;&#10;               LEFT JOIN GPE_HORARIO            D02 ON D02.ID_HORARIO           = HOR.ID_HORARIO_02&#13;&#10;               LEFT JOIN GPE_HORARIO            D03 ON D03.ID_HORARIO           = HOR.ID_HORARIO_03&#13;&#10;               LEFT JOIN GPE_HORARIO            D04 ON D04.ID_HORARIO           = HOR.ID_HORARIO_04&#13;&#10;               LEFT JOIN GPE_HORARIO            D05 ON D05.ID_HORARIO           = HOR.ID_HORARIO_05&#13;&#10;               LEFT JOIN GPE_HORARIO            D06 ON D06.ID_HORARIO           = HOR.ID_HORARIO_06&#13;&#10;               LEFT JOIN GPE_HORARIO            D07 ON D07.ID_HORARIO           = HOR.ID_HORARIO_07&#13;&#10;               LEFT JOIN GPE_TP_CARGA_HORARIA   TCG ON TCG.ID_TP_CARGA_HORARIA  = D01.ID_TP_CARGA_HORARIA&#13;&#10;         WHERE HOR.ID_HORARIO_CONTRATUAL = :ID_HORARIO_CONTRATUAL&#13;&#10;       ) AA1, SIS_SEQUENCIA SEQ&#13;&#10; WHERE SEQ.ID_SEQUENCIA &#60;= CASE WHEN AA1.ID_TP_HORARIO_CONTRATUAL = 1 THEN 7&#13;&#10;                                WHEN AA1.ID_TP_HORARIO_CONTRATUAL = 2 THEN 4&#13;&#10;                                WHEN AA1.ID_TP_HORARIO_CONTRATUAL = 3 THEN 1&#13;&#10;                           ELSE 0 END&#13;&#10; ORDER BY&#13;&#10;       SEQ.ID_SEQUENCIA    " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlOutrosRegistros" UserName="SqlOutrosRegistros" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="True" SQL.Text="SELECT A1.DT_HR_REGISTRO&#13;&#10;     , CAST(CASE WHEN A1.ID_TP_REGISTRO = 2  THEN '*' || A1.HR_REGISTRO   -- INCLUIDO POR DIGITACAO&#13;&#10;                 WHEN A1.ID_TP_REGISTRO = 3  THEN '^' || A1.HR_REGISTRO   -- PRE ASSINALADO&#13;&#10;                 WHEN A1.ID_TP_REGISTRO = 4  THEN 'A.M.'                 -- AUSENCIA DE MARCACAO&#13;&#10;                 WHEN A1.ID_TP_REGISTRO = 5  THEN 'H.P.'                 -- HORARIO PADRAO&#13;&#10;                 ELSE A1.HR_REGISTRO&#13;&#10;        END AS VARCHAR(10)) AS HR_REGISTRO&#13;&#10;&#13;&#10;      , CAST(CASE WHEN A1.DT_HR_REGISTRO IS NOT NULL AND UDF_ENCDATE(UDF_YEAR(A1.DT_HR_REGISTRO), UDF_MONTH(A1.DT_HR_REGISTRO), UDF_DAY(A1.DT_HR_REGISTRO)) &#60;&#62; :DT_PONTO THEN&#13;&#10;                  CAST(CASE WHEN :APURACAO_DIA_SEGUINTE = 1 THEN '-' ELSE '+' END AS VARCHAR(1)) ||&#13;&#10;                  CAST(UDF_DAYSBETWEEN(UDF_ENCDATE(UDF_YEAR(A1.DT_HR_REGISTRO), UDF_MONTH(A1.DT_HR_REGISTRO), UDF_DAY(A1.DT_HR_REGISTRO)), :DT_PONTO) AS VARCHAR(10))&#13;&#10;             END AS VARCHAR(10)) AS HR_REGISTRO_DIA&#13;&#10;&#13;&#10;  FROM (&#13;&#10;        SELECT A.DT_HR_REGISTRO&#13;&#10;             , CAST(LPAD(CAST(UDF_HOUR(A.DT_HR_REGISTRO)   AS VARCHAR(2)), 2, '0') || ':' ||&#13;&#10;                    LPAD(CAST(UDF_MINUTE(A.DT_HR_REGISTRO) AS VARCHAR(2)), 2, '0') AS VARCHAR(5)) AS HR_REGISTRO&#13;&#10;             , A.ID_TP_REGISTRO&#13;&#10;          FROM GPE_PONTO_REGISTRADO A&#13;&#10;               LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;         WHERE A.ID_FUNCIONARIO = :ID_FUNCIONARIO&#13;&#10;           AND A.DT_REFERENCIA  = :DT_PONTO&#13;&#10;           AND A.ID_TP_MARCACAO &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;           AND ((CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 0 AND A.ID_TP_REGISTRO &#60;&#62; 3) -- PRE-ASSINALADO&#13;&#10;            OR   CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 1)&#13;&#10;           AND B.INTEGRADO_RH   = 0&#13;&#10;         UNION&#13;&#10;        SELECT A.DT_HR_REGISTRO&#13;&#10;             , CAST(LPAD(CAST(UDF_HOUR(A.DT_HR_REGISTRO)   AS VARCHAR(2)), 2, '0') || ':' ||&#13;&#10;                    LPAD(CAST(UDF_MINUTE(A.DT_HR_REGISTRO) AS VARCHAR(2)), 2, '0') AS VARCHAR(5)) AS HR_REGISTRO&#13;&#10;             , A.ID_TP_REGISTRO&#13;&#10;          FROM GPE_PONTO_REGISTRADO A&#13;&#10;               LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;         WHERE A.ID_FUNCIONARIO_RH = :ID_FUNCIONARIO&#13;&#10;           AND A.DT_REFERENCIA     = :DT_PONTO&#13;&#10;           AND A.ID_TP_MARCACAO   &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;           AND ((CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 0 AND A.ID_TP_REGISTRO &#60;&#62; 3) -- PRE-ASSINALADO&#13;&#10;            OR   CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 1)&#13;&#10;           AND B.INTEGRADO_RH      = 1&#13;&#10;         ORDER BY&#13;&#10;               1&#13;&#10;       ) A1&#13;&#10; UNION ALL&#13;&#10;/* SEMPRE RETORNAR MULTIPLOS DE 4 */&#13;&#10;SELECT UDF_ENCDATE(2100, 12, 31) AS DT_HR_REGISTRO&#13;&#10;     , CAST('' AS VARCHAR(10) )  AS HR_REGISTRO&#13;&#10;     , CAST(NULL AS VARCHAR(2) ) AS HR_REGISTRO_DIA&#13;&#10;  FROM SIS_SEQUENCIA S&#13;&#10; WHERE S.ID_SEQUENCIA &#60;= 4 - ( SELECT CASE WHEN MOD(SUM(QT.QTDE), 4) = 0 THEN 4 ELSE MOD(SUM(QT.QTDE), 4) END&#13;&#10;                                 FROM (&#13;&#10;                                        SELECT COUNT(1) AS QTDE&#13;&#10;                                          FROM GPE_PONTO_REGISTRADO A&#13;&#10;                                               LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;                                         WHERE A.ID_FUNCIONARIO = :ID_FUNCIONARIO&#13;&#10;                                           AND A.DT_REFERENCIA  = :DT_PONTO&#13;&#10;                                           AND A.ID_TP_MARCACAO &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;                                           AND ((CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 0 AND A.ID_TP_REGISTRO &#60;&#62; 3) -- PRE-ASSINALADO&#13;&#10;                                            OR   CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 1)&#13;&#10;                                           AND B.INTEGRADO_RH   = 0&#13;&#10;                                         UNION&#13;&#10;                                        SELECT COUNT(1) AS QTDE&#13;&#10;                                          FROM GPE_PONTO_REGISTRADO A&#13;&#10;                                               LEFT JOIN GPE_CONFIGURACAO B ON B.ID_CONFIGURACAO = 1&#13;&#10;                                         WHERE A.ID_FUNCIONARIO_RH = :ID_FUNCIONARIO&#13;&#10;                                           AND A.DT_REFERENCIA     = :DT_PONTO&#13;&#10;                                           AND A.ID_TP_MARCACAO   &#60;&#62; 3 -- DESCONSIDERADO&#13;&#10;                                           AND ((CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 0 AND A.ID_TP_REGISTRO &#60;&#62; 3) -- PRE-ASSINALADO&#13;&#10;                                            OR   CAST(:EXIBIR_INTERVALO_PRE_ASSINALADO AS SMALLINT) = 1)&#13;&#10;                                           AND B.INTEGRADO_RH      = 1&#13;&#10;                                      ) QT&#13;&#10;                             )&#13;&#10; ORDER BY 1      " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
    <TfrxADOQuery Name="SqlSomaPonto" UserName="SqlSomaPonto" CloseDataSource="True" FieldAliases.Text="" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT CAST(LPAD(CAST(A3.HR_NORMAL_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_NORMAL_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_NORMAL_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                      AS HR_NORMAL&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_NOTURNA_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_NOTURNA_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_NOTURNA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                     AS HR_NOTURNA&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_ATRASO_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_ATRASO_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_ATRASO_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                      AS HR_ATRASO&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_SAIDA_ANTECIPADA_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_SAIDA_ANTECIPADA_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_SAIDA_ANTECIPADA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                            AS HR_SAIDA_ANTECIPADA&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_FALTA_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_FALTA_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_FALTA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                       AS HR_FALTA&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_EXCEDENTE_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXCEDENTE_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_EXCEDENTE_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                                   AS HR_EXCEDENTE&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_EXCEDENTE_NOTURNA_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_EXCEDENTE_NOTURNA_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_EXCEDENTE_NOTURNA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                           AS HR_EXCEDENTE_NOTURNA&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_NORMAL_EXC_TURNO_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_NORMAL_EXC_TURNO_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_NORMAL_EXC_TURNO_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                            AS HR_NORMAL_EXC_TURNO&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_ATRASO_ABONADO_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_ATRASO_ABONADO_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_ATRASO_ABONADO_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                              AS HR_ATRASO_ABONADO&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_SAIDA_ANTECIPADA_ABONADA_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_SAIDA_ANTECIPADA_ABONADA_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_SAIDA_ANTECIPADA_ABONADA_M AS VARCHAR(2)), 2, '0') AS VARCHAR(10))                                    AS HR_SAIDA_ANTECIPADA_ABONADA&#13;&#10;&#13;&#10;     , CAST(LPAD(CAST(A3.HR_FALTA_ABONADA_H AS VARCHAR(6)),&#13;&#10;                 CAST(UDF_MAX(UDF_LEN(CAST(A3.HR_FALTA_ABONADA_H AS VARCHAR(6))), 2) AS INTEGER), '0') || ':' ||&#13;&#10;            LPAD(CAST(A3.HR_FALTA_ABONADA_M AS VARCHAR(2)), 2, '0') AS VARCHA