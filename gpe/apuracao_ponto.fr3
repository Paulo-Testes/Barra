    <TfrxADOQuery Name="SqlFuncionario" UserName="SqlFuncionario" CloseDataSource="True" FieldAliases.Text="id_funcionario=id_funcionario&#13;&#10;matricula=matricula&#13;&#10;nome=nome&#13;&#10;cpf=cpf&#13;&#10;pis=pis&#13;&#10;cargo=cargo&#13;&#10;funcao=funcao&#13;&#10;ds_local_trabalho=ds_local_trabalho&#13;&#10;ds_area_apuracao=ds_area_apuracao&#13;&#10;cp_ano=cp_ano&#13;&#10;cp_mes=cp_mes&#13;&#10;dt_inicial=dt_inicial&#13;&#10;dt_final=dt_final&#13;&#10;id_entidade=id_entidade&#13;&#10;ds_entidade=ds_entidade&#13;&#10;doc_entidade=doc_entidade&#13;&#10;id_horario_contratual=id_horario_contratual&#13;&#10;cd_horario_contratual=cd_horario_contratual&#13;&#10;ds_horario_contratual=ds_horario_contratual&#13;&#10;id_tp_horario_contratual=id_tp_horario_contratual" BCDToCurrency="False" IgnoreDupParams="False" SQL.Text="SELECT A1.ID_FUNCIONARIO&#13;&#10;     , A1.MATRICULA&#13;&#10;     , A1.NOME&#13;&#10;     , A1.CPF&#13;&#10;     , A1.PIS&#13;&#10;     , A1.CARGO&#13;&#10;     , A1.FUNCAO&#13;&#10;     , A1.DS_LOCAL_TRABALHO&#13;&#10;     , A1.DS_AREA_APURACAO&#13;&#10;     , A1.CP_ANO&#13;&#10;     , A1.CP_MES&#13;&#10;     , A1.DT_INICIAL&#13;&#10;     , A1.DT_FINAL&#13;&#10;     , A1.ID_ENTIDADE&#13;&#10;     , A1.DS_ENTIDADE&#13;&#10;     , A1.DOC_ENTIDADE&#13;&#10;     , A1.ID_HORARIO_CONTRATUAL&#13;&#10;     , B1.CD_HORARIO_CONTRATUAL&#13;&#10;     , B1.DS_HORARIO_CONTRATUAL&#13;&#10;     , B1.ID_TP_HORARIO_CONTRATUAL&#13;&#10;  FROM (&#13;&#10;        SELECT G.ID_FUNCIONARIO&#13;&#10;             , G.MATRICULA_SEQ AS MATRICULA                                               &#13;&#10;             , G.NOME&#13;&#10;             , H.DOCUMENTO_FORMATADO AS CPF&#13;&#10;             , G.PIS&#13;&#10;             , G.CARGO&#13;&#10;             , G.FUNCAO&#13;&#10;             , I.DS_LOCAL_TRABALHO&#13;&#10;             , F.DS_AREA_APURACAO&#13;&#10;             , B.CP_ANO&#13;&#10;             , B.CP_MES&#13;&#10;             , B.DT_INICIAL&#13;&#10;             , B.DT_FINAL&#13;&#10;             , K.ID_ENTIDADE&#13;&#10;             , L.NOME_RAZAO AS DS_ENTIDADE&#13;&#10;             , L.DOCUMENTO_FORMATADO AS DOC_ENTIDADE           &#13;&#10;             , (SELECT XX.ID_HORARIO_CONTRATUAL&#13;&#10;                  FROM (&#13;&#10;                        SELECT FF.ID_HORARIO_CONTRATUAL AS ID_HORARIO_CONTRATUAL&#13;&#10;                             , FF.DT_INICIO_HORARIO&#13;&#10;                          FROM GPE_FUNCIONARIO_HORARIO FF&#13;&#10;                         WHERE 0                     = C.INTEGRADO_RH&#13;&#10;                           AND FF.ID_FUNCIONARIO     = A.ID_FILTRO&#13;&#10;                           AND FF.DT_INICIO_HORARIO &#60;= B.DT_FINAL&#13;&#10;                         UNION&#13;&#10;                        SELECT FF.ID_HORARIO_CONTRATUAL_RH AS ID_HORARIO_CONTRATUAL&#13;&#10;                             , FF.DT_INICIO_HORARIO&#13;&#10;                          FROM GPE_FUNCIONARIO_HORARIO FF&#13;&#10;                         WHERE 1                     = C.INTEGRADO_RH&#13;&#10;                           AND FF.ID_FUNCIONARIO_RH  = A.ID_FILTRO&#13;&#10;                           AND FF.DT_INICIO_HORARIO &#60;= B.DT_FINAL&#13;&#10;                         ORDER BY 2 DESC&#13;&#10;                          ROWS 1&#13;&#10;                       ) XX ) AS ID_HORARIO_CONTRATUAL&#13;&#10;          FROM SIS_RELATORIO_FILTRO A&#13;&#10;               LEFT JOIN GPE_PERIODO_APURACAO                B ON B.ID_PERIODO_APURACAO                            = :ID_PERIODO_APURACAO&#13;&#10;               LEFT JOIN GPE_CONFIGURACAO                    C ON C.ID_CONFIGURACAO                                = 1&#13;&#10;        &#13;&#10;               LEFT JOIN GPE_AREA_FUNCIONARIO                E ON E.ID_AREA_APURACAO                               = B.ID_AREA_APURACAO&#13;&#10;                                                              AND ( (  E.ID_FUNCIONARIO_RH  = A.ID_FILTRO   AND 1  = C.INTEGRADO_RH) OR&#13;&#10;                                                                    (  E.ID_FUNCIONARIO     = A.ID_FILTRO   AND 0 &#60;&#62; C.INTEGRADO_RH)   )&#13;&#10;               LEFT JOIN GPE_AREA_APURACAO                   F ON F.ID_AREA_APURACAO                               = B.ID_AREA_APURACAO&#13;&#10;               LEFT JOIN GPE_FUNCIONARIO_POR_ID(A.ID_FILTRO) G ON 1 = 1&#13;&#10;               LEFT JOIN SIS_FORMATA_DOCUMENTO(2, G.CPF)     H ON 1 = 1&#13;&#10;               LEFT JOIN GPE_LOCAL_TRABALHO_VIEW             I ON I.ID_LOCAL_TRABALHO                              = G.ID_LOCAL_TRABALHO&#13;&#10;               LEFT JOIN SIS_ENTIDADE                        K ON K.ID_ENTIDADE                                    = G.ID_ENTIDADE&#13;&#10;               LEFT JOIN SIS_PESSOA                          L ON L.ID_PESSOA                                      = K.ID_PESSOA_JURIDICA&#13;&#10;         WHERE A.ID_RELATORIO_FILTRO = :ID_RELATORIO_FILTRO                                                        &#13;&#10;       ) A1&#13;&#10;       LEFT JOIN GPE_HORARIO_CONTRATUAL_VIEW B1 ON B1.ID_HORARIO_CONTRATUAL = A1.ID_HORARIO_CONTRATUAL&#13;&#10; ORDER BY&#13;&#10;       A1.NOME&#13;&#10;     , A1.MATRICULA  " CommandTimeout="30" LockType="ltReadOnly" PropData=""/>
