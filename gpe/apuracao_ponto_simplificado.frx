      <TableDataSource Name="Table" Alias="SqlFuncionario" DataType="System.Int32" Enabled="true" SelectCommand="SELECT A1.ID_FUNCIONARIO&#13;&#10;     , A1.MATRICULA&#13;&#10;     , A1.NOME&#13;&#10;     , A1.CPF&#13;&#10;     , A1.PIS&#13;&#10;     , A1.CARGO&#13;&#10;     , A1.FUNCAO&#13;&#10;     , A1.DS_LOCAL_TRABALHO&#13;&#10;     , A1.DS_AREA_APURACAO&#13;&#10;     , A1.CP_ANO&#13;&#10;     , A1.CP_MES&#13;&#10;     , A1.DT_INICIAL&#13;&#10;     , A1.DT_FINAL&#13;&#10;     , A1.ID_ENTIDADE&#13;&#10;     , A1.DS_ENTIDADE&#13;&#10;     , A1.DOC_ENTIDADE&#13;&#10;     , A1.ID_HORARIO_CONTRATUAL&#13;&#10;     , B1.CD_HORARIO_CONTRATUAL&#13;&#10;     , B1.DS_HORARIO_CONTRATUAL&#13;&#10;     , B1.ID_TP_HORARIO_CONTRATUAL&#13;&#10;  FROM (&#13;&#10;        SELECT G.ID_FUNCIONARIO&#13;&#10;             , G.MATRICULA_SEQ AS MATRICULA                                       &#13;&#10;             , G.NOME&#13;&#10;             , H.DOCUMENTO_FORMATADO AS CPF&#13;&#10;             , G.PIS&#13;&#10;             , G.CARGO&#13;&#10;             , G.FUNCAO&#13;&#10;             , I.DS_LOCAL_TRABALHO&#13;&#10;             , F.DS_AREA_APURACAO&#13;&#10;             , B.CP_ANO&#13;&#10;             , B.CP_MES&#13;&#10;             , B.DT_INICIAL&#13;&#10;             , B.DT_FINAL&#13;&#10;             , K.ID_ENTIDADE&#13;&#10;             , L.NOME_RAZAO AS DS_ENTIDADE&#13;&#10;             , L.DOCUMENTO_FORMATADO AS DOC_ENTIDADE           &#13;&#10;             , (SELECT XX.ID_HORARIO_CONTRATUAL&#13;&#10;                  FROM (&#13;&#10;                        SELECT FF.ID_HORARIO_CONTRATUAL AS ID_HORARIO_CONTRATUAL&#13;&#10;                             , FF.DT_INICIO_HORARIO&#13;&#10;                          FROM GPE_FUNCIONARIO_HORARIO FF&#13;&#10;                         WHERE 0                     = C.INTEGRADO_RH&#13;&#10;                           AND FF.ID_FUNCIONARIO     = A.ID_FILTRO&#13;&#10;                           AND FF.DT_INICIO_HORARIO &lt;= B.DT_FINAL&#13;&#10