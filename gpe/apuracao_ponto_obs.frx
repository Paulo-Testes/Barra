      SqlAM.Parameters.FindByName(&quot;id_funcionario&quot;).Value = Report.GetColumnValue(&quot;SqlFuncionario.id_funcionario&quot;);  
      
      (SqlAM as DataSourceBase).Init();
     
      // Acessa as fontes de dados SqlHP
