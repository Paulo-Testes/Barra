<inherited ParentReport="Retrato" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1535" ReportOptions.Author="geoSIAP" ReportOptions.Name="Relatório de Frequência" ReportOptions.LastChange="45754,4426838889" ReportOptions.VersionBuild="0" ReportOptions.VersionMajor="1" ReportOptions.VersionMinor="5" ReportOptions.VersionRelease="1" ScriptText.Text="procedure mdtFuncionarioOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SqlPonto.Close;&#13;&#10;  SqlPonto.ParamByName('id_funcionario').Value := &#60;SqlFuncionario.&#34;ID_FUNCIONARIO&#34;&#62;;&#13;&#10;  SqlPonto.Open;&#13;&#10;&#13;&#10;  SqlAM.Close;&#13;&#10;  SqlAM.ParamByName('id_funcionario').Value := &#60;SqlFuncionario.&#34;ID_FUNCIONARIO&#34;&#62;;&#13;&#10;  SqlAM.Open;&#13;&#10;&#13;&#10;  SqlHP.Close;&#13;&#10;  SqlHP.ParamByName('id_funcionario').Value := &#60;SqlFuncionario.&#34;ID_FUNCIONARIO&#34;&#62;;&#13;&#10;  SqlHP.Open;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cabHorarioOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cabHorario.Visible := (&#60;SqlHorarioContratual.&#34;ID_TP_HORARIO_CONTRATUAL&#34;&#62; in [1, 2]);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure cabCargaHorariaOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  cabCargaHoraria.Visible := &#60;SqlHorarioContratual.&#34;ID_TP_HORARIO_CONTRATUAL&#34;&#62; = 3;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure mdtHorarioContratualOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  mdtHorarioContratual.Visible := (&#60;SqlHorarioContratual.&#34;ID_TP_HORARIO_CONTRATUAL&#34;&#62; in [1, 2]);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndHorarioContratualCargaHorariaOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bndHorarioContratualCargaHoraria.Visible := &#60;SqlHorarioContratual.&#34;ID_TP_HORARIO_CONTRATUAL&#34;&#62; = 3;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ftrHorarioOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  ftrHorario.Visible := (&#60;SqlHorarioContratual.&#34;ID_TP_HORARIO_CONTRATUAL&#34;&#62; in [1, 2]);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure ftrCargaHorariaOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  ftrCargaHoraria.Visible := &#60;SqlHorarioContratual.&#34;ID_TP_HORARIO_CONTRATUAL&#34;&#62; = 3;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndPontoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  if &#60;SqlPonto.&#34;TEM_OUTROS_REGISTROS&#34;&#62; = 1 then&#13;&#10;  begin&#13;&#10;    SqlOutrosRegistros.Close;&#13;&#10;    SqlOutrosRegistros.ParamByName('id_funcionario').Value := &#60;SqlFuncionario.&#34;ID_FUNCIONARIO&#34;&#62;;&#13;&#10;    SqlOutrosRegistros.ParamByName('dt_ponto').Value := &#60;SqlPonto.&#34;DT_PONTO&#34;&#62;;&#13;&#10;    SqlOutrosRegistros.Open;&#13;&#10;  end;&#13;&#10;&#13;&#10;  Set('LineBndPonto', &#60;Line&#62;);&#13;&#10;&#13;&#10;  if &#60;ColorirTipoDia&#62; = 1 then&#13;&#10;  begin&#13;&#10;    SqlPontoDT_PONTO.Color := &#60;SqlPonto.&#34;COR&#34;&#62;;&#13;&#10;    SqlPontoDT_PONTO.Highlight.ApplyFill := False;&#13;&#10;    SqlPontoDS_TP_DIA_PONTO.Color := &#60;SqlPonto.&#34;COR&#34;&#62;;&#13;&#10;    SqlPontoDS_TP_DIA_PONTO.Highlight.ApplyFill := False;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndOutrosRegistrosOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  bndOutrosRegistros.Visible := (&#60;SqlPonto.&#34;TEM_OUTROS_REGISTROS&#34;&#62; = 1) and (not SqlOutrosRegistros.IsEmpty) and (&#60;SqlOutrosRegistros.&#34;HR_REGISTRO&#34;&#62; &#60;&#62; '');&#13;&#10;&#13;&#10;  if &#60;ColorirTipoDia&#62; = 1 then&#13;&#10;  begin&#13;&#10;    memDiaOutrosRegistros.Color := &#60;SqlPonto.&#34;COR&#34;&#62;;&#13;&#10;    memDiaOutrosRegistros.Highlight.ApplyFill := False;&#13;&#10;    memTipoDiaOutrosRegistros.Color := &#60;SqlPonto.&#34;COR&#34;&#62;;&#13;&#10;    memTipoDiaOutrosRegistros.Highlight.ApplyFill := False;&#13;&#10;  end;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndOutrosRegistrosOnAfterCalcHeight(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  memDiaOutrosRegistros.Height := 13 * (SqlOutrosRegistros.RecordCount div 8);&#13;&#10;  memTipoDiaOutrosRegistros.Height := 13 * (SqlOutrosRegistros.RecordCount div 8);&#13;&#10;end;&#13;&#10;&#13;&#10;procedure JustificativaObservacaoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  TfrxBand(Sender).Visible := &#60;ImprimeJustificativa&#62; = 1;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  if &#60;dbms&#62; = 'PostgreSQL' then&#13;&#10;  begin&#13;&#10;    SqlPonto.SQL.Text := StringReplace(SqlPonto.SQL.Text, 'A.DT_FINAL - A.DT_INICIAL + 1', 'EXTRACT(DAYS FROM A.DT_FINAL - A.DT_INICIAL) + 1');&#13;&#10;    SqlOutrosRegistros.SQL.Text := SqlOutrosRegistros.SQL.Text + #13 + 'OFFSET 4';&#13;&#10;  end&#13;&#10;  else&#13;&#10;    SqlOutrosRegistros.SQL.Text := SqlOutrosRegistros.SQL.Text + #13 + 'ROWS 5 TO 1000';&#13;&#10;end." StoreInDFM="False">
