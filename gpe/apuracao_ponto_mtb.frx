      <TableDataSource Name="Table" Alias="SqlFuncionario" DataType="System.Int32" Enabled="true" SelectCommand="SELECT A1.ID_FUNCIONARIO&#13;&#10;     , A1.MATRICULA&#13;&#10;     , A1.NOME&#13;&#10;     , A1.CPF&#13;&#10;     , A1.PIS&#13;&#10;     , A1.CARGO&#13;&#10;     , A1.FUNCAO&#13;&#10;     , A1.DT_ADMISSAO&#13;&#10;     , A1.DS_AREA_APURACAO&#13;&#10;     , A1.CP_ANO&#13;&#10;     , A1.CP_MES&#13;&#10;     , A1.DT_INICIAL&#13;&#10;     , A1.DT_FINAL&#13;&#10;     , A1.ID_ENTIDADE&#13;&#10;     , A1.DS_ENTIDADE&#13;&#10;     , A1.DOC_ENTIDADE&#13;&#10;     , CASE &#13;&#10;    WHEN A1.ID_ENDERECO IS NOT NULL THEN&#13;&#10;         UPPER(COALESCE(A1.DS_TP_LOGRADOURO, '')) ||&#13;&#10;         COALESCE(' '  || A1.DS_LOGRADOURO, '')   ||&#13;&#10;         COALESCE(', ' || A1.NR_ENDERECO, '')     ||&#13;&#10;         COALESCE(', ' || A1.COMPLEMENTO, '')     ||&#13;&#10;         COALESCE(', ' || A1.BAIRRO, '')          ||&#13;&#10;         COALESCE(' - '|| A1.DS_MUNICIPIO, '')    ||&#13;&#10;         COALESCE('/'  || A1.UF, '')&#13;&#10;    ELSE ''&#13;&#10;  END AS ENDERECO_ENTIDADE&#13;&#10;  FROM (&#13;&#10;        SELECT G.ID_FUNCIONARIO&#13;&#10;             , G.MATRICULA&#13;&#10;             , G.NOME&#13;&#10;             , H.DOCUMENTO_FORMATADO AS CPF&#13;&#10;             , G.PIS&#13;&#10;             , G.CARGO&#13;&#10;             , G.FUNCAO&#13;&#10;             , G.DT_ADMISSAO&#13;&#10;             , F.DS_AREA_APURACAO&#13;&#10;             , B.CP_ANO&#13;&#10;             , B.CP_MES&#13;&#10;             , B.DT_INICIAL&#13;&#10;             , B.DT_FINAL&#13;&#10;             , K.ID_ENTIDADE&#13;&#10;             , M.NOME_RAZAO AS DS_ENTIDADE&#13;&#10;             , M.DOCUMENTO_FORMATADO AS DOC_ENTIDADE&#13;&#10;             , M.ID_ENDERECO&#13;&#10;             , M.DS_TP_LOGRADOURO&#13;&#10;             , M.DS_LOGRADOURO&#13;&#10;             , M.NR_ENDERECO&#13;&#10;             , M.COMPLEMENTO&#13;&#10;             , M.BAIRRO&#13;&#10;          