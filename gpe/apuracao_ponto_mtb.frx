    // Evento bndPonto.BeforePrint
   private void bndPonto_BeforePrint(object sender, EventArgs e)
    {
      DateTime dataAtual = Convert.ToDateTime(Report.GetColumnValue(&quot;SqlPonto.DT_PONTO&quot;));
      int ordemAtual = Convert.ToInt32(Report.GetColumnValue(&quot;SqlPonto.ORDEM&quot;));

      bool repetir = (Dia == dataAtual) &amp;&amp; (Ordem == ordemAtual);
      Color corFonte = repetir ? Color.White : SystemColors.WindowText;

      Dia = dataAtual;
      Ordem = ordemAtual;
    }
    
