<inherited ParentReport="Retrato" PreviewOptions.AllowEdit="False" PreviewOptions.Buttons="1535" ReportOptions.Author="geoSIAP" ReportOptions.Name="Relatório Espelho de Ponto Eletrônico" ReportOptions.LastChange="45754,4415713542" ReportOptions.VersionBuild="0" ReportOptions.VersionMajor="1" ReportOptions.VersionMinor="5" ReportOptions.VersionRelease="1" ScriptText.Text="var&#13;&#10;  Dia: TDateTime;&#13;&#10;  Ordem: Integer;&#13;&#10;&#13;&#10;procedure mdtFuncionarioOnBeforePrint(Sender: TfrxComponent);&#13;&#10;begin&#13;&#10;  SqlHorarioContratual.Close;&#13;&#10;  SqlHorarioContratual.ParamByName('id_funcionario').Value := &#60;SqlFuncionario.&#34;ID_FUNCIONARIO&#34;&#62;;&#13;&#10;  SqlHorarioContratual.Open;&#13;&#10;&#13;&#10;  SqlPonto.Close;&#13;&#10;  SqlPonto.ParamByName('id_funcionario').Value := &#60;SqlFuncionario.&#34;ID_FUNCIONARIO&#34;&#62;;&#13;&#10;  SqlPonto.Open;&#13;&#10;&#13;&#10;  grpHorarioContratual.Visible := not SqlPonto.IsEmpty;&#13;&#10;  bndHorarioContratual.Visible := not SqlPonto.IsEmpty;&#13;&#10;  grfHorarioContratual.Visible := not SqlPonto.IsEmpty;&#13;&#10;&#13;&#10;  bndSemRegistro.Visible := SqlPonto.IsEmpty;&#13;&#10;end;&#13;&#10;&#13;&#10;procedure bndPontoOnBeforePrint(Sender: TfrxComponent);&#13;&#10;var&#13;&#10;  Visivel: Boolean;&#13;&#10;begin&#13;&#10;  if ((Dia = &#60;SqlPonto.&#34;DT_PONTO&#34;&#62;) and (Ordem = &#60;SqlPonto.&#34;ORDEM&#34;&#62;)) then&#13;&#10;  begin&#13;&#10;    SqlPontoDT_PONTO.Font.Color := clWhite;&#13;&#10;    SqlPontoMARCACOES.Font.Color := clWhite;&#13;&#10;    SqlPontoENTRADA_01.Font.Color := clWhite;&#13;&#10;    SqlPontoSAIDA_01.Font.Color := clWhite;&#13;&#10;    SqlPontoENTRADA_02.Font.Color := clWhite;&#13;&#10;    SqlPontoSAIDA_02.Font.Color := clWhite;&#13;&#10;    SqlPontoENTRADA_03.Font.Color := clWhite;&#13;&#10;    SqlPontoSAIDA_03.Font.Color := clWhite;&#13;&#10;    SqlPontoENTRADA_04.Font.Color := clWhite;&#13;&#10;    SqlPontoSAIDA_04.Font.Color := clWhite;&#13;&#10;    SqlPontoID_HORARIO.Font.Color := clWhite;&#13;&#10;  end&#13;&#10;  else&#13;&#10;  begin&#13;&#10;    SqlPontoDT_PONTO.Font.Color := clWindowText;&#13;&#10;    SqlPontoMARCACOES.Font.Color := clWindowText;&#13;&#10;    SqlPontoENTRADA_01.Font.Color := clWindowText;&#13;&#10;    SqlPontoSAIDA_01.Font.Color := clWindowText;&#13;&#10;    SqlPontoENTRADA_02.Font.Color := clWindowText;&#13;&#10;    SqlPontoSAIDA_02.Font.Color := clWindowText;&#13;&#10;    SqlPontoENTRADA_03.Font.Color := clWindowText;&#13;&#10;    SqlPontoSAIDA_03.Font.Color := clWindowText;&#13;&#10;    SqlPontoENTRADA_04.Font.Color := clWindowText;&#13;&#10;    SqlPontoSAIDA_04.Font.Color := clWindowText;&#13;&#10;    SqlPontoID_HORARIO.Font.Color := clWindowText;&#13;&#10;  end;&#13;&#10;&#13;&#10;  Dia := &#60;SqlPonto.&#34;DT_PONTO&#34;&#62;;&#13;&#10;  Ordem := &#60;SqlPonto.&#34;ORDEM&#34;&#62;;&#13;&#10;end;&#13;&#10;&#13;&#10;begin&#13;&#10;  if &#60;dbms&#62; = 'PostgreSQL' then&#13;&#10;    SqlPonto.SQL.Text := StringReplace(SqlPonto.SQL.Text, 'A.DT_FINAL - A.DT_INICIAL + 1', 'EXTRACT(DAYS FROM A.DT_FINAL - A.DT_INICIAL) + 1');&#13;&#10;&#13;&#10;  Dia := EncodeDate(1900, 1, 1);&#13;&#10;  Ordem := -1;&#13;&#10;end." StoreInDFM="False">
