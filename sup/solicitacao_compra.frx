      <TableDataSource Name="Table4" Alias="SqlDotacao" DataType="System.Int32" Enabled="true" SelectCommand="SELECT cast(1                 as integer          ) as comdotacao&#13;&#10;     , cast('01|'&#13;&#10;            || lpad(coalesce(ares.nr_reserva              ,99999999) ,10, 'x') ||'|'&#13;&#10;            || lpad(coalesce(appe.nr_pedido_pre_empenho   ,99999999), 10, 'x') ||'|'&#13;&#10;            || lpad(coalesce(adot.nr_ficha_despesa        ,0       ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_un_executora         ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_funcao               ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_subfuncao            ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_programa             ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_acao                 ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_elemento             ,'0'     ) ,20, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_fonte_recurso        ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_fixo                 ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_variavel             ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_ficha_contas_a_pagar ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_centro_custo         ,'0'     ) ,30, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.cd_tipo_despesa         ,'0'     ) ,10, '0') ||'|'&#13;&#10;            || lpad(coalesce(adot.id_responsavel          , 0      ) ,10, '0')  as varchar(250) ) as chave&#13;&#10;     , scif.id_compra_item_ficha&#13;&#10;     , appe.nr_pedido_pre_empenho                             as nr_pedido_reserva&#13;&#10;     , appe.vl_documento                                      as vl_pedido_reserva&#13;&#10;     , ares.nr_reserva&#13;&#10;     , ares.vl_inicial                                        as vl_reserva&#13;&#10;     , adot.nr_ficha_despesa&#13;&#10;     , cast(   trim(adot.cd_un_executora  ) ||' | '&#13;&#10;            || trim(adot.cd_funcao        ) ||'.'&#13;&#10;            || trim(adot.cd_subfuncao     ) ||'.'&#13;&#10;            || trim(adot.cd_programa      ) ||'.'&#13;&#10;            || trim(adot.cd_acao          ) ||' | '&#13;&#10;            || trim(adot.cd_elemento      ) ||' | '&#13;&#10;            || trim(adot.cd_fonte_recurso ) ||' | '&#13;&#10;            || trim(adot.cd_fixo          ) ||'.'&#13;&#10;            || trim(adot.cd_variavel      )  as varchar(100) ) as dotacao&#13;&#10;     , adot.cd_elemento&#13;&#10;     , adot.ds_elemento&#13;&#10;     , adot.cd_centro_custo&#13;&#10;     , adot.ds_centro_custo&#13;&#10;     , adot.cd_tipo_despesa&#13;&#10;     , adot.ds_tipo_despesa&#13;&#10;     , adot.responsavel&#13;&#10;&#13;&#10;     , scit.ordem&#13;&#10;     , scit.id_compra_item&#13;&#10;&#13;&#10;     , scit.id_material&#13;&#10;     , scit.id_grupo_servico&#13;&#10;     , sccp.nr_solicitacao_compra&#13;&#10;     , sccp.tp_solicitacao_compra&#13;&#10;&#13;&#10;     , smat.tp_material&#13;&#10;     , sgmt.cd_grupo                as cd_subgrupo_material&#13;&#10;     , grmt.cd_grupo                as cd_grupo_material&#13;&#10;     , smat.cd_material&#13;&#10;     , smat.ds_material&#13;&#10;     , smat.un_material&#13;&#10;     , smat.imprime_ds_completa&#13;&#10;     , smat.ds_completa&#13;&#10;&#13;&#10;     , grsv.cd_grupo_servico        as cd_grupo_servico&#13;&#10;     , grsv.ds_grupo_servico        as ds_grupo_servico&#13;&#10;     , sgsv.cd_grupo_servico        as cd_subgrupo_servico&#13;&#10;     , sgsv.ds_grupo_servico        as ds_subgrupo_servico&#13;&#10;     , scit.un_servico&#13;&#10;&#13;&#10;     , scit.especificacao&#13;&#10;     , scit.local_entrega&#13;&#10;&#13;&#10;     , scit.quantitativo&#13;&#10;     , scif.qtd&#13;&#10;     , scit.vl_unitario&#13;&#10;     , coalesce(scif.valor, round(scit.vl_unitario * scif.qtd, 2))     as vl_total&#13;&#10;     , case when scit.st_compra_item = 2 then 0 else 1 end                    as dot_liberado_dot_recusado&#13;&#10;     , CAST(COALESCE(APE.DESEMBOLSO, SPR.DESEMBOLSO) AS SMALLINT) AS DESEMBOLSO&#13;&#10;     , APE.CD_TP_EMPENHO&#13;&#10;     , adot.cd_un_orcamentaria&#13;&#10;     , adot.ds_un_orcamentaria&#13;&#10;     , adot.cd_un_executora&#13;&#10;     , adot.ds_un_executora&#13;&#10;     , cast(   trim(adot.cd_funcao        ) ||'.'&#13;&#10;            || trim(adot.cd_subfuncao     ) ||'.'&#13;&#10;            || trim(adot.cd_programa      ) ||'.'&#13;&#10;            || trim(adot.cd_acao          ) ||' - '&#13;&#10;            || trim(adot.ds_acao          ) as varchar(255) ) as funcional_prog&#13;&#10;     , adot.cd_ficha_despesa&#13;&#10;     , adot.ds_ficha_despesa&#13;&#10;     , cast( adot.nr_ficha_contas_a_pagar || ' - ' || adot.cd_ficha_contas_a_pagar || ' - ' || adot.ds_ficha_contas_a_pagar as varchar(255)) contas_a_pagar&#13;&#10;     , cast(adot.cd_fonte_recurso || ' - ' || adot.DS_fonte_recurso as varchar(100)) fonte_recurso&#13;&#10;     , cast(adot.cd_fixo || '.' || adot.cd_variavel as varchar(100)) aplicacao&#13;&#10;     , spr.saldo_disponivel&#13;&#10;     , ape.vl_documento&#13;&#10;     , spr.saldo_disponivel - ape.vl_documento saldo_seguinte&#13;&#10;     , cast(case&#13;&#10;        when ape.id_st_pedido in (1,5) then 'Solicitado'&#13;&#10;        when ape.id_st_pedido in (2,3) then 'Cancelado'&#13;&#10;        else 'Aprovado' end as varchar(50)) st_pedido&#13;&#10;  from sup_compra_item scit&#13;&#10;       left join sup_compra_item_ficha          scif on scif.id_compra_item         = scit.id_compra_item&#13;&#10;       left join sup_material                   smat on smat.id_material            = scit.id_material&#13;&#10;       left join sup_grupo                      sgmt on sgmt.id_grupo               = smat.id_grupo&#13;&#10;       left join sup_grupo                      grmt on grmt.id_grupo               = sgmt.id_grupo_pai&#13;&#10;       left join sup_grupo_servico              sgsv on sgsv.id_grupo_servico       = scit.id_grupo_servico&#13;&#10;       left join sup_grupo_servico              grsv on grsv.id_grupo_servico       = sgsv.id_grupo_servico_pai&#13;&#10;       left join sup_solicitacao_compra         sccp on sccp.id_solicitacao_compra  = scit.id_solicitacao_compra&#13;&#10;&#13;&#10;       left join api_pedido_pre_empenho_por_id(scif.id_pedido_reserva)  appe on 1 = 1&#13;&#10;       left join api_reserva_por_id(scif.id_reserva, null)              ares on 1 = 1&#13;&#10;       left join api_dotacao_por_id(scif.id_conta_funcional_1, scif.id_conta, scif.id_aplicacao, scif.id_centro_custo, scif.id_tipo_despesa, scif.id_responsavel) adot on 1 = 1&#13;&#10;       left join api_pedido_pre_empenho ape on ape.id_tp_pedido = 1 and (ape.id_pedido_pre_empenho = scif.id_pedido_reserva or ape.id_pre_empenho = scif.id_reserva)&#13;&#10;       left join sup_pedido_reserva spr on spr.id_pedido_reserva = coalesce(scif.id_pedido_reserva, ape.id_pedido_pre_empenho)&#13;&#10;       &#13;&#10;&#13;&#10; where scit.id_solicitacao_compra = :id_solicitacao_compra&#13;&#10;   and cast(:tipo_de_impressao as integer) &gt;= 2                                      &#13;&#10;   and scit.tp_item               in (0, 3)&#13;&#10;   and scif.id_compra_item_ficha is not null  -- com dotacao definida&#13;&#10;   -- 00 aberto                                                                                                                     -- nao entra&#13;&#10;   -- 01 excluído                                                                                                                   -- nao entra&#13;&#10;   -- 02 liberado para dotação     ( aprovado na aprovação financeira ou na aprovação da sc quando não usa aprov. finac.  )         -- entra&#13;&#10;   -- 03 não liberado para dotação ( recusado na aprovação financeira ou na aprovação da sc quando não usa aprov. finac.  )         -- nao entra&#13;&#10;&#13;&#10;   -- 08 liberado para autorização de compra     ( aprovado na tela de dotação quando usa a tela de autorização da compra )         -- entra&#13;&#10;   -- 09 não liberado para autorização de compra ( recusado na tela de dotação quando usa a tela de autorização da compra )         -- nao entra&#13;&#10;&#13;&#10;   -- 10 recusado na definição de comprador      ( aprovado na tela de liberação de compra e recusado na definição de comprador )   -- entra&#13;&#10;&#13;&#10;   -- 04 liberado para compra      ( aprovado na tela de dotação não usando tela de autorização da compra e definição de comprador) -- entra&#13;&#10;   --                                aprovado na tela de autorização da compra quando não usa definição de compradores            ) -- entra&#13;&#10;   --                                aprovado na tela de definição de comprador                                                   ) -- entra&#13;&#10;&#13;&#10;   -- 05 não liberado para compra  ( recusado na tela de dotação não usando tela de autorização da compra e definição de comprador) -- nao entra&#13;&#10;   --                                recusado na tela de autorização da compra não usando definição de compradores                ) -- entra&#13;&#10;   --                                recusado na tela de definição de comprador                                                   ) -- entra&#13;&#10;&#13;&#10;   -- 06 recusado                  ( recusado na tela de itens para compra                                                        ) -- entra&#13;&#10;   -- 07 em processo               ( selecionado na tela de itens para compra para compor um processo de compra                   ) -- entra&#13;&#10;   and (    ( scit.st_compra_item        in (2, 8, 10, 4, 6, 7)     )&#13;&#10;&#13;&#10;         or ( scit.st_compra_item = 5                         and&#13;&#10;              (select coalesce(spar.utiliza_autcompra,0)&#13;&#10;                    + coalesce(spar.utiliza_comp     ,0)&#13;&#10;                 from sup_parametro spar&#13;&#10;                 limit 1                                  ) &gt; 0      )   )&#13;&#10;&#13;&#10;  union all&#13;&#10;&#13;&#10;select cast(2                 as integer          ) as comdotacao&#13;&#10;     , cast('02'              as varchar(250)     ) as chave&#13;&#10;     , cast(null              as integer          ) as id_compra_item_ficha&#13;&#10;     , cast(null              as integer          ) as nr_pedido_reserva&#13;&#10;     , cast(null              as double precision ) as vl_pedido_reserva&#13;&#10;     , cast(null              as integer          ) as nr_reserva&#13;&#10;     , cast(null              as double precision ) as vl_reserva&#13;&#10;     , cast(null              as integer          ) as nr_ficha_despesa&#13;&#10;     , cast('não informado'   as varchar(100)     ) as dotacao&#13;&#10;     , cast(null              as varchar(250)     ) as cd_elemento&#13;&#10;     , cast(null              as varchar(250)     ) as ds_elemento&#13;&#10;     , cast(null              as varchar(250)     ) as cd_centro_custo&#13;&#10;     , cast(null              as varchar(250)     ) as ds_centro_custo&#13;&#10;     , cast(null              as varchar(250)     ) as cd_tipo_despesa&#13;&#10;     , cast(null              as varchar(250)     ) as ds_tipo_despesa&#13;&#10;     , cast(null              as varchar(250)     ) as responsavel&#13;&#10;     , scit.ordem&#13;&#10;     , scit.id_compra_item&#13;&#10;&#13;&#10;     , scit.id_material&#13;&#10;     , scit.id_grupo_servico&#13;&#10;     , sccp.nr_solicitacao_compra&#13;&#10;     , sccp.tp_solicitacao_compra&#13;&#10;&#13;&#10;     , smat.tp_material&#13;&#10;     , sgmt.cd_grupo                as cd_subgrupo_material&#13;&#10;     , grmt.cd_grupo                as cd_grupo_material&#13;&#10;     , smat.cd_material&#13;&#10;     , smat.ds_material&#13;&#10; 