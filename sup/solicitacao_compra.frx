         = sccp.id_contrato&#13;&#10;       left join gcc_tp_categoria           gctc on gctc.id_tp_contrato         = gcnt.id_tp_contrato&#13;&#10;                                                and gctc.id_tp_categoria        = gcnt.id_tp_categoria&#13;&#10;       left join gcc_tp_contrato            gccn on gccn.id_tp_contrato         = gcnt.id_tp_contrato                                                    &#13;&#10;       --left join sup_fornecedor             sfor on sfor.id_fornecedor          = sccp.id_fornecedor&#13;&#10;       left join sis_pessoa                 pess on pess.id_pessoa              = sccp.id_fornecedor --sfor.id_credor&#13;&#10;      -- left join sup_fornecedor             fres on fres.id_fornecedor          = sccp.id_fornecedor_reserva&#13;&#10;       left join sis_pessoa                 pres on pres.id_pessoa              = sccp.id_fornecedor_reserva --fres.id_credor&#13;&#10;       left join sup_modalidade             smod on smod.id_modalidade          = sprc.id_modalidade&#13;&#10;       left join sup_tp_solicitacao_compra  stsc on stsc.tp_solicitacao_compra  = sccp.tp_solicitacao_compra&#13;&#10;       left join sup_st_solicitacao_compra  sssc on sssc.st_solicitacao_compra  = sccp.st_solicitacao_compra&#13;&#10;       left join sup_tp_solicitacao         stps on stps.tp_solicitacao         = sccp.tp_solicitacao&#13;&#10;       left join sup_tp_entrega             steg on steg.tp_entrega             = sccp.tp_entrega&#13;&#10;       left join sup_tp_medida_prazo        stmp on stmp.id_tp_medida_prazo     = sccp.id_tp_medida_prazo&#13;&#10;       left join api_fonte_recurso          afnt on afnt.id_fonte_recurso       = sccp.id_fonte_recurso&#13;&#10;       left join api_aplicacao              aapl on aapl.id_aplicacao           = sccp.id_aplicacao&#13;&#10;       left join api_dotacao_por_id(sccp.id_conta_funcional, null, null, null, null, null) adot on 1 = 1&#13;&#10;&#13;&#10; where sccp.id_solicitacao_compra = :idsolicitacaocompra">
