      <TableDataSource Name="SqlRequisicao" DataType="System.Int32" Enabled="true" SelectCommand="select&#13;&#10;  req.id_requisicao&#13;&#10;  , req.nr_requisicao&#13;&#10;  , req.cp_ano ano_requisicao&#13;&#10;  , req.dt_requisicao&#13;&#10;  , req.id_tp_requisicao&#13;&#10;  , trq.ds_tp_requisicao&#13;&#10;  , urq.ds_usuario nome_requisitante&#13;&#10;  , urq.nome_usuario nome_completo_requisitante&#13;&#10;  , case when par.definir_dt_aprovacao_req = 1&#13;&#10;      then req.dt_aprovacao&#13;&#10;      else apr.dt_historico&#13;&#10;   end dt_aprovacao&#13;&#10;  , uap.ds_usuario nome_aprovador&#13;&#10;  , uap.nome_usuario nome_completo_aprovador&#13;&#10;  , alx.cd_almoxarifado&#13;&#10;  , alx.ds_almoxarifado&#13;&#10;  , ald.cd_almoxarifado cd_almoxarifado_destino&#13;&#10;  , ald.ds_almoxarifado ds_almoxarifado_destino&#13;&#10;  , ues.cd_unidade_estoque&#13;&#10;  , ues.ds_unidade_estoque&#13;&#10;  , uad.cd_unidade_adm&#13;&#10;  , uad.ds_unidade_adm&#13;&#10;  , ued.cd_unidade_estoque cd_unidade_estoque_destino&#13;&#10;  , ued.ds_unidade_estoque ds_unidade_estoque_destino&#13;&#10;  , cec.cd_centro_consumo&#13;&#10;  , cec.ds_centro_consumo&#13;&#10;  , req.obs&#13;&#10;  , usu_recus.ds_usuario nome_recusa                      &#13;&#10;  , usu_recus.nome_usuario nome_completo_recusa                                        &#13;&#10;  , req.st_requisicao&#13;&#10;  , recus.dt_historico dt_recusa      &#13;&#10;from&#13;&#10;  sup_requisicao req&#13;&#10;  left join sup_tp_requisicao trq on&#13;&#10;    trq.id_tp_requisicao = req.id_tp_requisicao&#13;&#10;  left join sis_usuario urq on&#13;&#10;    urq.id_usuario = req.id_usuario_requi&#13;&#10;  left join sis_usuario uap on&#13;&#10;    uap.id_usuario = req.id_usuario_aprov&#13;&#10;  left join sup_almoxarifado alx on&#13;&#10;    alx.id_almoxarifado = req.id_almoxarifado&#13;&#10;  left join sup_almoxarifado ald on&#13;&#10;    ald.id_almoxarifado = req.id_almoxarifado_destino&#13;&#10;  left join sup