      <TableDataSource Name="Table" Alias="SqlPrincipal" DataType="System.Int32" Enabled="true" SelectCommand="SELECT CASE WHEN CAST(:TP_AGRUPAMENTO AS INTEGER) IN (1,3) THEN A2.CD_ALMOXARIFADO      END AS CD_ALMOXARIFADO&#13;&#10;     , CASE WHEN CAST(:TP_AGRUPAMENTO AS INTEGER) IN (1,3) THEN A2.DS_ALMOXARIFADO      END AS DS_ALMOXARIFADO&#13;&#10;     , CASE WHEN CAST(:TP_AGRUPAMENTO AS INTEGER) IN (2,3) THEN A2.CD_UNIDADE_ESTOQUE   END AS CD_UNIDADE_ESTOQUE&#13;&#10;     , CASE WHEN CAST(:TP_AGRUPAMENTO AS INTEGER) IN (2,3) THEN A2.DS_UNIDADE_ESTOQUE   END AS DS_UNIDADE_ESTOQUE&#13;&#10;     , A2.CP_MES&#13;&#10;     , ROUND(SUM(A2.VL_SALDO_ANTERIOR)  , 2) AS VL_SALDO_ANTERIOR&#13;&#10;     , ROUND(SUM(A2.VL_ENTRADA)         , 2) AS VL_ENTRADA&#13;&#10;     , ROUND(SUM(A2.VL_SAIDA)           , 2) AS VL_SAIDA&#13;&#10;     , ROUND(SUM(A2.VL_SALDO_ANTERIOR&#13;&#10;               + A2.VL_ENTRADA&#13;&#10;               + A2.VL_SAIDA         )  , 2) AS VL_SALDO_SEGUINTE&#13;&#10;  FROM (&#13;&#10;        SELECT A1.CD_ALMOXARIFADO&#13;&#10;             , A1.DS_ALMOXARIFADO&#13;&#10;             , A1.CD_UNIDADE_ESTOQUE&#13;&#10;             , A1.DS_UNIDADE_ESTOQUE&#13;&#10;             , A1.CP_MES&#13;&#10;             , (SELECT COALESCE(SUM(ZZ.VL_TOTAL), 0)&#13;&#10;                  FROM SUP_LANCAMENTO XX&#13;&#10;                       LEFT JOIN SUP_LANCAMENTO_IT ZZ ON ZZ.ID_LANCAMENTO = XX.ID_LANCAMENTO&#13;&#10;                 WHERE ( ( XX.CP_ANO &lt; :CP_ANO ) OR&#13;&#10;                         ( XX.CP_ANO = :CP_ANO AND XX.CP_MES &lt; A1.CP_MES ) )&#13;&#10;                   AND XX.ID_ENTIDADE          = :ID_ENTIDADE                                                                                                                                                                                                                                           &#13;&#10;                   AND XX.ID_ALMOXARIFADO      = A1.ID_ALMOXARIFADO&#13;&#10;                   AND XX.ID_UNIDADE_ESTOQUE   = A1.ID_UNIDADE_ESTOQUE) AS VL_SALDO_ANTERIOR&#13;&#10;             , SUM(A1.VL_ENTRADA) AS VL_ENTRADA&#13;&#10;             , SUM(A1.VL_SAIDA)   AS VL_SAIDA&#13;&#10;          FROM (&#13;&#10;                SELECT A.ID_ALMOXARIFADO&#13;&#10;                     , C.CD_ALMOXARIFADO&#13;&#10;                     , C.DS_ALMOXARIFADO&#13;&#10;                     , A.ID_UNIDADE_ESTOQUE&#13;&#10;                     , D.CD_UNIDADE_ESTOQUE&#13;&#10;                     , D.DS_UNIDADE_ESTOQUE&#13;&#10;                     , A.CP_MES&#13;&#10;                     , SUM(CASE WHEN A.TP_LANCAMENTO = 0 THEN B.VL_TOTAL ELSE 0 END) AS VL_ENTRADA&#13;&#10;                     , SUM(CASE WHEN A.TP_LANCAMENTO = 1 THEN B.VL_TOTAL ELSE 0 END) AS VL_SAIDA&#13;&#10;                  FROM SUP_LANCAMENTO A&#13;&#10;                       LEFT JOIN SUP_LANCAMENTO_IT          B ON B.ID_LANCAMENTO        = A.ID_LANCAMENTO&#13;&#10;                       LEFT JOIN SUP_ALMOXARIFADO           C ON C.ID_ALMOXARIFADO      = A.ID_ALMOXARIFADO&#13;&#10;                       LEFT JOIN SUP_UNIDADE_ESTOQUE        D ON D.ID_UNIDADE_ESTOQUE   = A.ID_UNIDADE_ESTOQUE&#13;&#10;                       LEFT JOIN SUP_USUARIO                F ON F.ID_USUARIO           = :ID_USUARIO&#13;&#10;                       LEFT JOIN SUP_ACESSO_ALMOXARIFADO    E ON E.ID_ALMOXARIFADO      = A.ID_ALMOXARIFADO&#13;&#10;                                                             AND E.ID_GRUPO_USUARIO     = F.ID_GRUPO_USUARIO&#13;&#10;                 WHERE A.ID_ENTIDADE         = :ID_ENTIDADE                                                                                                                                                                               &#13;&#10;                   AND A.CP_ANO              = :CP_ANO&#13;&#10;                   AND A.CP_MES        BETWEEN :CP_MES_INICIAL AND :CP_MES_FINAL&#13;&#10;                   AND (A.ID_ALMOXARIFADO    = :ID_ALMOXARIFADO    OR -1 = :ID_ALMOXARIFADO)&#13;&#10;                   AND (A.ID_UNIDADE_ESTOQUE = :ID_UNIDADE_ESTOQUE OR -1 = :ID_UNIDADE_ESTOQUE)&#13;&#10;                   AND E.ALMOXARIFE          = 1&#13;&#10;                 GROUP BY 1,2,3,4,5,6,7&#13;&#10;        &#13;&#10;                UNION&#13;&#10;        &#13;&#10;                SELECT B.ID_ALMOXARIFADO&#13;&#10;                     , B.CD_ALMOXARIFADO&#13;&#10;                     , B.DS_ALMOXARIFADO&#13;&#10;                     , C.ID_UNIDADE_ESTOQUE&#13;&#10;                     , C.CD_UNIDADE_ESTOQUE&#13;&#10;                     , C.DS_UNIDADE_ESTOQUE&#13;&#10;                     , S.CP_MES&#13;&#10;                     , CAST(0 AS DOUBLE PRECISION) AS VL_ENTRADA&#13;&#10;                     , CAST(0 AS DOUBLE PRECISION) AS VL_SAIDA&#13;&#10;                  FROM SUP_ALMOX_UNIDADE_ESTOQUE A&#13;&#10;                       LEFT JOIN SUP_ALMOXARIFADO           B ON B.ID_ALMOXARIFADO      = A.ID_ALMOXARIFADO&#13;&#10;                       LEFT JOIN SUP_UNIDADE_ESTOQUE        C ON C.ID_UNIDADE_ESTOQUE   = A.ID_UNIDADE_ESTOQUE&#13;&#10;                       LEFT JOIN SUP_USUARIO                F ON F.ID_USUARIO           = :ID_USUARIO&#13;&#10;                       LEFT JOIN SUP_ACESSO_ALMOXARIFADO    E ON E.ID_ALMOXARIFADO      = A.ID_ALMOXARIFADO&#13;&#10;                                                             AND E.ID_GRUPO_USUARIO     = F.ID_GRUPO_USUARIO&#13;&#10;                       LEFT JOIN (&#13;&#10;                                  SELECT SEQ.ID_SEQUENCIA AS CP_MES&#13;&#10;                                    FROM SIS_SEQUENCIA SEQ&#13;&#10;                                   WHERE SEQ.ID_SEQUENCIA BETWEEN :CP_MES_INICIAL AND :CP_MES_FINAL&#13;&#10;                                 ) S ON 1 = 1&#13;&#10;        &#13;&#10;                 WHERE B.ID_ENTIDADE         = :ID_ENTIDADE&#13;&#10;                   AND C.ID_ENTIDADE         = :ID_ENTIDADE                                                                                                                                   &#13;&#10;                   AND (A.ID_ALMOXARIFADO    = :ID_ALMOXARIFADO    OR -1 = :ID_ALMOXARIFADO)&#13;&#10;                   AND (A.ID_UNIDADE_ESTOQUE = :ID_UNIDADE_ESTOQUE OR -1 = :ID_UNIDADE_ESTOQUE)&#13;&#10;                   AND E.ALMOXARIFE = 1&#13;&#10;               ) A1&#13;&#10;         GROUP BY&#13;&#10;               A1.ID_ALMOXARIFADO&#13;&#10;             , A1.CD_ALMOXARIFADO&#13;&#10;             , A1.DS_ALMOXARIFADO&#13;&#10;             , A1.ID_UNIDADE_ESTOQUE&#13;&#10;             , A1.CD_UNIDADE_ESTOQUE&#13;&#10;             , A1.DS_UNIDADE_ESTOQUE&#13;&#10;             , A1.CP_MES&#13;&#10;&#13;&#10;       ) A2&#13;&#10; GROUP BY&#13;&#10;       1,2,3,4,5&#13;&#10; ORDER BY&#13;&#10;       1, 3">
